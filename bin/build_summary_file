#!/usr/bin/perl -w
# to be run in top directory
#
use strict;

my @g;
my $line=<>;
print $line;

while (defined($line = <> )) {
    my @l = ($line =~ /((?:[^,]+,){3})([0-9]+)(.*)/s);

    if (@g and ($l[0] ne $g[0][0])) {
        print (join ("", @{(sort { $b->[1] <=> $a->[1] } @g)[0]}));
	@g = ();
    }
    
    push @g, \@l;
}

