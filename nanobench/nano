# !/bin/bash
# The Computer Language Benchmarks Game
# $Id: nano,v 1.4 2008-08-10 02:12:06 igouy-guest Exp $

# convenient

nanobench=~/shootout/nanobench/bin/nanobench
ini=~/shootout/nanobench/makefiles/gp4.ini 

if [ $# = 0 ]; then
    $nanobench --conf $ini
elif [ $1 = --force ]; then
    $nanobench --conf $ini --force $2
fi


