void main(String[] args){
    let n = args.length == 0 ? 1 : Integer.parseInt(args[0]);
    println( fib(n) );
}


int fib(int n) requires n >= 0 { // precondition
   if (n < 2) return 1; else return fib(n-2) + fib(n-1);
}
