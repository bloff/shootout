-module(fibo).
-export([main/1]).

main([Arg]) ->
    Num = list_to_integer(Arg),
    io:fwrite("~w\n", [fib(Num)]),
    halt(0).

fib(0) -> 0;
fib(1) -> 1;
fib(N) -> fib(N-2) + fib(N-1).
