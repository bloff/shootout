with Ada.Text_IO, Ada.Integer_Text_IO, Ada.Command_Line;
use  Ada.Text_IO, Ada.Integer_Text_IO, Ada.Command_Line;

with Pck_Fibo;

procedure Fibo is
   N : Natural;
begin
   N := Natural'Value (Argument (1));
   Put (Item => Pck_Fibo.Fib (N), Width => 0);
   New_Line;
end Fibo;
