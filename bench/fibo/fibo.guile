#!/usr/bin/guile \
-e main -s
!#
;;; $Id: fibo.guile,v 1.1 2004-05-19 18:09:49 bfulgham Exp $
;;; http://www.bagley.org/~doug/shootout/

(define (main args)
  (let ((n (or (and (= (length args) 2) (string->number (cadr args))) 1)))
    (display (fib n))
    (newline)))

(define (fib n)
  (cond ((< n 2) 1)
	(else (+ (fib (- n 2)) (fib (- n 1))))))

