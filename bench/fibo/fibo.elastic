package fibo;

import basic;
import sys;
import array;

private function fib( n )
{
    if (n < 2) return(1);
    return(fib(n-2) + fib(n-1));
}


private n = 1;
if(array.length(sys.args) > 0) {
    n = basic.int(sys.args[0]);
} else {
    n = 1;
}
basic.print(fib(n), "\n");
