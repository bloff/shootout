module fibo

import StdEnv, LanguageShootout

fib :: !Int -> Int
fib n
	| n < 2 = 1
	| otherwise = fib (n-2) + fib (n-1)

Start
	= toString (fib n) +++ "\n"
	where
	    n = argi
