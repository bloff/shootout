'From Squeak3.8.1 of ''28 Aug 2006'' [latest update: #6747] on 30 November 2006 at 5:08:03 pm'!!Array methodsFor: 'benchmarking' stamp: '  11/29/2006 22:50'!multiplyAtAv   ^(self multiplyAv) multiplyAtv! !!Array methodsFor: 'benchmarking' stamp: '  11/29/2006 22:49'!multiplyAtv   | n atv |   n := self size.   atv := Array new: n withAll: 0.0d0.   1 to: n do: [:i| 	      1 to: n do: [:j|         atv at: i put: (atv at: i) + ((j matrixA: i) * (self at: j)) ]].   ^atv! !!Array methodsFor: 'benchmarking' stamp: '  11/29/2006 22:49'!multiplyAv   | n av |   n := self size.   av := Array new: n withAll: 0.0d0.   1 to: n do: [:i| 	      1 to: n do: [:j|         av at: i put: (av at: i) + ((i matrixA: j) * (self at: j)) ]].   ^av! !!Array methodsFor: 'benchmarking' stamp: '  11/29/2006 23:11'!pfannkuchen    | first complement a b k |    k := 0.    [ (first := self at: 1) == 1 ] whileFalse: [	k := k + 1.	complement := first + 1.	1 to: first // 2 do: [ :i |	    a := self at: i.	    b := self at: complement - i.	    self at: i put: b.	    self at: complement - i put: a.	]    ].    ^k! !