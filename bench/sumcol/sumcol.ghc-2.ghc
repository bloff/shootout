import qualified Data.ByteString.Char8 as B
import Data.Char

main = print . sum . map parseInt . B.lines =<< B.getContents

parseInt bs = case B.head bs of '-' -> -parseNat (B.tail bs)
                                _   ->  parseNat bs

parseNat = B.foldl' (\n c -> n * 10 + ord c - ord '0') 0
