class SUMCOL

creation make

feature

   make is

      local
	 sum: INTEGER;
      do
	 sum := 0
	 from
	    io.read_line
	 until
	    io.end_of_input
	 loop
	    sum := sum + io.last_string.to_integer
	    io.read_line
	 end
         std_output.put_integer(sum)
         std_output.put_character('%N')
      end
end
