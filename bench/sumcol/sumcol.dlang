import std.conv, std.stdio, std.string;

const size_t MAXLINELEN = 128;

int main()
{
    char* bufr = new char[MAXLINELEN];
    int   sum;

    while(fgets(bufr,MAXLINELEN,stdin))
    {
        sum += toInt(bufr[0..strlen(bufr)-1]);
    }

    printf("%d\n", sum);

    return 0;
}
