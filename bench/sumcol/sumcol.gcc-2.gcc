#include <stdio.h>
#include <stdlib.h>

#define MAXLINELEN 128

int
main() {
    int sum = 0;
    char line[MAXLINELEN];

    while (fgets(line, MAXLINELEN, stdin)) {
	sum += strtol(line,NULL,0);
    }
    printf("%d\n", sum);
    return(0);
}
