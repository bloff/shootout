using System;

class sumcol {
    public static void Main(String[] args)
    {
        int sum = 0;

        String line = Console.In.ReadLine();
	while (line != null)
	{
	    sum += Int32.Parse(line);
	    line = Console.In.ReadLine();
        }
        Console.WriteLine(sum);
    }
}
