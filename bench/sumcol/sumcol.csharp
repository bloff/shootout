using System;

class sumcol {
    public static int Main(String[] args) {
        int sum = 0;

        for (String line = Console.In.ReadLine(); line != null; line = Console.In.ReadLine()) {
            sum += System.Convert.ToInt32(line);
        }
        Console.WriteLine(sum.ToString() + "\n");
        return(0);
    }
}
