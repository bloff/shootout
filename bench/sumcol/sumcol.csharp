using System;
using System.IO;

class SumCol 
{
   static void Main(){
      StreamReader r = new StreamReader(Console.OpenStandardInput());

      int sum = 0;
      string line;
      while ((line = r.ReadLine()) != null) sum += Int32.Parse(line);
      Console.WriteLine(sum);
   }
}
