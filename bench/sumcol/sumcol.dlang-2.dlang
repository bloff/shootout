import std.stdio, std.c.stdlib;

const size_t MAXLINELEN = 128;

void main()
{
    int     	     sum;
    char[MAXLINELEN] line;

    while(fgets(line.ptr, MAXLINELEN, stdin))
    {
	sum += atoi(line.ptr);
    }

    writefln(sum);
}
