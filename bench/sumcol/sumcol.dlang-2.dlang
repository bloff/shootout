import std.cstream, std.conv;

void main()
{
    int     sum;
    char[128] bufr;
    char[]  line;

    BufferedStream bsi = new BufferedStream(din,4096);
    while(!bsi.eof)
    {
        line = bsi.readLine(bufr);
        if(line.length) sum += toInt(line);
    }

    dout.writefln(sum);
}
