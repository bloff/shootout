program sumcol;

var num, tot: longint;
    textbuf:array[0..8191] of char;

begin
  settextbuf(input,textbuf);
  while not eof do begin
    ReadLn(input, num);
    inc(tot,num);
  end;
  WriteLn(tot);
end.
