256 constant max-line
create line-buffer max-line 1 + allot

: sumcol ( -- )
    0
    begin
	0.0 line-buffer
	line-buffer max-line stdin read-line throw
    while
	>number drop drop d>s +
    repeat
    drop drop drop drop 1 u.r cr ;

sumcol

bye \ th-th-that's all folks!
