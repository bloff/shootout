MODULE Sumcol;
IMPORT In, Out;

VAR sum, v: LONGINT;
BEGIN
   sum := 0;
   LOOP
      In.LongInt(v); 
      IF In.Done() THEN INC(sum,v); ELSE EXIT; END;
   END;
   Out.Int(sum, 0); Out.Ln;
END Sumcol.
