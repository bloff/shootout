-- TBD: try rewrite with STUArray or IOUArray

module Main where

import IArray
import System
import Data.Array.Unboxed

arr :: Int -> UArray Int Int
arr len = array (1,len) [(i, i-1) | i <- [1..len]]

arr2 :: Int -> UArray Int Int
arr2 len = array (1,len) [(i, arr1 ! (len-i+1)) | i <- [1..len]]
           where arr1 = arr len

getAns :: Int -> IO ()
getAns len = do
               putStrLn (show ((arr2 len) ! 1))

main = do
        ~[n] <- getArgs 
        getAns (read n::Int)
