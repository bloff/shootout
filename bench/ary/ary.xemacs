(defun main ()
  (let* ((arg (car command-line-args-left))
	 (n (if arg (string-to-number arg) 1))
	 (x (make-vector n 0))
	 (y (make-vector n 0))
	 (last (- n 1)))
    (dotimes (i n)
      (aset x i i))
    (do ((i last (- i 1)))
	((< i 0))
      (aset y i (aref x i)))
    (princ (format "%d\n" (aref y last)))))
