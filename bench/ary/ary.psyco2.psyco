import sys
import psyco
psyco.full()

def main():
    n = int(sys.argv[1])
    x = n * [0]       
    y = n * [0]
    for i in range(0,n):
        x[i] = i
    for i in range(n-1,-1,-1):
        y[i] = x[i]
    print y[-1]

main()
