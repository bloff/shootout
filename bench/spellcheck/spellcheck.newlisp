#!/usr/bin/newlisp

;; spellcheck.newlisp - benchmark

(context 'Dictionary)
(context 'MAIN)

(define (main)
	(set 'infile (open "Usr.Dict.Words" "read"))
	(while (set 'word (read-line infile))
		(symbol (append "_" word) 'Dictionary))
	(close infile)

	(while (set 'word (read-line))
		(if (not (symbol (append "_" word) 'Dictionary nil))
			(println word))))

(main)

