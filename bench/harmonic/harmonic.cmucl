(defun sum-harmonic (n)
   (declare (fixnum n)
            (optimize (speed 3)))
   (loop for i from 1 upto n
        sum (/ 1.0d0 i) of-type double-float))

(defun main ()
   (let ((n (parse-integer (or (car (last sb-ext:*posix-argv*))
			      "1"))))
     (format t "~A~%" (sum-harmonic n))))