using System;

class Harmonic
{
   public static void Main(String[] args) 
   {        
      int n = 10000000;
      if (args.Length > 0) n = Int32.Parse(args[0]);

      double d = 0.0, partialSum = 0.0;    

      while (n-- > 0) 
         partialSum += 1.0/++d;
     
      Console.WriteLine("{0:f9}", partialSum);
   }
}