import std.stdio, std.string;

void main(char[][] args)
{
    int i = 0, n = args.length > 1 ? atoi(args[1]) : 10000000;
    double d = 0.0, partialSum = 0.0;

    while(i < n)
    {
        d++;
        partialSum += 1.0 / d;
        i++;
    }

    writefln("%0.9f\n",partialSum);
}
