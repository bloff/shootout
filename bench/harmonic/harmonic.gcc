#include<stdio.h>
#include<stdlib.h>

int main (int argc, char **argv)
{
    double i, n, sum=0;

    n = atof(argv[1]);
   
    for(i=1; i<n+1; i++)
        sum += 1/i;

    printf("%.9f\n", sum);
}

