module takfp
import StdEnv, LanguageShootout

Start = tak (n*3.0) (n*2.0) (n*1.0)
   where n = toReal argi

tak x y z
   | y < x   = tak (tak (x-1.0) y z)(tak (y-1.0) z x)(tak (z-1.0) x y)
             = z