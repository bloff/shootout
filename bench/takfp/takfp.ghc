-- http://shootout.alioth.debian.org/
-- GHC version of floating point Tak function
-- by Don Stewart

import System

main = putStrLn . show . (\n -> tak (3*n) (2*n) n) . read . head =<< getArgs

tak x y z | y >= x    = z :: Float
          | otherwise = tak (tak (x-1) y z) (tak (y-1) z x) (tak (z-1) x y)

