import sys

comps = {'A':"T", 'C':"G", 'G':"C", 'T':"A",
         'B':"V", 'D':"H", 'H':"D", 'K':"M",
	 'M':"K", 'R':"Y", 'V':"B", 'Y':"R",
	 'W':'W', 'N':'N', 'S':'S'}
	 
def flip(line):
    if line[0] == ">":
        return line
    else:
        return ''.join([comps[x] for x in line.upper()])

f = file('fastaOut.txt', 'w')
sys.stdout.writelines(["%s\n" % (flip(x.strip())) for x in sys.stdin])
