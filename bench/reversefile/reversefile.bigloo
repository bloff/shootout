(module reversefile
	(main main))

(define (main args)
  (reverse-input))

(define (reverse-input)
  (let ((line (read-line)))
    (cond ((eof-object? line) #f)
	  (else
	   (reverse-input)
	   (display line)
	   (newline)))))
