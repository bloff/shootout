#!/usr/bin/guile \
-e main -s
!#

;;; $Id: reversefile.guile,v 1.1 2004-05-19 18:12:18 bfulgham Exp $
;;; http://www.bagley.org/~doug/shootout/
;;; from Brad Knotwell

(define (main args)
  (let ((lines '()))
    (do ((line (read-line) (read-line)))
	((eof-object? line) (map write-line lines))
      (set! lines (cons line lines)))))
