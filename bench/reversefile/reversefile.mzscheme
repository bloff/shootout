#!/usr/bin/mzscheme -qu
;;; -*- mode: scheme -*-
;;; $Id: reversefile.mzscheme,v 1.2 2006-06-09 23:27:21 bfulgham Exp $
;;; http://shootout.alioth.debian.org/
;;; Provided by Bengt Kleberg

(module reversefile mzscheme 
  (let ([inport (current-input-port)])
    (let rev ([lines null])
      (let ([line (read-bytes-line inport)])
	(if (eof-object? line)
	    (for-each (lambda (l) (printf "~a\n" l))
		      lines)
	    (rev (cons line lines)))))))
