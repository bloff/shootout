import sys

n = int(sys.argv[1])
hash1 = {}
hash2 = {}
for i in xrange(10000):
    key = 'foo_%d' % i
    hash1[key] = i
    hash2[key] = 0

for i in xrange(n):
    for key in hash1.keys():
        hash2[key] += hash1[key]

print hash1['foo_1'], hash1['foo_9999'], hash2['foo_1'], hash2['foo_9999']
