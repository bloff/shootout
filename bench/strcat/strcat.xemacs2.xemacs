(defun main ()
  (let* ((arg (car command-line-args-left))
	 (num (if arg (string-to-number arg) 1)))
    (let ((str ""))
      (dotimes (iter num)
	(setq str (concat str "hello\n")))
      (princ (format "%d\n" (length str))))))
