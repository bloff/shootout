(let ((n (parse-integer (or (car pop11::poparglist) "1")))
(x 0))
(declare (fixnum n)
	 (fixnum x)
	 (optimize (speed 3) (debug 0) (safety 0)))
(dotimes (a n)
  (dotimes (b n)
	(dotimes (c n)
	  (dotimes (d n)
		(dotimes (e n)
		  (dotimes (f n)
			(incf x)))))))
(format t "~A~%" x))
