include "std";

val n = atoi(System::argv 1);

fun ack(x:int,y:int):int =>
  if x == 0 then  y + 1
  elif y == 0 then  ack(x-1, 1)
  else ack(x-1, ack(x, y-1))
  endif
;

val v = ack(3,n);
print "Ack(3,"; print n; print "): "; print v; endl;


