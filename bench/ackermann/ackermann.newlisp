#!/usr/bin/newlisp
;;
;; Ackermann's Function
;; 

(define (ack m n)
    (cond ((= m 0) (+ n 1))
          ((= n 0) (ack (- m 1) 1))
	  (true    (ack (- m 1) (ack m (- n 1))))))

(define (main)
    (set 'N (integer (nth 2 (main-args))))
    (println
        (format "Ack(3,%d): %d" N (ack 3 N))))

(main)
(exit)


