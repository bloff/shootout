#! /bin/bash

CVS=~dunham/shootout
GP4=~dunham/gp4/shootout

cp -fp $GP4/website/data/data.csv $CVS/website/websites/gp4/data/data.csv 2>/dev/null
cp -fp $GP4/website/data/ndata.csv $CVS/website/websites/gp4/data/ndata.csv 2>/dev/null

for each in $(find $GP4/website/code -mtime -2)
do
   if [ -f $each ]; then
      cp -fp $each $CVS/website/websites/gp4/code 2>/dev/null
   fi
done
