<span class="kwa">program</span> nestedloop<span class="sym">;</span>
<span class="kwa">uses</span> SysUtils<span class="sym">;</span>

<span class="kwa">var</span> n<span class="sym">,</span> a<span class="sym">,</span> b<span class="sym">,</span> c<span class="sym">,</span> d<span class="sym">,</span> e<span class="sym">,</span> f <span class="sym">:</span> <span class="kwb">integer</span><span class="sym">;</span>
<span class="kwa">var</span> x <span class="sym">:</span> <span class="kwb">longint</span><span class="sym">;</span>

<span class="kwa">begin</span>
	<span class="kwa">if</span> ParamCount <span class="sym">=</span> <span class="num">0</span> <span class="kwa">then</span>
		n <span class="sym">:=</span> <span class="num">1</span>
	<span class="kwa">else</span>
		n <span class="sym">:=</span> <span class="kwd">StrToInt</span><span class="sym">(</span><span class="kwd">ParamStr</span><span class="sym">(</span><span class="num">1</span><span class="sym">));</span>
	<span class="kwa">if</span> n <span class="sym">&lt;</span> <span class="num">1</span> <span class="kwa">then</span> n <span class="sym">:=</span> <span class="num">1</span><span class="sym">;</span>
	x <span class="sym">:=</span> <span class="num">0</span><span class="sym">;</span>
	<span class="kwa">For</span> a <span class="sym">:=</span> <span class="num">1</span> <span class="kwa">to</span> n <span class="kwa">Do</span>
	<span class="kwa">For</span> b <span class="sym">:=</span> <span class="num">1</span> <span class="kwa">to</span> n <span class="kwa">Do</span>
	<span class="kwa">For</span> c <span class="sym">:=</span> <span class="num">1</span> <span class="kwa">to</span> n <span class="kwa">Do</span>
	<span class="kwa">For</span> d <span class="sym">:=</span> <span class="num">1</span> <span class="kwa">to</span> n <span class="kwa">Do</span>
	<span class="kwa">For</span> e <span class="sym">:=</span> <span class="num">1</span> <span class="kwa">to</span> n <span class="kwa">Do</span>
	<span class="kwa">For</span> f <span class="sym">:=</span> <span class="num">1</span> <span class="kwa">to</span> n <span class="kwa">Do</span>
	<span class="kwd">Inc</span><span class="sym">(</span>x<span class="sym">);</span>
	<span class="kwd">WriteLn</span><span class="sym">(</span> <span class="kwd">IntToStr</span><span class="sym">(</span>x<span class="sym">) );</span>
<span class="kwa">end</span>.
