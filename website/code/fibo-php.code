function fibo($n){
    <B><FONT COLOR="#A020F0">return</FONT></B>(($n &lt; 2) ? 1 : fibo($n - 2) + fibo($n - 1));
}
$n = ($argc == 2) ? $argv[1] : 1;
$r = fibo($n);
<B><FONT COLOR="#A020F0">print</FONT></B> <B><FONT COLOR="#BC8F8F">&quot;$r\n&quot;</FONT></B>;
