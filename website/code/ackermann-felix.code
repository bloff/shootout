</FONT></I>include <B><FONT COLOR="#BC8F8F">&quot;std&quot;</FONT></B>;
n := atoi(System::argv 1);
fun <B><FONT COLOR="#0000FF">ack</FONT></B>(x:<B><FONT COLOR="#228B22">int</FONT></B>,y:<B><FONT COLOR="#228B22">int</FONT></B>):<B><FONT COLOR="#228B22">int</FONT></B> =&gt;
  <B><FONT COLOR="#A020F0">if</FONT></B> x == 0 then  y + 1
  elif y == 0 then  ack(x-1, 1)
  <B><FONT COLOR="#A020F0">else</FONT></B> ack(x-1, ack(x, y-1))
  endif
;

v := ack(3,n);
print <B><FONT COLOR="#BC8F8F">&quot;Ack(3,&quot;</FONT></B>; print n; print <B><FONT COLOR="#BC8F8F">&quot;): &quot;</FONT></B>; print v; endl;

