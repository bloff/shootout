</span><span class="line">   62 </span><span class="sym"></span>            print<span class="sym">(</span><span class="str">&quot;%N&quot;</span><span class="sym">)
</span><span class="line">   63 </span><span class="sym"></span>            index <span class="sym">:=</span> index <span class="sym">+</span> <span class="num">1
</span><span class="line">   64 </span><span class="num"></span>         <span class="kwa">end
</span><span class="line">   65 </span><span class="kwa">         from</span> index <span class="sym">:=</span> <span class="num">0</span> until index <span class="sym">=</span> n <span class="kwa">loop
</span><span class="line">   66 </span><span class="kwa"></span>            <span class="sym">!!</span>nth_toggle.make<span class="sym">(</span><span class="kwa">True</span><span class="sym">,</span> <span class="num">3</span><span class="sym">)
