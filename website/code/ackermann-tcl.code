<table width="100%" border="0" cellpadding="5" cellspacing="0"><tr><td bgcolor="#FFFFFF"><pre>
<I><FONT COLOR="#B22222">#!/usr/bin/tclsh
</FONT></I><I><FONT COLOR="#B22222"># $Id: ackermann-tcl.code,v 1.2 2004-11-08 08:15:12 bfulgham Exp $
</FONT></I><I><FONT COLOR="#B22222"># http://www.bagley.org/~doug/shootout/
</FONT></I>
<B><FONT COLOR="#A020F0">set</FONT></B> NUM [<B><FONT COLOR="#A020F0">lindex</FONT></B> $<FONT COLOR="#B8860B">argv</FONT> 0]
<B><FONT COLOR="#A020F0">if</FONT></B> {$<FONT COLOR="#B8860B">NUM</FONT> &lt; 1} {
    <B><FONT COLOR="#A020F0">set</FONT></B> NUM 1
}

<B><FONT COLOR="#A020F0">proc</FONT></B> <B><FONT COLOR="#0000FF">ack</FONT></B> {<FONT COLOR="#B8860B">m n</FONT>} {
    <B><FONT COLOR="#A020F0">if</FONT></B> {$<FONT COLOR="#B8860B">m</FONT> == 0} {
	<B><FONT COLOR="#A020F0">return</FONT></B> [<B><FONT COLOR="#A020F0">expr</FONT></B> {$<FONT COLOR="#B8860B">n</FONT> + 1}]
    } elseif {$<FONT COLOR="#B8860B">n</FONT> == 0} {
	<B><FONT COLOR="#A020F0">return</FONT></B> [ack [<B><FONT COLOR="#A020F0">expr</FONT></B> {$<FONT COLOR="#B8860B">m</FONT> - 1}] 1]
    } else {
	<B><FONT COLOR="#A020F0">return</FONT></B> [ack [<B><FONT COLOR="#A020F0">expr</FONT></B> {$<FONT COLOR="#B8860B">m</FONT> - 1}] [ack $<FONT COLOR="#B8860B">m</FONT> [<B><FONT COLOR="#A020F0">expr</FONT></B> {$<FONT COLOR="#B8860B">n</FONT> - 1}]]]
    }
}

<B><FONT COLOR="#A020F0">set</FONT></B> ack [ack 3 $<FONT COLOR="#B8860B">NUM</FONT>]
<B><FONT COLOR="#A020F0">puts</FONT></B> <B><FONT COLOR="#BC8F8F">&quot;Ack(3,$NUM): $ack&quot;</FONT></B></pre></td></tr></table>
