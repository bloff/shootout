<B><FONT COLOR="#228B22">assert</FONT></B>(io.open(<B><FONT COLOR="#BC8F8F">&quot;Usr.Dict.Words&quot;</FONT></B>))
<B><FONT COLOR="#A020F0">local</FONT></B> dict <B><FONT COLOR="#5F9EA0">=</FONT></B> {}
<B><FONT COLOR="#A020F0">for</FONT></B> line <B><FONT COLOR="#A020F0">in</FONT></B> io.lines(<B><FONT COLOR="#BC8F8F">&quot;Usr.Dict.Words&quot;</FONT></B>) <B><FONT COLOR="#A020F0">do</FONT></B>
