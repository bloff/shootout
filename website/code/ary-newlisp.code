#!/usr/bin/newlisp

# arrays benchmark
#
# this requires newLISP 7.5.2 <B><FONT COLOR="#A020F0">or</FONT></B> later to handle arrays
#
# Contributed by Lutz Mueller


(<B><FONT COLOR="#A020F0">define</FONT></B> (<B><FONT COLOR="#0000FF">main</FONT></B>)
	(set 'n (integer (nth 2 (main-args))))
	(set 'x (array n (sequence 1 n)))
	(set 'y (array n '(0)))
	(dotimes (k 1000)
		(for (i (- n 1) 0)
			(nth-set i y (+ (nth i y) (nth i x)) )))

	(println (nth 0 y) <B><FONT COLOR="#BC8F8F">&quot; &quot;</FONT></B> (nth (- n 1) y)))

(main)
(exit)


	

