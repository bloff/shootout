<B><FONT COLOR="#A020F0">procedure </FONT></B><B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">main</FONT></I></B></FONT></B>(argv)
    n <B><FONT COLOR="#5F9EA0">:=</FONT></B> argv[1] <B><FONT COLOR="#5F9EA0">|</FONT></B> 1
    hash1 <B><FONT COLOR="#5F9EA0">:=</FONT></B> <B><FONT COLOR="#A020F0">table</FONT></B>(0)
    hash2 <B><FONT COLOR="#5F9EA0">:=</FONT></B> <B><FONT COLOR="#A020F0">table</FONT></B>(0)
    <B><FONT COLOR="#A020F0">every</FONT></B> i <B><FONT COLOR="#5F9EA0">:=</FONT></B> 0 <B><FONT COLOR="#A020F0">to</FONT></B> 10000 <B><FONT COLOR="#A020F0">do</FONT></B> {
	hash1[<B><FONT COLOR="#BC8F8F">&quot;foo_&quot;</FONT></B><B><FONT COLOR="#5F9EA0">|</FONT></B><B><FONT COLOR="#5F9EA0">|</FONT></B><B><FONT COLOR="#A020F0">string</FONT></B>(i)] <B><FONT COLOR="#5F9EA0">:=</FONT></B> i
    }
    <B><FONT COLOR="#A020F0">every</FONT></B> i <B><FONT COLOR="#5F9EA0">:=</FONT></B> 1 <B><FONT COLOR="#A020F0">to</FONT></B> n <B><FONT COLOR="#A020F0">do</FONT></B>
	<B><FONT COLOR="#A020F0">every</FONT></B> k <B><FONT COLOR="#5F9EA0">:=</FONT></B> <B><FONT COLOR="#A020F0">key</FONT></B>(hash1) <B><FONT COLOR="#A020F0">do</FONT></B>
	    hash2[k] <B><FONT COLOR="#5F9EA0">+</FONT></B><B><FONT COLOR="#5F9EA0">:=</FONT></B> hash1[k]
    <B><FONT COLOR="#A020F0">write</FONT></B>(hash1[<B><FONT COLOR="#BC8F8F">&quot;foo_1&quot;</FONT></B>], <B><FONT COLOR="#BC8F8F">&quot; &quot;</FONT></B>, hash1[<B><FONT COLOR="#BC8F8F">&quot;foo_9999&quot;</FONT></B>], <B><FONT COLOR="#BC8F8F">&quot; &quot;</FONT></B>,
	  hash2[<B><FONT COLOR="#BC8F8F">&quot;foo_1&quot;</FONT></B>], <B><FONT COLOR="#BC8F8F">&quot; &quot;</FONT></B>, hash2[<B><FONT COLOR="#BC8F8F">&quot;foo_9999&quot;</FONT></B>])
<B><FONT COLOR="#A020F0">end</FONT></B></pre></td></tr></table>
