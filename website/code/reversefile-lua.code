<B><FONT COLOR="#A020F0">local</FONT></B> lines <B><FONT COLOR="#5F9EA0">=</FONT></B> {}
<B><FONT COLOR="#A020F0">local</FONT></B> nl <B><FONT COLOR="#5F9EA0">=</FONT></B> 0

<B><FONT COLOR="#A020F0">for</FONT></B> l <B><FONT COLOR="#A020F0">in</FONT></B> io.lines() <B><FONT COLOR="#A020F0">do</FONT></B>
    nl <B><FONT COLOR="#5F9EA0">=</FONT></B> nl <B><FONT COLOR="#5F9EA0">+</FONT></B> 1
    lines[nl] <B><FONT COLOR="#5F9EA0">=</FONT></B> l
<B><FONT COLOR="#A020F0">end</FONT></B>

<B><FONT COLOR="#A020F0">for</FONT></B> i<B><FONT COLOR="#5F9EA0">=</FONT></B>nl,1,<B><FONT COLOR="#5F9EA0">-</FONT></B>1 <B><FONT COLOR="#A020F0">do</FONT></B>
    io.write(lines[i], <B><FONT COLOR="#BC8F8F">&quot;\n&quot;</FONT></B>)
<B><FONT COLOR="#A020F0">end</FONT></B></pre></td></tr></table>
