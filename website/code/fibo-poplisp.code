(defun fib (n)
(<B><FONT COLOR="#A020F0">if</FONT></B> (&lt; n 2) 1 (+ (fib (- n 1)) (fib (- n 2)))))

(<B><FONT COLOR="#A020F0">let</FONT></B> 
	((n (parse-integer (<B><FONT COLOR="#A020F0">or</FONT></B> (car pop11::poparglist) <B><FONT COLOR="#BC8F8F">&quot;1&quot;</FONT></B>))))
	(format *standard-output* <B><FONT COLOR="#BC8F8F">&quot;~A~%&quot;</FONT></B> (fib n))
)</pre></td></tr></table>
