</FONT></I>include <B><FONT COLOR="#BC8F8F">&quot;std&quot;</FONT></B>;
n := atoi(System::argv 1);

fun <B><FONT COLOR="#0000FF">fib</FONT></B>(n:<B><FONT COLOR="#228B22">int</FONT></B>):<B><FONT COLOR="#228B22">int</FONT></B> =&gt;
  <B><FONT COLOR="#A020F0">if</FONT></B> n &lt; 2 then 1 <B><FONT COLOR="#A020F0">else</FONT></B> fib (n - 2) + fib (n - 1) endif
;

v := fib(n);
print <B><FONT COLOR="#BC8F8F">&quot;Fib(&quot;</FONT></B>; print n; print <B><FONT COLOR="#BC8F8F">&quot;): &quot;</FONT></B>; print v; endl;
