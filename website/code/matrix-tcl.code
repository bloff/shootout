<table width="100%" border="0" cellpadding="5" cellspacing="0"><tr><td bgcolor="#FFFFFF"><pre>
<I><FONT COLOR="#B22222">#!/usr/bin/tclsh
</FONT></I><I><FONT COLOR="#B22222"># $Id: matrix-tcl.code,v 1.1 2004-11-02 08:52:36 bfulgham Exp $
</FONT></I><I><FONT COLOR="#B22222"># http://www.bagley.org/~doug/shootout/
</FONT></I>
<I><FONT COLOR="#B22222"># This program based on the original from:
</FONT></I><I><FONT COLOR="#B22222"># &quot;The What, Why, Who, and Where of Python&quot; By Aaron R. Watters
</FONT></I><I><FONT COLOR="#B22222"># http://www.networkcomputing.com/unixworld/tutorial/005/005.html
</FONT></I>
<I><FONT COLOR="#B22222"># modified to avoid matrix size checks
</FONT></I><I><FONT COLOR="#B22222"># --Doug
</FONT></I>
<I><FONT COLOR="#B22222"># additional speedups by Kristoffer Lawson and Miguel Sofer
</FONT></I>
<B><FONT COLOR="#A020F0">set</FONT></B> size 30;

<B><FONT COLOR="#A020F0">proc</FONT></B> <B><FONT COLOR="#0000FF">mkmatrix</FONT></B> {<FONT COLOR="#B8860B">rows cols</FONT>} {
    <B><FONT COLOR="#A020F0">set</FONT></B> count 1;
    <B><FONT COLOR="#A020F0">set</FONT></B> mx [<B><FONT COLOR="#A020F0">list</FONT></B>]
    <B><FONT COLOR="#A020F0">for</FONT></B> { <B><FONT COLOR="#A020F0">set</FONT></B> i 0 } { $<FONT COLOR="#B8860B">i</FONT> &lt; $<FONT COLOR="#B8860B">rows</FONT> } { <B><FONT COLOR="#A020F0">incr</FONT></B> i } {
	<B><FONT COLOR="#A020F0">set</FONT></B> row [<B><FONT COLOR="#A020F0">list</FONT></B>]
	<B><FONT COLOR="#A020F0">for</FONT></B> { <B><FONT COLOR="#A020F0">set</FONT></B> j 0 } { $<FONT COLOR="#B8860B">j</FONT> &lt; $<FONT COLOR="#B8860B">cols</FONT> } { <B><FONT COLOR="#A020F0">incr</FONT></B> j } {
	    <B><FONT COLOR="#A020F0">lappend</FONT></B> row $<FONT COLOR="#B8860B">count</FONT>;
	    <B><FONT COLOR="#A020F0">incr</FONT></B> count;
	}
	<B><FONT COLOR="#A020F0">lappend</FONT></B> mx $<FONT COLOR="#B8860B">row</FONT>;
    }
    <B><FONT COLOR="#A020F0">return</FONT></B> $<FONT COLOR="#B8860B">mx</FONT>;
}

<B><FONT COLOR="#A020F0">proc</FONT></B> <B><FONT COLOR="#0000FF">mmult</FONT></B> {<FONT COLOR="#B8860B">m1 m2</FONT>} {
    <B><FONT COLOR="#A020F0">set</FONT></B> cols [<B><FONT COLOR="#A020F0">lindex</FONT></B> $<FONT COLOR="#B8860B">m2</FONT> 0]
    <B><FONT COLOR="#A020F0">foreach</FONT></B> row1 $<FONT COLOR="#B8860B">m1</FONT> {
        <B><FONT COLOR="#A020F0">set</FONT></B> row [<B><FONT COLOR="#A020F0">list</FONT></B>]
        <B><FONT COLOR="#A020F0">set</FONT></B> i 0
        <B><FONT COLOR="#A020F0">foreach</FONT></B> - $<FONT COLOR="#B8860B">cols</FONT> {
            <B><FONT COLOR="#A020F0">set</FONT></B> elem 0
            <B><FONT COLOR="#A020F0">foreach</FONT></B> elem1 $<FONT COLOR="#B8860B">row1</FONT> row2 $<FONT COLOR="#B8860B">m2</FONT> {
                <B><FONT COLOR="#A020F0">set</FONT></B> elem [<B><FONT COLOR="#A020F0">expr</FONT></B> {$<FONT COLOR="#B8860B">elem</FONT> + $<FONT COLOR="#B8860B">elem1</FONT> * [<B><FONT COLOR="#A020F0">lindex</FONT></B> $<FONT COLOR="#B8860B">row2</FONT> $<FONT COLOR="#B8860B">i</FONT>]}]
            }
            <B><FONT COLOR="#A020F0">lappend</FONT></B> row $<FONT COLOR="#B8860B">elem</FONT>
            <B><FONT COLOR="#A020F0">incr</FONT></B> i
        }
        <B><FONT COLOR="#A020F0">lappend</FONT></B> result $<FONT COLOR="#B8860B">row</FONT>
    }
    <B><FONT COLOR="#A020F0">return</FONT></B> $<FONT COLOR="#B8860B">result</FONT>
}

<B><FONT COLOR="#A020F0">proc</FONT></B> <B><FONT COLOR="#0000FF">main</FONT></B> {<FONT COLOR="#B8860B"></FONT>} {
    <B><FONT COLOR="#A020F0">global</FONT></B> argv size
    <B><FONT COLOR="#A020F0">set</FONT></B> num [<B><FONT COLOR="#A020F0">lindex</FONT></B> $<FONT COLOR="#B8860B">argv</FONT> 0]
    <B><FONT COLOR="#A020F0">if</FONT></B> {$<FONT COLOR="#B8860B">num</FONT> &lt; 1} {
	<B><FONT COLOR="#A020F0">set</FONT></B> num 1
    }

    <B><FONT COLOR="#A020F0">set</FONT></B> m1 [mkmatrix $<FONT COLOR="#B8860B">size</FONT> $<FONT COLOR="#B8860B">size</FONT>]
    <B><FONT COLOR="#A020F0">set</FONT></B> m2 [mkmatrix $<FONT COLOR="#B8860B">size</FONT> $<FONT COLOR="#B8860B">size</FONT>]
    <B><FONT COLOR="#A020F0">while</FONT></B> {$<FONT COLOR="#B8860B">num</FONT> &gt; 0} {
        <B><FONT COLOR="#A020F0">incr</FONT></B> num -1
        <B><FONT COLOR="#A020F0">set</FONT></B> m [mmult $<FONT COLOR="#B8860B">m1</FONT> $<FONT COLOR="#B8860B">m2</FONT>]
    }

    <B><FONT COLOR="#A020F0">puts</FONT></B> <B><FONT COLOR="#BC8F8F">&quot;[lindex [lindex $m 0] 0] [lindex [lindex $m 2] 3] [lindex [lindex $m 3] 2] [lindex [lindex $m 4] 4]&quot;</FONT></B>
}

main</pre></td></tr></table>
