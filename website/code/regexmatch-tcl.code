<table width="100%" border="0" cellpadding="5" cellspacing="0"><tr><td bgcolor="#FFFFFF"><pre>
<I><FONT COLOR="#B22222">#!/usr/bin/tclsh
</FONT></I><I><FONT COLOR="#B22222"># $Id: regexmatch-tcl.code,v 1.2 2004-11-08 08:15:19 bfulgham Exp $
</FONT></I><I><FONT COLOR="#B22222"># http://www.bagley.org/~doug/shootout/
</FONT></I><I><FONT COLOR="#B22222"># from: Miguel Sofer, with modifications by Kristoffer Lawson
</FONT></I>
<B><FONT COLOR="#A020F0">proc</FONT></B> <B><FONT COLOR="#0000FF">main</FONT></B> {<FONT COLOR="#B8860B"></FONT>} {
    <B><FONT COLOR="#A020F0">global</FONT></B> argv
    <B><FONT COLOR="#A020F0">set</FONT></B> NUM [<B><FONT COLOR="#A020F0">lindex</FONT></B> $<FONT COLOR="#B8860B">argv</FONT> 0]
    <B><FONT COLOR="#A020F0">if</FONT></B> {$<FONT COLOR="#B8860B">NUM</FONT> &lt; 1} {
        <B><FONT COLOR="#A020F0">set</FONT></B> NUM 1
    }

    <B><FONT COLOR="#A020F0">set</FONT></B> data [<B><FONT COLOR="#A020F0">read</FONT></B> stdin]
    <B><FONT COLOR="#A020F0">set</FONT></B> count 0    
    <B><FONT COLOR="#A020F0">set</FONT></B> rExp {(?:^|[^\d(])(\(\d{3}\)|\d{3}) (\d{3}[ -]\d{4})(?:$|[^\d])}

    <B><FONT COLOR="#A020F0">while</FONT></B> {$<FONT COLOR="#B8860B">NUM</FONT>} {
	<B><FONT COLOR="#A020F0">incr</FONT></B> NUM -1
	<B><FONT COLOR="#A020F0">foreach</FONT></B> {-- area num} [<B><FONT COLOR="#A020F0">regexp</FONT></B> -all -line -inline $<FONT COLOR="#B8860B">rExp</FONT> $<FONT COLOR="#B8860B">data</FONT>] {
	    <B><FONT COLOR="#A020F0">set</FONT></B> pnum <B><FONT COLOR="#BC8F8F">&quot;([string trim $area () ]) [string map {&quot;</FONT></B> <B><FONT COLOR="#BC8F8F">&quot; -} $num]&quot;</FONT></B>
	    <B><FONT COLOR="#A020F0">if</FONT></B> {!$<FONT COLOR="#B8860B">NUM</FONT>} {
		<B><FONT COLOR="#A020F0">puts</FONT></B> <B><FONT COLOR="#BC8F8F">&quot;[incr count]: $pnum&quot;</FONT></B>
	    }
	}
    }
}

main</pre></td></tr></table>
