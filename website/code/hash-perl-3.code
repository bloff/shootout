</FONT></I>
<B><FONT COLOR="#A020F0">use</FONT></B> strict;

<B><FONT COLOR="#A020F0">my</FONT></B> $n = $ARGV[0] || 1;
<B><FONT COLOR="#A020F0">my</FONT></B> %X = ();
<B><FONT COLOR="#A020F0">keys</FONT></B> %X = $n / 3;
<B><FONT COLOR="#A020F0">my</FONT></B> $c = 0;

<B><FONT COLOR="#A020F0">for</FONT></B> <B><FONT COLOR="#A020F0">my</FONT></B> $i (1..$n) {
    $X{<B><FONT COLOR="#A020F0">sprintf</FONT></B>(<B><FONT COLOR="#BC8F8F">'%x'</FONT></B>, $i)} = $i;
}
<B><FONT COLOR="#A020F0">for</FONT></B> <B><FONT COLOR="#A020F0">my</FONT></B> $i (<B><FONT COLOR="#A020F0">reverse</FONT></B> 1..$n) {
    ++$c <B><FONT COLOR="#A020F0">if</FONT></B> <B><FONT COLOR="#A020F0">exists</FONT></B> $X{$i};
}
<B><FONT COLOR="#A020F0">print</FONT></B> <B><FONT COLOR="#BC8F8F">&quot;$c\n&quot;</FONT></B>;</pre></td></tr></table>
