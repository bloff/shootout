</span>	<span class="kwa">if</span> <span class="sym">(</span>strcmp<span class="sym">(*</span>av<span class="sym">,</span> <span class="str">&quot;-f&quot;</span><span class="sym">) ==</span> <span class="num">0</span><span class="sym">) {
</span>		nsieve <span class="sym">=</span> nsieve_boolarray<span class="sym">;
		++</span>av<span class="sym">;
	}</span> <span class="kwa">else
</span>		nsieve <span class="sym">=</span> nsieve_bitmap<span class="sym">;
</span>	<span class="kwa">if</span> <span class="sym">(*</span>av <span class="sym">==</span> NULL<span class="sym">)
</span>		<span class="kwa">goto</span> usage<span class="sym">;
</span>	n <span class="sym">=</span> strtoul<span class="sym">(*</span>av<span class="sym">, &amp;</span>cp<span class="sym">,</span> <span class="num">10</span><span class="sym">);
</span>	<span class="kwa">if</span> <span class="sym">(**</span>av <span class="sym">==</span> <span class="str">'</span><span class="esc">\0</span><span class="str">'</span> <span class="sym">|| *</span>cp <span class="sym">!=</span> <span class="str">'</span><span class="esc">\0</span><span class="str">'</span> <span class="sym">||</span> n <span class="sym">==</span> ULONG_MAX<span class="sym">)
</span>		<span class="kwa">goto</span> usage<span class="sym">;
</span>	test<span class="sym">(</span>nsieve<span class="sym">,</span> n<span class="sym">);
</span>	<span class="kwa">if</span> <span class="sym">(</span>n <span class="sym">&gt;=</span> <span class="num">1</span><span class="sym">)
</span>		test<span class="sym">(</span>nsieve<span class="sym">,</span> n <span class="num">- 1</span><span class="sym">);
</span>	<span class="kwa">if</span> <span class="sym">(</span>n <span class="sym">&gt;=</span> <span class="num">2</span><span class="sym">)
</span>		test<span class="sym">(</span>nsieve<span class="sym">,</span> n <span class="num">- 2</span><span class="sym">);
</span>	exit<span class="sym">(</span><span class="num">0</span><span class="sym">);
}</span>
