<span class="kwc">typedef</span> <span class="kwb">uint_fast8_t</span> bits<span class="sym">;
</span><span class="dir">#define	NBITS	(CHAR_BIT * sizeof(bits))
</span>
<span class="kwb">static uintmax_t
</span>nsieve_bitmap<span class="sym">(</span><span class="kwb">uintmax_t</span> m<span class="sym">)
{
</span>	<span class="kwb">uintmax_t</span> count<span class="sym">,</span> i<span class="sym">,</span> j<span class="sym">;
</span>	bits a<span class="sym">[</span>m <span class="sym">/</span> NBITS<span class="sym">];

</span>	memset<span class="sym">(</span>a<span class="sym">, (</span><span class="num">1</span> <span class="sym">&lt;&lt;</span> CHAR_BIT<span class="sym">) -</span> <span class="num">1</span><span class="sym">,</span> <span class="kwa">sizeof</span><span class="sym">(</span>a<span class="sym">));
</span>	count <span class="sym">=</span> <span class="num">0</span><span class="sym">;
</span>	<span class="kwa">for</span> <span class="sym">(</span>i <span class="sym">=</span> <span class="num">2</span><span class="sym">;</span> i <span class="sym">&lt;</span> m<span class="sym">; ++</span>i<span class="sym">)
</span>		<span class="kwa">if</span> <span class="sym">(</span>a<span class="sym">[</span>i <span class="sym">/</span> NBITS<span class="sym">] &amp; (</span><span class="num">1</span> <span class="sym">&lt;&lt;</span> i <span class="sym">%</span> NBITS<span class="sym">)) {
</span>			<span class="kwa">for</span> <span class="sym">(</span>j <span class="sym">=</span> i <span class="sym">+</span> i<span class="sym">;</span> j <span class="sym">&lt;</span> m<span class="sym">;</span> j <span class="sym">+=</span> i<span class="sym">)
</span>				a<span class="sym">[</span>j <span class="sym">/</span> NBITS<span class="sym">] &amp;= ~(</span><span class="num">1</span> <span class="sym">&lt;&lt;</span> j <span class="sym">%</span> NBITS<span class="sym">);
			++</span>count<span class="sym">;
		}
</span>	<span class="kwa">return</span> <span class="sym">(</span>count<span class="sym">);
}

</span><span class="kwb">static uintmax_t
</span>nsieve_boolarray<span class="sym">(</span><span class="kwb">uintmax_t</span> m<span class="sym">)
{
</span>	<span class="kwb">uintmax_t</span> count<span class="sym">,</span> i<span class="sym">,</span> j<span class="sym">;
</span>	<span class="kwb">char</span> a<span class="sym">[</span>m<span class="sym">];

</span>	memset<span class="sym">(</span>a<span class="sym">,</span> <span class="num">1</span><span class="sym">,</span> <span class="kwa">sizeof</span><span class="sym">(</span>a<span class="sym">));
</span>	count <span class="sym">=</span> <span class="num">0</span><span class="sym">;
</span>	<span class="kwa">for</span> <span class="sym">(</span>i <span class="sym">=</span> <span class="num">2</span><span class="sym">;</span> i <span class="sym">&lt;</span> m<span class="sym">; ++</span>i<span class="sym">)
</span>		<span class="kwa">if</span> <span class="sym">(</span>a<span class="sym">[</span>i<span class="sym">]) {
</span>			<span class="kwa">for</span> <span class="sym">(</span>j <span class="sym">=</span> i <span class="sym">+</span> i<span class="sym">;</span> j <span class="sym">&lt;</span> m<span class="sym">;</span> j <span class="sym">+=</span> i<span class="sym">)
</span>				a<span class="sym">[</span>j<span class="sym">] =</span> <span class="num">0</span><span class="sym">;
			++</span>count<span class="sym">;
