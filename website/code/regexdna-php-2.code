<span class="kwb">$contents</span> <span class="sym">=</span> <span class="kwa">preg_replace</span><span class="sym">(</span><span class="kwb">$IUB</span><span class="sym">,</span> <span class="kwb">$IUBnew</span><span class="sym">,</span> <span class="kwb">$contents</span><span class="sym">);</span>

<span class="kwa">print</span> <span class="str">&quot;</span><span class="esc">\n</span><span class="str">&quot;</span> .
