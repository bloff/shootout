<table width="100%" border="0" cellpadding="5" cellspacing="0"><tr><td bgcolor="#FFFFFF"><pre>
<I><FONT COLOR="#B22222"># -*- mode: icon -*-
</FONT></I><I><FONT COLOR="#B22222"># $Id: wordfreq-icon.code,v 1.1 2004-11-02 08:53:34 bfulgham Exp $
</FONT></I><I><FONT COLOR="#B22222"># http://shootout.alioth.debian.org/
</FONT></I>
<B><FONT COLOR="#A020F0">procedure </FONT></B><B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">main</FONT></I></B></FONT></B>(argv)
    <B><FONT COLOR="#5F9EA0">local</FONT></B> wcount, buckets, w, c, pair, wl

    wcount <B><FONT COLOR="#5F9EA0">:=</FONT></B> <B><FONT COLOR="#A020F0">table</FONT></B>(0)
    <B><FONT COLOR="#A020F0">every</FONT></B> wcount[words()] <B><FONT COLOR="#5F9EA0">+</FONT></B><B><FONT COLOR="#5F9EA0">:=</FONT></B> 1

    buckets <B><FONT COLOR="#5F9EA0">:=</FONT></B> <B><FONT COLOR="#A020F0">table</FONT></B>()
    <B><FONT COLOR="#A020F0">every</FONT></B> w <B><FONT COLOR="#5F9EA0">:=</FONT></B> <B><FONT COLOR="#A020F0">key</FONT></B>(wcount) <B><FONT COLOR="#A020F0">do</FONT></B> {
	c <B><FONT COLOR="#5F9EA0">:=</FONT></B> wcount[w]
	<B><FONT COLOR="#5F9EA0">/</FONT></B> buckets[c] <B><FONT COLOR="#5F9EA0">:=</FONT></B> <B><FONT COLOR="#A020F0">list</FONT></B>()
	<B><FONT COLOR="#A020F0">push</FONT></B>(buckets[c], w)
    }
    buckets <B><FONT COLOR="#5F9EA0">:=</FONT></B> <B><FONT COLOR="#A020F0">sort</FONT></B>(buckets,1)
    <B><FONT COLOR="#A020F0">while</FONT></B> pair <B><FONT COLOR="#5F9EA0">:=</FONT></B> <B><FONT COLOR="#A020F0">pull</FONT></B>(buckets) <B><FONT COLOR="#A020F0">do</FONT></B> {
 	c <B><FONT COLOR="#5F9EA0">:=</FONT></B> pair[1]
 	wl <B><FONT COLOR="#5F9EA0">:=</FONT></B> <B><FONT COLOR="#A020F0">sort</FONT></B>(pair[2],1)
	<B><FONT COLOR="#A020F0">while</FONT></B> (<B><FONT COLOR="#A020F0">write</FONT></B>(<B><FONT COLOR="#A020F0">right</FONT></B>(c,7), <B><FONT COLOR="#BC8F8F">&quot; &quot;</FONT></B>, <B><FONT COLOR="#A020F0">pull</FONT></B>(wl)))
    }
<B><FONT COLOR="#A020F0">end</FONT></B>

<B><FONT COLOR="#A020F0">procedure </FONT></B><B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">words</FONT></I></B></FONT></B>()
    <B><FONT COLOR="#5F9EA0">local</FONT></B> line, word
    <B><FONT COLOR="#A020F0">while</FONT></B> line <B><FONT COLOR="#5F9EA0">:=</FONT></B> <B><FONT COLOR="#A020F0">read</FONT></B>() <B><FONT COLOR="#A020F0">do</FONT></B> {
	line <B><FONT COLOR="#5F9EA0">:=</FONT></B> <B><FONT COLOR="#A020F0">map</FONT></B>(line)
	line ? <B><FONT COLOR="#A020F0">while</FONT></B> <B><FONT COLOR="#A020F0">tab</FONT></B>(<B><FONT COLOR="#A020F0">upto</FONT></B>(&amp;letters)) <B><FONT COLOR="#A020F0">do</FONT></B> {
	    word <B><FONT COLOR="#5F9EA0">:=</FONT></B> <B><FONT COLOR="#A020F0">tab</FONT></B>(<B><FONT COLOR="#A020F0">many</FONT></B>(&amp;letters))
	    <B><FONT COLOR="#5F9EA0">suspend</FONT></B>(word)
	}
    }
<B><FONT COLOR="#A020F0">end</FONT></B></pre></td></tr></table>
