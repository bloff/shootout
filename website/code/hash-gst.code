   To run: gst -QI /usr/local/share/smalltalk/gst.im hash.st -a 80000
&quot;</FONT></B>

| n count table |
n := Smalltalk arguments first asInteger.

count := 0.
table := Set <B><FONT COLOR="#A020F0">new</FONT></B>: n + (n // 5).

1 to: n <B><FONT COLOR="#A020F0">do</FONT></B>: [:<B><FONT COLOR="#A020F0">each</FONT></B>| table add: (<B><FONT COLOR="#A020F0">each</FONT></B> printString: 16)].

1 to: n <B><FONT COLOR="#A020F0">do</FONT></B>: [:<B><FONT COLOR="#A020F0">each</FONT></B> | 
   (table includes: <B><FONT COLOR="#A020F0">each</FONT></B> printString) ifTrue: [count := count + 1] ].

Transcript show: count printString; nl !</pre></td></tr></table>
