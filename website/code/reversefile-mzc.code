#!/usr/bin/mzscheme -r
<I><FONT COLOR="#B22222">;;; -*- mode: scheme -*-
</FONT></I><I><FONT COLOR="#B22222">;;; $Id: reversefile-mzc.code,v 1.1 2004-11-02 08:53:07 bfulgham Exp $
</FONT></I><I><FONT COLOR="#B22222">;;; http://shootout.alioth.debian.org/
</FONT></I><I><FONT COLOR="#B22222">;;; Provided by Bengt Kleberg
</FONT></I>(<B><FONT COLOR="#A020F0">define</FONT></B> <B><FONT COLOR="#0000FF">reverse-input</FONT></B>
   (<B><FONT COLOR="#A020F0">lambda</FONT></B> (inport)
     (<B><FONT COLOR="#A020F0">letrec</FONT></B> ((rev (<B><FONT COLOR="#A020F0">lambda</FONT></B> (line)
		    (<B><FONT COLOR="#A020F0">if</FONT></B> (eof-object? line)
                         'ok
		      (<B><FONT COLOR="#A020F0">begin</FONT></B>
		       (rev (read-line inport))
		       (display line)
		       (newline))))))
	    (rev  (read-line inport)))))

(<B><FONT COLOR="#A020F0">define</FONT></B> <B><FONT COLOR="#0000FF">main</FONT></B>
   (<B><FONT COLOR="#A020F0">lambda</FONT></B> ()
     (reverse-input (current-input-port))))

(main)</pre></td></tr></table>
