</FONT></I>package fibo;

import basic;
import sys;
import array;

private function <B><FONT COLOR="#0000FF">fib</FONT></B>( n )
{
    <B><FONT COLOR="#A020F0">if</FONT></B> (n &lt; 2) <B><FONT COLOR="#A020F0">return</FONT></B>(1);
    <B><FONT COLOR="#A020F0">return</FONT></B>(fib(n-2) + fib(n-1));
}


private n = 1;
<B><FONT COLOR="#0000FF">if</FONT></B>(array.length(sys.args) &gt; 0) {
    n = basic.<B><FONT COLOR="#228B22">int</FONT></B>(sys.args[0]);
} <B><FONT COLOR="#A020F0">else</FONT></B> {
    n = 1;
}
basic.print(fib(n), <B><FONT COLOR="#BC8F8F">&quot;\n&quot;</FONT></B>);</pre></td></tr></table>
