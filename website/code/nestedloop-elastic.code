</FONT></I>package nestedloop;

import basic;
import sys;
import array;

private n = 1;
<B><FONT COLOR="#0000FF">if</FONT></B>(array.length(sys.args) &gt; 0) {
    n = basic.<B><FONT COLOR="#228B22">int</FONT></B>(sys.args[0]);
} <B><FONT COLOR="#A020F0">else</FONT></B> {
    n = 1;
}
private a, b, c, d, e, f, x = 0;

<B><FONT COLOR="#0000FF">for</FONT></B> (a=0; a&lt;n; a++)
    <B><FONT COLOR="#A020F0">for</FONT></B> (b=0; b&lt;n; b++)
        <B><FONT COLOR="#A020F0">for</FONT></B> (c=0; c&lt;n; c++)
            <B><FONT COLOR="#A020F0">for</FONT></B> (d=0; d&lt;n; d++)
                <B><FONT COLOR="#A020F0">for</FONT></B> (e=0; e&lt;n; e++)
                    <B><FONT COLOR="#A020F0">for</FONT></B> (f=0; f&lt;n; f++)
                        x++;
basic.print(x, <B><FONT COLOR="#BC8F8F">&quot;\n&quot;</FONT></B>);</pre></td></tr></table>
