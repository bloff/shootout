using System;
using System.Collections;

class hash {
    public <B><FONT COLOR="#228B22">static</FONT></B> <B><FONT COLOR="#228B22">int</FONT></B> Main(String[] args) {        
        Hashtable X = new Hashtable();     
        <B><FONT COLOR="#228B22">int</FONT></B> c = 0;
        
        <B><FONT COLOR="#228B22">int</FONT></B> n = System.Convert.ToInt32(args[0]);
        <B><FONT COLOR="#A020F0">if</FONT></B>(n &lt; 1) n = 1;
        
        <B><FONT COLOR="#A020F0">for</FONT></B>(<B><FONT COLOR="#228B22">int</FONT></B> i = 1; i &lt;= n; i++) {
            X.Add( i.ToString(<B><FONT COLOR="#BC8F8F">&quot;x&quot;</FONT></B>), i);
        }
        
        <B><FONT COLOR="#A020F0">for</FONT></B>(<B><FONT COLOR="#228B22">int</FONT></B> i = n; i &gt; 0; i--) {
            <B><FONT COLOR="#A020F0">if</FONT></B>(X.ContainsKey(i.ToString())) c++;
        }
        Console.WriteLine(c);
        <B><FONT COLOR="#A020F0">return</FONT></B>(0);
    }
}</pre></td></tr></table>
