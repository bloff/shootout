</FONT></I>package xhash;

import basic;
import sys;
import array;
import hash;

private n = 1;
private i;
private c = 0;
private X = %[];

<B><FONT COLOR="#0000FF">if</FONT></B>(array.length(sys.args) &gt; 0) {
    n = basic.<B><FONT COLOR="#228B22">int</FONT></B>(sys.args[0]);
} <B><FONT COLOR="#A020F0">else</FONT></B> {
    n = 1;
}
<B><FONT COLOR="#0000FF">for</FONT></B>(i=1; i&lt;=n; i++) {
    X[basic.sprintf(<B><FONT COLOR="#BC8F8F">&quot;%x&quot;</FONT></B>, i)] = i;
}
<B><FONT COLOR="#0000FF">for</FONT></B>(i=n; i&gt;0; i=i-1) {
     <B><FONT COLOR="#A020F0">if</FONT></B>(hash.has_key(X, basic.sprintf(<B><FONT COLOR="#BC8F8F">&quot;%d&quot;</FONT></B>, i))) {
        c = c + 1;
    }
}
basic.print(c, <B><FONT COLOR="#BC8F8F">&quot;\n&quot;</FONT></B>);</pre></td></tr></table>
