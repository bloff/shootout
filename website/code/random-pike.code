#!/<B><FONT COLOR="#5F9EA0">usr/bin/pike</FONT></B>
<I><FONT COLOR="#B22222">// -*- mode: pike -*-
</FONT></I><I><FONT COLOR="#B22222">// $Id: random-pike.code,v 1.2 2004-11-08 08:15:19 bfulgham Exp $
</FONT></I><I><FONT COLOR="#B22222">// http://www.bagley.org/~doug/shootout/
</FONT></I>
<B><FONT COLOR="#228B22">int</FONT></B> IM = 139968;
<B><FONT COLOR="#228B22">int</FONT></B> IA = 3877;
<B><FONT COLOR="#228B22">int</FONT></B> IC = 29573;

<B><FONT COLOR="#228B22">int</FONT></B> last = 42;

<B><FONT COLOR="#228B22">float</FONT></B>
<B><FONT COLOR="#0000FF">gen_random</FONT></B>(<B><FONT COLOR="#228B22">float</FONT></B> max) { <B><FONT COLOR="#A020F0">return</FONT></B>(max * (last = (last * IA + IC) % IM) / IM); }

<B><FONT COLOR="#228B22">int</FONT></B>
<B><FONT COLOR="#0000FF">main</FONT></B>(<B><FONT COLOR="#228B22">int</FONT></B> argc, array(string) argv) {
    <B><FONT COLOR="#228B22">int</FONT></B> N = ((<B><FONT COLOR="#228B22">int</FONT></B>)argv[-1] || 1) - 1;
    <B><FONT COLOR="#A020F0">while</FONT></B> (N--) {
	gen_random(100.0);
    }
    write(<B><FONT COLOR="#BC8F8F">&quot;%.9f\n&quot;</FONT></B>, gen_random(100.0));
    <B><FONT COLOR="#A020F0">return</FONT></B>(0);
}
</pre></td></tr></table>
