define <B><FONT COLOR="#0000FF">main</FONT></B> ()
{
   variable n = 1;
   <B><FONT COLOR="#A020F0">if</FONT></B> (__argc &gt; 1) n = integer(__argv[1]);
   variable hash1 = Assoc_Type[Int_Type];
   _for (1, 10000, 1)
     {
	variable i = ();
	hash1[strcat (<B><FONT COLOR="#BC8F8F">&quot;foo_&quot;</FONT></B>, string(i))] = i;
     }

   variable hash2 = Assoc_Type[Int_Type, 0];
   loop (n)
     {
	foreach (hash1) using (<B><FONT COLOR="#BC8F8F">&quot;keys&quot;</FONT></B>, <B><FONT COLOR="#BC8F8F">&quot;values&quot;</FONT></B>)
	  {
	     variable k, v;
	     (k,v) = ();
	     !<B><FONT COLOR="#A020F0">if</FONT></B> (assoc_key_exists (hash2, k))
	       <B><FONT COLOR="#A020F0">continue</FONT></B>;
	     hash2[k] += v;
	  }
     }

   fprintf (stdout, <B><FONT COLOR="#BC8F8F">&quot;%u %u %u %u\n&quot;</FONT></B>,
	     hash1[<B><FONT COLOR="#BC8F8F">&quot;foo_1&quot;</FONT></B>], hash1[<B><FONT COLOR="#BC8F8F">&quot;foo_9999&quot;</FONT></B>], 
	     hash2[<B><FONT COLOR="#BC8F8F">&quot;foo_1&quot;</FONT></B>], hash2[<B><FONT COLOR="#BC8F8F">&quot;foo_9999&quot;</FONT></B>]);
}

<B><FONT COLOR="#0000FF">main</FONT></B>();

