ack(0,N,Val) :- Val <B><FONT COLOR="#A020F0">is</FONT></B> N + 1.
ack(M,0,Val) :- M &gt; 0, M1 <B><FONT COLOR="#A020F0">is</FONT></B> M - 1, ack(M1,1,Val).
ack(M,N,Val) :- M &gt; 0, N &gt; 0, M1 <B><FONT COLOR="#A020F0">is</FONT></B> M - 1, N1 <B><FONT COLOR="#A020F0">is</FONT></B> N -1,
		ack(M, N1, Val1), ack(M1, Val1, Val).

main :-
	argument_value(1, ACount),
	number_atom(Count, ACount),
	<B><FONT COLOR="#A020F0">write</FONT></B>(<B><FONT COLOR="#BC8F8F">'Ack(3,'</FONT></B>),
	<B><FONT COLOR="#A020F0">write</FONT></B>(Count),
	<B><FONT COLOR="#A020F0">write</FONT></B>(<B><FONT COLOR="#BC8F8F">'): '</FONT></B>),
	ack(3,Count,Val),
	<B><FONT COLOR="#A020F0">write</FONT></B>(Val),
	nl,
	halt.

:- <B><FONT COLOR="#A020F0">initialization</FONT></B>(main).</pre></td></tr></table>
