variable size = 30;

define <B><FONT COLOR="#0000FF">mkmatrix</FONT></B>(rows, cols)
{
   variable mx = [1:rows*cols];
   reshape (mx, [rows, cols]);
   <B><FONT COLOR="#A020F0">return</FONT></B> mx;
}

define <B><FONT COLOR="#0000FF">main</FONT></B>()
{
   variable iter = 1;
   <B><FONT COLOR="#A020F0">if</FONT></B> (__argc &gt; 1) iter = integer (__argv[1]);
   variable m1 = mkmatrix(size, size);
   variable m2 = mkmatrix(size, size);
   
   loop (iter)
     variable mm = m1 # m2;

   fprintf (stdout, <B><FONT COLOR="#BC8F8F">&quot;%.10g %.10g %.10g %.10g\n&quot;</FONT></B>, mm[0,0], mm[2,3], mm[3,2], mm[4,4]);
}

<B><FONT COLOR="#0000FF">main</FONT></B> ();</pre></td></tr></table>
