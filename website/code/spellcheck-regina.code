TRUE <span class="sym">=</span> <span class="num">1</span> <span class="sym">;</span> NULL <span class="sym">=</span> <span class="str">&quot;&quot;</span> <span class="sym">;</span> DICTIONARY <span class="sym">=</span> <span class="str">&quot;Usr.Dict.Words&quot;</span>

parse value LINEIN<span class="sym">(</span>DICTIONARY<span class="sym">)</span> with word
<span class="kwa">do while</span> word <span class="sym">&lt;&gt;</span> NULL
  dict<span class="sym">.</span>word <span class="sym">=</span> TRUE
  parse value LINEIN<span class="sym">(</span>DICTIONARY<span class="sym">)</span> with word
end

parse value LINEIN<span class="sym">()</span> with word
<span class="kwa">do while</span> word <span class="sym">&lt;&gt;</span> NULL
  <span class="kwa">if</span> SYMBOL<span class="sym">(</span><span class="str">'dict.word'</span><span class="sym">)</span> \<span class="sym">=</span> <span class="str">'VAR'</span> then <span class="sym">;</span> say word
  parse value LINEIN<span class="sym">()</span> with word
end
