    <span class="sym">|</span> n <span class="sym">&gt;</span> stop  <span class="sym">=</span> <span class="kwa">True</span>
    <span class="sym">|</span> otherwise <span class="sym">=</span> <span class="kwa">False</span>
  <span class="kwa">where</span>
