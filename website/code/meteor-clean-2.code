masksAtCell <span class="sym">=:</span> transpose {values <span class="sym">(</span>masksForColor c<span class="sym">)</span> <span class="num">10 0</span> <span class="sym">(</span>createArray <span class="sym">(</span><span class="num">1024</span><span class="sym">*</span>N_ROW<span class="sym">) [</span>#<span class="sym">!])</span> \\ c <span class="sym">&lt;|-</span> COLORS}
  <span class="kwa">where</span>
    values <span class="sym">:: !</span>{<span class="sym">![</span>#<span class="kwb">Int</span><span class="sym">!]</span>} <span class="sym">!</span><span class="kwb">Int</span> <span class="sym">!</span><span class="kwb">Int</span> <span class="sym">!*</span>{<span class="sym">![</span>#<span class="kwb">Int</span><span class="sym">!]</span>} <span class="sym">-&gt;</span> {<span class="sym">![</span>#<span class="kwb">Int</span><span class="sym">!]</span>}
    values a <span class="num">0 0</span> result <span class="sym">=</span> result
    values a y <span class="num">0</span> result <span class="sym">=</span> values a <span class="sym">(</span>dec y<span class="sym">)</span> <span class="num">1024</span> result
    values a y n result
        #<span class="sym">!</span> n <span class="sym">=</span> dec n
        <span class="sym">=</span>  values a y n {result <span class="sym">&amp; [</span>y<span class="sym">*</span><span class="num">1024</span><span class="sym">+</span>n<span class="sym">]=[</span>#m \\ m <span class="sym">&lt;|-</span> a.<span class="sym">[</span><span class="num">5</span><span class="sym">*</span>y<span class="sym">+</span>firstZero.<span class="sym">[</span>n bitand <span class="num">31</span><span class="sym">]]</span>
                                  <span class="sym">|</span>m bitand n <span class="sym">==</span> <span class="num">0</span> <span class="sym">&amp;&amp;</span> noSimpleIslands <span class="sym">(</span>m bitor n<span class="sym">) (</span>isOdd y<span class="sym">)!]</span>}
    transpose <span class="sym">::</span>{<span class="sym">!</span>{<span class="sym">![</span>#<span class="kwb">Int</span><span class="sym">!]</span>}} <span class="sym">-&gt;</span> MaskData
    transpose a <span class="sym">=</span> {{a.<span class="sym">[</span>i<span class="sym">,</span> j<span class="sym">]</span> \\ i <span class="sym">&lt;|-</span> COLORS} \\ j <span class="sym">&lt;|- [</span>#<span class="num">0</span>.<span class="num">.1024</span><span class="sym">*</span>N_ROW<span class="sym">-</span><span class="num">1</span><span class="sym">!]</span>}


