<span class="line">    1 </span>#<span class="sym">!/</span>usr<span class="sym">/</span>bin<span class="sym">/</span>newlisp
<span class="line">    2 </span>
<span class="line">    3 </span><span class="slc">;; moments - benchmark
</span><span class="line">    4 </span><span class="slc"></span><span class="slc">;;
</span><span class="line">    5 </span><span class="slc"></span><span class="slc">;; Contributed by Lutz Mueller
</span><span class="line">    6 </span><span class="slc"></span><span class="slc">;;
</span><span class="line">    7 </span><span class="slc"></span><span class="slc">;; read file filter empty lines
</span><span class="line">    8 </span><span class="slc"></span><span class="sym">(</span>define <span class="sym">(</span>main<span class="sym">)
</span><span class="line">    9 </span><span class="sym">  (</span><span class="kwa">while</span> <span class="sym">(</span><span class="kwa">read</span><span class="sym">-</span><span class="kwa">line</span><span class="sym">) (</span>push <span class="sym">(</span><span class="kwa">float</span> <span class="sym">(</span>current<span class="sym">-</span><span class="kwa">line</span><span class="sym">))</span> nums<span class="sym">))
</span><span class="line">   10 </span><span class="sym">  (</span><span class="kwa">set</span> <span class="str">'nums (reverse (filter float? nums)))
</span><span class="line">   11 </span><span class="str">  (set '</span>n <span class="sym">(</span><span class="kwa">length</span> nums<span class="sym">))
</span><span class="line">   12 </span><span class="sym">
</span><span class="line">   13 </span><span class="sym">
</span><span class="line">   14 </span><span class="sym">  (</span><span class="kwa">set</span> <span class="str">'sum (apply add nums))
</span><span class="line">   15 </span><span class="str">  (set '</span>mean <span class="sym">(</span>div sum n<span class="sym">))
</span><span class="line">   16 </span><span class="sym">
</span><span class="line">   17 </span><span class="sym">  (</span><span class="kwa">set</span> <span class="str">'avg-dev 0 '</span>std<span class="sym">-</span>dev <span class="num">0</span> <span class="str">'var 0 '</span>skew <span class="num">0</span> <span class="str">'kurtosis 0)
</span><span class="line">   18 </span><span class="str">
</span><span class="line">   19 </span><span class="str">  (set '</span>dev <span class="sym">(</span>map sub nums <span class="sym">(</span>dup mean n<span class="sym">)))
</span><span class="line">   20 </span><span class="sym">  (</span><span class="kwa">set</span> <span class="str">'avg-dev (div (apply add (map abs dev)) n))
</span><span class="line">   21 </span><span class="str">  (set '</span>var <span class="sym">(</span>div <span class="sym">(</span><span class="kwa">apply</span> add <span class="sym">(</span>map mul dev dev<span class="sym">)) (-</span> n <span class="num">1</span><span class="sym">)))
</span><span class="line">   22 </span><span class="sym">  (</span><span class="kwa">set</span> <span class="str">'skew (apply add (map mul dev dev dev)))
</span><span class="line">   23 </span><span class="str">  (set '</span>kurtosis <span class="sym">(</span><span class="kwa">apply</span> add <span class="sym">(</span>map  mul dev dev dev dev<span class="sym">)))
</span><span class="line">   24 </span><span class="sym">
</span><span class="line">   25 </span><span class="sym">  (</span><span class="kwa">set</span> <span class="str">'std-dev (sqrt var))
</span><span class="line">   26 </span><span class="str">
</span><span class="line">   27 </span><span class="str">  (if (&gt; var 0.0)
</span><span class="line">   28 </span><span class="str">    (begin
</span><span class="line">   29 </span><span class="str">        (set '</span>skew <span class="sym">(</span>div skew <span class="sym">(</span>mul n var std<span class="sym">-</span>dev<span class="sym">)))
</span><span class="line">   30 </span><span class="sym">        (</span><span class="kwa">set</span> <span class="str">'kurtosis (sub (div kurtosis (mul n var var)) 3.0))))
</span><span class="line">   31 </span><span class="str">
</span><span class="line">   32 </span><span class="str">  (sort nums)
</span><span class="line">   33 </span><span class="str">  (set '</span>mid <span class="sym">(/</span> n <span class="num">2</span><span class="sym">))
</span><span class="line">   34 </span><span class="sym">
</span><span class="line">   35 </span><span class="sym">  (</span><span class="kwa">if</span> <span class="sym">(=</span> <span class="num">0</span> <span class="sym">(</span>% n <span class="num">2</span><span class="sym">))
</span><span class="line">   36 </span><span class="sym">        (</span><span class="kwa">set</span> <span class="str">'median (div (add (nth mid nums) (nth (- mid 1) nums)) 2))
</span><span class="line">   37 </span><span class="str">        (set '</span>median <span class="sym">(</span><span class="kwa">nth</span> mid nums<span class="sym">)))
</span><span class="line">   38 </span><span class="sym">
</span><span class="line">   39 </span><span class="sym">  (</span>println <span class="sym">(</span>format <span class="str">&quot;n:                  %d&quot;</span> n<span class="sym">))
</span><span class="line">   40 </span><span class="sym">  (</span>println <span class="sym">(</span>format <span class="str">&quot;median:             %f&quot;</span> median<span class="sym">))
</span><span class="line">   41 </span><span class="sym">  (</span>println <span class="sym">(</span>format <span class="str">&quot;mean:               %f&quot;</span> mean<span class="sym">))
</span><span class="line">   42 </span><span class="sym">  (</span>println <span class="sym">(</span>format <span class="str">&quot;average_deviation:  %f&quot;</span> avg<span class="sym">-</span>dev<span class="sym">))
</span><span class="line">   43 </span><span class="sym">  (</span>println <span class="sym">(</span>format <span class="str">&quot;standard_deviation: %f&quot;</span> std<span class="sym">-</span>dev<span class="sym">))
</span><span class="line">   44 </span><span class="sym">  (</span>println <span class="sym">(</span>format <span class="str">&quot;variance:           %f&quot;</span> var<span class="sym">))
</span><span class="line">   45 </span><span class="sym">  (</span>println <span class="sym">(</span>format <span class="str">&quot;skew:               %f&quot;</span> skew<span class="sym">))
</span><span class="line">   46 </span><span class="sym">  (</span>println <span class="sym">(</span>format <span class="str">&quot;kurtosis:           %f&quot;</span> kurtosis<span class="sym">))
</span><span class="line">   47 </span><span class="sym">)
</span><span class="line">   48 </span><span class="sym">
</span><span class="line">   49 </span><span class="sym">(</span>main<span class="sym">)
</span><span class="line">   50 </span><span class="sym">
</span><span class="line">   51 </span><span class="sym"></span><span class="slc">;; eof</span>
