<table width="100%" border="0" cellpadding="5" cellspacing="0"><tr><td bgcolor="#FFFFFF"><pre>
<I><FONT COLOR="#B22222">-- -*- mode: eiffel -*-
</FONT></I><I><FONT COLOR="#B22222">-- $Id: hash-se.code,v 1.1 2004-11-02 08:52:31 bfulgham Exp $
</FONT></I><I><FONT COLOR="#B22222">-- http://www.bagley.org/~doug/shootout/
</FONT></I>
<B><FONT COLOR="#A020F0">class</FONT></B> HASH
   
<B><FONT COLOR="#A020F0">creation</FONT></B> make
   
<B><FONT COLOR="#A020F0">feature</FONT></B> <I><FONT COLOR="#B22222">-- Initialization
</FONT></I>   
<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">   make</FONT></I></B></FONT></B> <B><FONT COLOR="#A020F0">is</FONT></B>
      <B><FONT COLOR="#A020F0">local</FONT></B>
<B><FONT COLOR="#5F9EA0">	 n : </FONT></B><B><FONT COLOR="#228B22">INTEGER</FONT></B>
<B><FONT COLOR="#5F9EA0">	 i, j, c : </FONT></B><B><FONT COLOR="#228B22">INTEGER</FONT></B>
<B><FONT COLOR="#5F9EA0">	 s : </FONT></B><B><FONT COLOR="#228B22">STRING</FONT></B>
<B><FONT COLOR="#5F9EA0">	 ht : </FONT></B><B><FONT COLOR="#228B22">DICTIONARY</FONT></B>[<B><FONT COLOR="#228B22">INTEGER</FONT></B>, <B><FONT COLOR="#228B22">STRING</FONT></B>]
	 
<B><FONT COLOR="#5F9EA0">	 arg: </FONT></B><B><FONT COLOR="#228B22">ARGUMENTS</FONT></B>
      <B><FONT COLOR="#A020F0">do</FONT></B>
	 n <B><FONT COLOR="#5F9EA0">:=</FONT></B> argument(1).to_integer

	 <B><FONT COLOR="#5F9EA0">!!</FONT></B>ht.with_capacity(n);
	 
	 <B><FONT COLOR="#A020F0">from</FONT></B>  i <B><FONT COLOR="#5F9EA0">:=</FONT></B> 1
	 <B><FONT COLOR="#A020F0">until</FONT></B> i &gt; n
	 <B><FONT COLOR="#A020F0">loop</FONT></B>
	    s <B><FONT COLOR="#5F9EA0">:=</FONT></B> i.to_hexadecimal
	    <B><FONT COLOR="#A020F0">from</FONT></B>  j <B><FONT COLOR="#5F9EA0">:=</FONT></B> 1
	    <B><FONT COLOR="#A020F0">until</FONT></B> s @ j <B><FONT COLOR="#5F9EA0">/=</FONT></B> <B><FONT COLOR="#BC8F8F">'0'</FONT></B>
	    <B><FONT COLOR="#A020F0">loop</FONT></B>  j <B><FONT COLOR="#5F9EA0">:=</FONT></B> j + 1
	    <B><FONT COLOR="#A020F0">end</FONT></B>
	    ht.put (i, s.substring(j,s.count))
	    i <B><FONT COLOR="#5F9EA0">:=</FONT></B> i+1
	 <B><FONT COLOR="#A020F0">end</FONT></B>
	 
	 <B><FONT COLOR="#A020F0">from</FONT></B>  i <B><FONT COLOR="#5F9EA0">:=</FONT></B> n
	 <B><FONT COLOR="#A020F0">until</FONT></B> i <B><FONT COLOR="#5F9EA0">=</FONT></B> 0
	 <B><FONT COLOR="#A020F0">loop</FONT></B>
	    s <B><FONT COLOR="#5F9EA0">:=</FONT></B> i.to_string
 	    <B><FONT COLOR="#A020F0">if</FONT></B> ht.has (s) <B><FONT COLOR="#A020F0">then</FONT></B>
	       c <B><FONT COLOR="#5F9EA0">:=</FONT></B> c + 1
	    <B><FONT COLOR="#A020F0">end</FONT></B>
	    i <B><FONT COLOR="#5F9EA0">:=</FONT></B> i - 1
	 <B><FONT COLOR="#A020F0">end</FONT></B>
	 
	 print (c.out + <B><FONT COLOR="#BC8F8F">&quot;%N&quot;</FONT></B>)

      <B><FONT COLOR="#A020F0">end</FONT></B>

<B><FONT COLOR="#A020F0">end</FONT></B> <I><FONT COLOR="#B22222">-- class HASH
</FONT></I></pre></td></tr></table>
