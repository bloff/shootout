</FONT></I>
<B><FONT COLOR="#228B22">functor</FONT></B>
<B><FONT COLOR="#5F9EA0">import</FONT></B>
  System
  Application
<B><FONT COLOR="#A020F0">define</FONT></B>
 <B><FONT COLOR="#5F9EA0">local</FONT></B> Args N A1 A2 <B><FONT COLOR="#A020F0">in</FONT></B>

   {<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">Application</FONT></I></B></FONT></B>.getCmdArgs plain Args}
   <B><FONT COLOR="#A020F0">if</FONT></B> {<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">List</FONT></I></B></FONT></B>.length Args} \<B><FONT COLOR="#5F9EA0">=</FONT></B> 1 <B><FONT COLOR="#A020F0">then</FONT></B> 
     N <B><FONT COLOR="#5F9EA0">=</FONT></B> 1
   <B><FONT COLOR="#A020F0">else</FONT></B>
     {<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">String</FONT></I></B></FONT></B>.toInt Args.1 N}
   <B><FONT COLOR="#A020F0">end</FONT></B>

   {<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">NewArray</FONT></I></B></FONT></B> 0 N 0 A1} 
   {<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">NewArray</FONT></I></B></FONT></B> 0 N 0 A2} 

   {<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">For</FONT></I></B></FONT></B> 0 (N - 1) 1 
     <B><FONT COLOR="#A020F0">proc</FONT></B> {$ I} {<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">Put</FONT></I></B></FONT></B> A1 I (I + 1)} <B><FONT COLOR="#A020F0">end</FONT></B> }

   {<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">For</FONT></I></B></FONT></B> 0 999 1 
      <B><FONT COLOR="#A020F0">proc</FONT></B> {$ I} 
        {<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">For</FONT></I></B></FONT></B> (N - 1) 0 ~1 
          <B><FONT COLOR="#A020F0">proc</FONT></B> {$ I} {<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">Put</FONT></I></B></FONT></B> A2 I ({<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">Array</FONT></I></B></FONT></B>.get A2 I} + {<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">Get</FONT></I></B></FONT></B> A1 I})} <B><FONT COLOR="#A020F0">end</FONT></B>}
      <B><FONT COLOR="#A020F0">end</FONT></B>}


   {<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">System</FONT></I></B></FONT></B>.showInfo {<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">Get</FONT></I></B></FONT></B> A2 0}#<B><FONT COLOR="#BC8F8F">&quot; &quot;</FONT></B>#{<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">Get</FONT></I></B></FONT></B> A2 (N - 1)}}
   {<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">Application</FONT></I></B></FONT></B>.exit 0}
 <B><FONT COLOR="#A020F0">end</FONT></B>
<B><FONT COLOR="#A020F0">end</FONT></B></pre></td></tr></table>
