using System;
using System.Collections;

class reversefile
{
    [STAThread]
    <B><FONT COLOR="#228B22">static</FONT></B> <B><FONT COLOR="#228B22">void</FONT></B> Main(string[] args)
    {
        Stack lines = new Stack();
        String line;
        <B><FONT COLOR="#A020F0">while</FONT></B> ( (line = Console.ReadLine()) != null ) lines.Push(line);

        System.Collections.IEnumerator items = lines.GetEnumerator();
        <B><FONT COLOR="#A020F0">while</FONT></B> ( items.MoveNext() ) {
	    Console.WriteLine( items.Current );
	}
    }
