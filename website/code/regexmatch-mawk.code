<B><FONT COLOR="#A020F0">BEGIN</FONT></B> {
    n = (<B><FONT COLOR="#A020F0">ARGV</FONT></B>[1] &lt; 1) ? 1 : <B><FONT COLOR="#A020F0">ARGV</FONT></B>[1];
    <B><FONT COLOR="#A020F0">delete</FONT></B> <B><FONT COLOR="#A020F0">ARGV</FONT></B>;
}

{ phones[p++] = $0; }

<B><FONT COLOR="#A020F0">END</FONT></B> {
    <B><FONT COLOR="#A020F0">for</FONT></B> (i=0; i&lt;n; i++) {
	<B><FONT COLOR="#A020F0">for</FONT></B> (j=0; j&lt;p; j++) {
	    line = phones[j];
	    <B><FONT COLOR="#A020F0">if</FONT></B> (<B><FONT COLOR="#A020F0">match</FONT></B>(line, /(^|[^0-9])(\([0-9][0-9][0-9]\)|[0-9][0-9][0-9]) [0-9][0-9][0-9][ -][0-9][0-9][0-9][0-9]($|[^0-9])/)) {
		m1 = <B><FONT COLOR="#A020F0">substr</FONT></B>(line, <B><FONT COLOR="#A020F0">RSTART</FONT></B>, <B><FONT COLOR="#A020F0">RLENGTH</FONT></B>);
		num = <B><FONT COLOR="#BC8F8F">&quot;&quot;</FONT></B>
		<B><FONT COLOR="#A020F0">if</FONT></B> (<B><FONT COLOR="#A020F0">match</FONT></B>(m1, /[0-9][0-9][0-9] [0-9][0-9][0-9][ -][0-9][0-9][0-9][0-9]/)) {
		    <B><FONT COLOR="#A020F0">if</FONT></B> (<B><FONT COLOR="#A020F0">substr</FONT></B>(m1, <B><FONT COLOR="#A020F0">RSTART</FONT></B>-1, 1) == <B><FONT COLOR="#BC8F8F">&quot;(&quot;</FONT></B>) {
			<B><FONT COLOR="#A020F0">break</FONT></B>;
		    }
		    <B><FONT COLOR="#A020F0">if</FONT></B> (x = <B><FONT COLOR="#A020F0">split</FONT></B>(<B><FONT COLOR="#A020F0">substr</FONT></B>(m1, <B><FONT COLOR="#A020F0">RSTART</FONT></B>, <B><FONT COLOR="#A020F0">RLENGTH</FONT></B>), parts, /[ -]/)) {
			num = <B><FONT COLOR="#BC8F8F">&quot;(&quot;</FONT></B> parts[1] <B><FONT COLOR="#BC8F8F">&quot;) &quot;</FONT></B> parts[2] <B><FONT COLOR="#BC8F8F">&quot;-&quot;</FONT></B> parts[3];
		    }
		} <B><FONT COLOR="#A020F0">else</FONT></B> <B><FONT COLOR="#A020F0">if</FONT></B> (<B><FONT COLOR="#A020F0">match</FONT></B>(m1, /\([0-9][0-9][0-9]\) [0-9][0-9][0-9][ -][0-9][0-9][0-9][0-9]/)) {
		    <B><FONT COLOR="#A020F0">if</FONT></B> (x = <B><FONT COLOR="#A020F0">split</FONT></B>(<B><FONT COLOR="#A020F0">substr</FONT></B>(m1, <B><FONT COLOR="#A020F0">RSTART</FONT></B>, <B><FONT COLOR="#A020F0">RLENGTH</FONT></B>), parts, /[ -]/)) {
			num = parts[1] <B><FONT COLOR="#BC8F8F">&quot; &quot;</FONT></B> parts[2] <B><FONT COLOR="#BC8F8F">&quot;-&quot;</FONT></B> parts[3];
		    }
		}
		<B><FONT COLOR="#A020F0">if</FONT></B> (i == (n-1)) {
		    count++;
		    <B><FONT COLOR="#A020F0">printf</FONT></B>(<B><FONT COLOR="#BC8F8F">&quot;%d: %s\n&quot;</FONT></B>, count, num);
		}
	    }
	}
    }
}</pre></td></tr></table>
