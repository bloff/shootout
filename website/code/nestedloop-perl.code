<table width="100%" border="0" cellpadding="5" cellspacing="0"><tr><td bgcolor="#FFFFFF"><pre>
<I><FONT COLOR="#B22222">#!/usr/bin/perl
</FONT></I><I><FONT COLOR="#B22222"># $Id: nestedloop-perl.code,v 1.1 2004-11-02 08:52:37 bfulgham Exp $
</FONT></I><I><FONT COLOR="#B22222"># http://www.bagley.org/~doug/shootout/
</FONT></I>
<B><FONT COLOR="#A020F0">use</FONT></B> strict;

<B><FONT COLOR="#A020F0">my</FONT></B> $n = ($ARGV[0] &gt; 0) ? $ARGV[0] : 1;
<B><FONT COLOR="#A020F0">my</FONT></B> $x = 0;
<B><FONT COLOR="#A020F0">my</FONT></B> $a = $n;
<B><FONT COLOR="#A020F0">while</FONT></B> ($a--) {
    <B><FONT COLOR="#A020F0">my</FONT></B> $b = $n;
    <B><FONT COLOR="#A020F0">while</FONT></B> ($b--) {
	<B><FONT COLOR="#A020F0">my</FONT></B> $c = $n;
	<B><FONT COLOR="#A020F0">while</FONT></B> ($c--) {
	    <B><FONT COLOR="#A020F0">my</FONT></B> $d = $n;
	    <B><FONT COLOR="#A020F0">while</FONT></B> ($d--) {
		<B><FONT COLOR="#A020F0">my</FONT></B> $e = $n;
		<B><FONT COLOR="#A020F0">while</FONT></B> ($e--) {
		    <B><FONT COLOR="#A020F0">my</FONT></B> $f = $n;
		    <B><FONT COLOR="#A020F0">while</FONT></B> ($f--) {
			$x++;
		    }
		}
	    }
	}
    }
}
<B><FONT COLOR="#A020F0">print</FONT></B> <B><FONT COLOR="#BC8F8F">&quot;$x\n&quot;</FONT></B>;</pre></td></tr></table>
