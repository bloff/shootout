<span class="str">: remove-newlines ( addr u -- addr u' )</span>
<span class="str">    BEGIN</span>
<span class="str">	(( // &gt;&gt; {{ ` &gt; {** .? **} || }} count #lf &lt;&gt; ?LEAVE &lt;&lt;&quot;</span> <span class="str">&quot; ))</span>
<span class="str">    WHILE</span>
<span class="str">	    &gt;&gt;next</span>
<span class="str">    REPEAT</span>
<span class="str">    end$ s&quot;</span> <span class="str">&quot; &lt;&lt; &gt;&gt;string&#64; ;</span>
