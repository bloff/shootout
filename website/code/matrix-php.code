set_time_limit(0);

$SIZE = 30;

function mkMatrix ($rows, $cols) {
    $count = 1;
    <B><FONT COLOR="#A020F0">for</FONT></B> ($i=0; $i&lt;$rows; $i++) {
	<B><FONT COLOR="#A020F0">for</FONT></B> ($j=0; $j&lt;$cols; $j++) {
	    $m[$i][$j] = $count++;
	}
    }
    <B><FONT COLOR="#A020F0">return</FONT></B> $m;
}


function mkZeroMatrix ($rows, $cols) {
    <B><FONT COLOR="#A020F0">for</FONT></B> ($i=0; $i&lt;$rows; $i++) {
	<B><FONT COLOR="#A020F0">for</FONT></B> ($j=0; $j&lt;$cols; $j++) {
	    $m[$i][$j] = 0;
	}
    }
    <B><FONT COLOR="#A020F0">return</FONT></B> $m;
}


function mmult ($rows, $cols, &amp;$m1, &amp;$m2, &amp;$mm) {
    <B><FONT COLOR="#A020F0">for</FONT></B> ($i=0; $i&lt;$rows; $i++) {
	<B><FONT COLOR="#A020F0">for</FONT></B> ($j=0; $j&lt;$cols; $j++) {
	    $x = 0;
	    <B><FONT COLOR="#A020F0">for</FONT></B> ($k=0; $k&lt;$cols; $k++) {
		$x += $m1[$i][$k] * $m2[$k][$j];
	    }
	    $mm[$i][$j] = $x;
	}
    }
    <B><FONT COLOR="#A020F0">return</FONT></B> $mm;
}


$n = ($argc == 2) ? $argv[1] : 1;

$m1 = mkMatrix($SIZE, $SIZE);
$m2 = mkMatrix($SIZE, $SIZE);
$mm = mkZeroMatrix($SIZE, $SIZE);

<B><FONT COLOR="#A020F0">while</FONT></B> ($n--) 
    mmult($SIZE, $SIZE, $m1, $m2, $mm);


<B><FONT COLOR="#A020F0">print</FONT></B> <B><FONT COLOR="#BC8F8F">&quot;{$mm[0][0]} {$mm[2][3]} {$mm[3][2]} {$mm[4][4]}\n&quot;</FONT></B>; 

?&gt;</pre></td></tr></table>
