$n = ($argc == 2) ? $argv[1] : 1;

<B><FONT COLOR="#A020F0">for</FONT></B> ($i = 0; $i &lt; 10000; $i++) 
   $hash1[<B><FONT COLOR="#BC8F8F">'foo_'</FONT></B>.$i] = $i;

<B><FONT COLOR="#A020F0">for</FONT></B> ($i = $n; $i &gt; 0; $i--) 
   <B><FONT COLOR="#A020F0">foreach</FONT></B>($hash1 as $key =&gt; $value) 
      <B><FONT COLOR="#A020F0">if</FONT></B> (isset($hash2[$key])){ 
         $hash2[$key] += $value; 
      } <B><FONT COLOR="#A020F0">else</FONT></B> { 
         $hash2[$key] = $value; 
      }
         
<B><FONT COLOR="#A020F0">print</FONT></B> <B><FONT COLOR="#BC8F8F">&quot;$hash1[foo_1] $hash1[foo_9999] $hash2[foo_1] $hash2[foo_9999]\n&quot;</FONT></B>;

