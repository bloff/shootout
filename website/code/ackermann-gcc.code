#<B><FONT COLOR="#5F9EA0">include</FONT></B> <B><FONT COLOR="#BC8F8F">&lt;stdio.h&gt;</FONT></B>
#<B><FONT COLOR="#5F9EA0">include</FONT></B> <B><FONT COLOR="#BC8F8F">&lt;stdlib.h&gt;</FONT></B>
#<B><FONT COLOR="#5F9EA0">include</FONT></B> <B><FONT COLOR="#BC8F8F">&lt;unistd.h&gt;</FONT></B>

<B><FONT COLOR="#228B22">int</FONT></B> <B><FONT COLOR="#0000FF">Ack</FONT></B>(<B><FONT COLOR="#228B22">int</FONT></B> M, <B><FONT COLOR="#228B22">int</FONT></B> N) { <B><FONT COLOR="#A020F0">return</FONT></B>(M ? (Ack(M-1,N ? Ack(M,(N-1)) : 1)) : N+1); }

<B><FONT COLOR="#228B22">int</FONT></B> <B><FONT COLOR="#0000FF">main</FONT></B>(<B><FONT COLOR="#228B22">int</FONT></B> argc, <B><FONT COLOR="#228B22">char</FONT></B> *argv[]) {
    <B><FONT COLOR="#228B22">int</FONT></B> n = ((argc == 2) ? atoi(argv[1]) : 1);

    printf(<B><FONT COLOR="#BC8F8F">&quot;Ack(3,%d): %d\n&quot;</FONT></B>, n, Ack(3, n));
    <I><FONT COLOR="#B22222">/* sleep long enough so we can measure memory usage */</FONT></I>
    sleep(1);
    <B><FONT COLOR="#A020F0">return</FONT></B>(0);
}
