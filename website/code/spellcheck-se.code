</span><span class="line">    4 </span><span class="slc"></span><span class="slc">-- from Steve Thompson
</span><span class="line">    5 </span><span class="slc"></span><span class="slc">-- Updated for current SmartEiffel by Brent Fulgham (with a lot
</span><span class="line">    6 </span><span class="slc"></span><span class="slc">--   of help from Philippe Ribet!)
</span><span class="line">    7 </span><span class="slc"></span>
<span class="line">    8 </span><span class="slc">-- &lt;LOC-OFF&gt;
</span><span class="line">    9 </span><span class="slc"></span><span class="kwa">indexing
</span><span class="line">   10 </span><span class="kwa"></span>   description<span class="sym">:</span> <span class="str">&quot;This class performs the spell check test&quot;</span>
<span class="line">   11 </span>   author <span class="sym">:</span> Steve Thompson
<span class="line">   12 </span>   email  <span class="sym">:</span> <span class="str">&quot;Steve_Thompson&#64;prodigy.net&quot;</span>
<span class="line">   13 </span>   date   <span class="sym">:</span> February <span class="num">18</span><span class="sym">,</span> <span class="num">2001
</span><span class="line">   14 </span><span class="num"></span>   compile<span class="sym">:</span> <span class="str">&quot;compile -clean -boost -no_split -O3 spellcheck.e -o spellcheck&quot;</span>
<span class="line">   15 </span>   run    <span class="sym">:</span> <span class="str">&quot;spellcheck &lt; Input&quot;</span>
<span class="line">   16 </span><span class="slc">-- &lt;LOC-ON&gt;
</span><span class="line">   17 </span><span class="slc"></span>
<span class="line">   18 </span><span class="kwa">class</span> SPELLCHECK
<span class="line">   19 </span>
<span class="line">   20 </span>creation make
<span class="line">   21 </span>
<span class="line">   22 </span><span class="kwa">feature</span> <span class="slc">-- Creation
</span><span class="line">   23 </span><span class="slc"></span>
<span class="line">   24 </span>   make <span class="kwa">is
</span><span class="line">   25 </span><span class="kwa">      do
</span><span class="line">   26 </span><span class="kwa"></span>         read_dictionary
<span class="line">   27 </span>         <span class="kwa">from
</span><span class="line">   28 </span><span class="kwa"></span>            std_input.read_line
<span class="line">   29 </span>         until std_input.end_of_input <span class="kwa">loop
</span><span class="line">   30 </span><span class="kwa">            if</span> dictionary.has<span class="sym">(</span>std_input.last_string<span class="sym">) =</span> <span class="kwa">False then
</span><span class="line">   31 </span><span class="kwa"></span>               std_output.put_string<span class="sym">(</span>std_input.last_string<span class="sym">)
</span><span class="line">   32 </span><span class="sym"></span>               std_output.put_new_line
<span class="line">   33 </span>            <span class="kwa">end
</span><span class="line">   34 </span><span class="kwa"></span>            std_input.read_line
<span class="line">   35 </span>         <span class="kwa">end
</span><span class="line">   36 </span><span class="kwa">      end</span> <span class="slc">-- make
</span><span class="line">   37 </span><span class="slc"></span>
<span class="line">   38 </span><span class="kwa">feature</span> <span class="slc">-- Queries
</span><span class="line">   39 </span><span class="slc"></span>
<span class="line">   40 </span>   dictionary<span class="sym">:</span> DICTIONARY<span class="sym">[</span><span class="kwb">INTEGER</span><span class="sym">,</span> STRING<span class="sym">]
</span><span class="line">   41 </span><span class="sym">
</span><span class="line">   42 </span><span class="sym"></span><span class="kwa">feature</span> <span class="slc">-- Commands
</span><span class="line">   43 </span><span class="slc"></span>
<span class="line">   44 </span>   read_dictionary <span class="kwa">is
</span><span class="line">   45 </span><span class="kwa">      local
</span><span class="line">   46 </span><span class="kwa"></span>         file<span class="sym">:</span> TEXT_FILE_READ
<span class="line">   47 </span>         value<span class="sym">:</span> <span class="kwb">INTEGER
</span><span class="line">   48 </span><span class="kwb"></span>      <span class="kwa">do
</span><span class="line">   49 </span><span class="kwa"></span>         value <span class="sym">:=</span> <span class="num">1
</span><span class="line">   50 </span><span class="num"></span>         <span class="kwa">from
</span><span class="line">   51 </span><span class="kwa"></span>            <span class="sym">!!</span>dictionary.with_capacity<span class="sym">(</span><span class="num">60000</span><span class="sym">)
</span><span class="line">   52 </span><span class="sym">            !!</span>file.connect_to<span class="sym">(</span><span class="str">&quot;Usr.Dict.Words&quot;</span><span class="sym">)
</span><span class="line">   53 </span><span class="sym"></span>         until
<span class="line">   54 </span>            file.end_of_input
<span class="line">   55 </span>         <span class="kwa">loop
</span><span class="line">   56 </span><span class="kwa"></span>            file.read_line
<span class="line">   57 </span>            <span class="kwa">if not</span> file.last_string.is_empty <span class="kwa">then
</span><span class="line">   58 </span><span class="kwa">               if not</span> dictionary.has<span class="sym">(</span>file.last_string<span class="sym">)</span> <span class="kwa">then
</span><span class="line">   59 </span><span class="kwa"></span>                  dictionary.add<span class="sym">(</span>value<span class="sym">,</span> file.last_string.twin<span class="sym">)
</span><span class="line">   60 </span><span class="sym"></span>               <span class="kwa">end
</span><span class="line">   61 </span><span class="kwa">            end
</span><span class="line">   62 </span><span class="kwa">         end
</span><span class="line">   63 </span><span class="kwa"></span>         file.disconnect
<span class="line">   64 </span>      <span class="kwa">end</span> <span class="slc">-- read_dictionary
</span><span class="line">   65 </span><span class="slc"></span>
<span class="line">   66 </span><span class="kwa">end</span>
