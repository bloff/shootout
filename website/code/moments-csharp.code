</span>        Console<span class="sym">.</span>WriteLine<span class="sym">(</span><span class="str">&quot;standard_deviation: {0:f6}&quot;</span><span class="sym">,</span> standard_deviation<span class="sym">);
</span>        Console<span class="sym">.</span>WriteLine<span class="sym">(</span><span class="str">&quot;variance:           {0:f6}&quot;</span><span class="sym">,</span> variance<span class="sym">);
</span>        Console<span class="sym">.</span>WriteLine<span class="sym">(</span><span class="str">&quot;skew:               {0:f6}&quot;</span><span class="sym">,</span> skew<span class="sym">);
</span>        Console<span class="sym">.</span>WriteLine<span class="sym">(</span><span class="str">&quot;kurtosis:           {0:f6}&quot;</span><span class="sym">,</span> kurtosis<span class="sym">);
    }
}</span>
