            ((</span><span class="kwb">double</span><span class="sym">)</span>numbers<span class="sym">[</span>mid<span class="sym">] + (</span><span class="kwb">double</span><span class="sym">)</span>numbers<span class="sym">[</span>mid<span class="num">-1</span><span class="sym">]) /</span> <span class="num">2</span><span class="sym">;

</span>        Console<span class="sym">.</span>WriteLine<span class="sym">(</span><span class="str">&quot;n:                  {0:d}&quot;</span><span class="sym">,</span> n<span class="sym">);
