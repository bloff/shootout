<B><FONT COLOR="#228B22">MODULE</FONT></B> Ackermann;

<B><FONT COLOR="#5F9EA0">IMPORT</FONT></B> LangShootout, Out;

<B><FONT COLOR="#A020F0">VAR</FONT></B> n: <B><FONT COLOR="#228B22">LONGINT</FONT></B>;

<B><FONT COLOR="#A020F0">PROCEDURE </FONT></B><B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">Ack</FONT></I></B></FONT></B> (m, n: <B><FONT COLOR="#228B22">LONGINT</FONT></B>) : <B><FONT COLOR="#228B22">LONGINT</FONT></B>;
<B><FONT COLOR="#A020F0">BEGIN</FONT></B>
  <B><FONT COLOR="#A020F0">IF</FONT></B> (m<B><FONT COLOR="#5F9EA0">=</FONT></B>0) <B><FONT COLOR="#A020F0">THEN</FONT></B> <B><FONT COLOR="#A020F0">RETURN</FONT></B> n<B><FONT COLOR="#5F9EA0">+</FONT></B>1; <B><FONT COLOR="#A020F0">END</FONT></B>;
  <B><FONT COLOR="#A020F0">IF</FONT></B> (n<B><FONT COLOR="#5F9EA0">=</FONT></B>0) <B><FONT COLOR="#A020F0">THEN</FONT></B> <B><FONT COLOR="#A020F0">RETURN</FONT></B> Ack(m<B><FONT COLOR="#5F9EA0">-</FONT></B>1, 1); <B><FONT COLOR="#A020F0">END</FONT></B>;
  <B><FONT COLOR="#A020F0">RETURN</FONT></B> Ack(m<B><FONT COLOR="#5F9EA0">-</FONT></B>1, Ack(m, n<B><FONT COLOR="#5F9EA0">-</FONT></B>1));
<B><FONT COLOR="#A020F0">END </FONT></B><B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">Ack</FONT></I></B></FONT></B>;

<B><FONT COLOR="#A020F0">BEGIN</FONT></B>
  n <B><FONT COLOR="#5F9EA0">:=</FONT></B> LangShootout.Argi();
  Out.String(<B><FONT COLOR="#BC8F8F">&quot;Ack(3,&quot;</FONT></B> ); Out.Int(n, 1); Out.String(<B><FONT COLOR="#BC8F8F">&quot;): &quot;</FONT></B>);
  Out.LongInt( Ack(3,n), 1 ); Out.Ln;
<B><FONT COLOR="#A020F0">END </FONT></B><B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">Ackermann</FONT></I></B></FONT></B>.</pre></td></tr></table>
