    done<span class="sym">;</span>
    <span class="sym">++</span>b<span class="sym">;</span>
  done<span class="sym">;</span>
  <span class="sym">++</span>a<span class="sym">;</span>
done<span class="sym">;</span>

<span class="kwa">print</span> x<span class="sym">;</span> <span class="kwa">endl</span><span class="sym">;</span>

