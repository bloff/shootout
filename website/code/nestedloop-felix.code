</FONT></I>include <B><FONT COLOR="#BC8F8F">&quot;std&quot;</FONT></B>;
n := atoi(System::argv 1);
var x = 0;

var a=0;
whilst a&lt;n <B><FONT COLOR="#A020F0">do</FONT></B>
  var b=0;
  whilst b&lt;n <B><FONT COLOR="#A020F0">do</FONT></B>
    var c=0;
    whilst c&lt;n <B><FONT COLOR="#A020F0">do</FONT></B>
      var d=0;
      whilst d&lt;n <B><FONT COLOR="#A020F0">do</FONT></B>
        var e=0;
        whilst e&lt;n <B><FONT COLOR="#A020F0">do</FONT></B>
          var f=0;
          whilst f&lt;n <B><FONT COLOR="#A020F0">do</FONT></B>
            x++;
            ++f;
          done;
          ++e;
        done;
        ++d;
      done;
      ++c;
    done;
    ++b;
  done;
  ++a;
done;

print x; endl;
</pre></td></tr></table>
