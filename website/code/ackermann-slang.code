define <B><FONT COLOR="#0000FF">ack</FONT></B> ();
define <B><FONT COLOR="#0000FF">ack</FONT></B> (M, N)
{
   !<B><FONT COLOR="#A020F0">if</FONT></B> (M)
     <B><FONT COLOR="#A020F0">return</FONT></B>( N + 1 );
   !<B><FONT COLOR="#A020F0">if</FONT></B> (N)
     <B><FONT COLOR="#A020F0">return</FONT></B> ( ack(M-1, 1) );
   <B><FONT COLOR="#A020F0">return</FONT></B>( ack(M-1, ack(M, N-1)) );
}


define <B><FONT COLOR="#0000FF">main</FONT></B>()
{
   variable NUM = 1;
   <B><FONT COLOR="#A020F0">if</FONT></B> (__argc &gt; 1) NUM = integer (__argv[1]);
   fprintf (stdout, <B><FONT COLOR="#BC8F8F">&quot;Ack(3,%d): %d\n&quot;</FONT></B>, (NUM, ack(3, NUM)));
}

<B><FONT COLOR="#0000FF">main</FONT></B>()</pre></td></tr></table>
