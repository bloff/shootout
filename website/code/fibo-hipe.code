<I><FONT COLOR="#B22222">%%% -*- mode: erlang -*-
</FONT></I><I><FONT COLOR="#B22222">%%% $Id: fibo-hipe.code,v 1.1 2004-11-02 08:52:30 bfulgham Exp $
</FONT></I><I><FONT COLOR="#B22222">%%% http://www.bagley.org/~doug/shootout/
</FONT></I>
<B><FONT COLOR="#5F9EA0">-module</FONT></B>(fibo).
<B><FONT COLOR="#5F9EA0">-export</FONT></B>([main/1]).

<B><FONT COLOR="#0000FF">main</FONT></B>() -&gt; main([<B><FONT COLOR="#BC8F8F">'1'</FONT></B>]).
<B><FONT COLOR="#0000FF">main</FONT></B>([<FONT COLOR="#B8860B">Arg</FONT>]) -&gt;
    <FONT COLOR="#B8860B">Num</FONT> = <B><FONT COLOR="#A020F0">list_to_integer</FONT></B>(<B><FONT COLOR="#A020F0">atom_to_list</FONT></B>(<FONT COLOR="#B8860B">Arg</FONT>)),
    io:fwrite(<B><FONT COLOR="#BC8F8F">&quot;~w\n&quot;</FONT></B>, [fib(<FONT COLOR="#B8860B">Num</FONT>)]),
    <B><FONT COLOR="#A020F0">halt</FONT></B>(0).

<B><FONT COLOR="#0000FF">fib</FONT></B>(0) -&gt; 1;
<B><FONT COLOR="#0000FF">fib</FONT></B>(1) -&gt; 1;
<B><FONT COLOR="#0000FF">fib</FONT></B>(<FONT COLOR="#B8860B">N</FONT>) -&gt; fib(<FONT COLOR="#B8860B">N</FONT>-2) + fib(<FONT COLOR="#B8860B">N</FONT>-1).</pre></td></tr></table>
