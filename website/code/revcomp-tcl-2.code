<span class="kwa">proc</span> main <span class="sym">{} {</span>
    <span class="kwa">set</span> acc <span class="str">&quot;&quot;</span>
    <span class="kwa">while</span> <span class="sym">{[</span><span class="kwa">gets</span> stdin line<span class="sym">] != -</span><span class="num">1</span><span class="sym">} {</span>
	<span class="kwa">if</span> <span class="sym">{[</span><span class="kwa">string</span> equal <span class="sym">[</span><span class="kwa">string</span> index <span class="kwb">$line</span> <span class="num">0</span><span class="sym">] &gt;]} {</span>
	    <span class="kwa">if</span> <span class="sym">{[</span><span class="kwa">string</span> length <span class="kwb">$acc</span><span class="sym">]} {</span>
		put_reverse_fasta <span class="kwb">$head $acc</span>
		<span class="kwa">set</span> acc <span class="str">&quot;&quot;</span>
	    <span class="sym">}</span>
	    <span class="kwa">set</span> head <span class="kwb">$line</span>
	<span class="sym">}</span> else <span class="sym">{</span>
	    <span class="kwa">append</span> acc <span class="kwb">$line</span>
	<span class="sym">}</span>
    <span class="sym">}</span>
    put_reverse_fasta <span class="kwb">$head $acc</span>
<span class="sym">}</span>
<span class="kwa">proc</span> put_reverse_fasta <span class="sym">{</span>head body<span class="sym">} {</span>
    <span class="kwa">set</span> body <span class="sym">[</span><span class="kwa">string</span> toupper <span class="kwb">$body</span><span class="sym">]</span>
    <span class="kwa">set</span> body <span class="sym">[</span><span class="kwa">string</span> map <span class="sym">{</span>A T C G G C T A U A M K R Y Y R K M V B H D D H B V<span class="sym">}</span> <span class="kwb">$body</span><span class="sym">]</span>
    <span class="kwa">set</span> l <span class="sym">[</span><span class="kwa">string</span> length <span class="kwb">$body</span><span class="sym">]</span>
    <span class="kwa">for</span> <span class="sym">{</span><span class="kwa">set</span> i <span class="num">0</span><span class="sym">} {</span><span class="kwb">$i</span><span class="sym">&lt;</span><span class="kwb">$l</span><span class="sym">} {</span><span class="kwa">incr</span> i<span class="sym">} {</span>
        <span class="kwa">if</span> <span class="sym">{!(</span><span class="kwb">$i</span> <span class="sym">%</span> <span class="num">60</span><span class="sym">)} {</span><span class="kwa">append</span> head \n<span class="sym">}</span>
        <span class="kwa">append</span> head <span class="sym">[</span><span class="kwa">string</span> index <span class="kwb">$body</span> end<span class="sym">-</span><span class="kwb">$i</span><span class="sym">]</span>
    <span class="sym">}</span>
    <span class="kwa">puts</span> <span class="kwb">$head</span>
<span class="sym">}</span>
main
