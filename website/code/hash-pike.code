<B><FONT COLOR="#228B22">void</FONT></B> <B><FONT COLOR="#0000FF">main</FONT></B>(<B><FONT COLOR="#228B22">int</FONT></B> argc, array(string) argv)
{
    <B><FONT COLOR="#228B22">int</FONT></B> i, c = 0;
    <B><FONT COLOR="#228B22">int</FONT></B> n = (<B><FONT COLOR="#228B22">int</FONT></B>)argv[-1];
    <B><FONT COLOR="#A020F0">if</FONT></B> (n &lt; 1) n = 1;
      
    mapping(string:<B><FONT COLOR="#228B22">int</FONT></B>) X = ([]);
    
    <B><FONT COLOR="#A020F0">for</FONT></B> (i=1; i&lt;=n; i++) {
	X[sprintf(<B><FONT COLOR="#BC8F8F">&quot;%x&quot;</FONT></B>, i)] = i;
    }
    <B><FONT COLOR="#A020F0">for</FONT></B> (i=n; i&gt;0; i--) {
	<B><FONT COLOR="#A020F0">if</FONT></B> (X[(string)i]) c++;
    }
    write(<B><FONT COLOR="#BC8F8F">&quot;%d\n&quot;</FONT></B>, c);
}</pre></td></tr></table>
