#!/usr/bin/newlisp
<I><FONT COLOR="#B22222">;;
</FONT></I><I><FONT COLOR="#B22222">;; fibonacci series
</FONT></I><I><FONT COLOR="#B22222">;; 
</FONT></I><I><FONT COLOR="#B22222">;; By Lutz Mueller
</FONT></I>
(<B><FONT COLOR="#A020F0">define</FONT></B> (<B><FONT COLOR="#0000FF">fibo</FONT></B> n)
	(<B><FONT COLOR="#A020F0">if</FONT></B>(&lt; n 2) 1
	(+  (fibo (- n 1))
	    (fibo (- n 2)))))

(println (fibo (integer (nth 2 (main-args)))))

(exit)

