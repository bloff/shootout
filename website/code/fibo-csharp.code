using System;

class fibo 
{
    public <B><FONT COLOR="#228B22">static</FONT></B> <B><FONT COLOR="#228B22">int</FONT></B> fib(<B><FONT COLOR="#228B22">int</FONT></B> n) {
        <B><FONT COLOR="#A020F0">return</FONT></B> (n &lt; 2) ? 1 : fib(n-2) + fib(n-1);
    }

    public <B><FONT COLOR="#228B22">static</FONT></B> <B><FONT COLOR="#228B22">void</FONT></B> Main(String[] args)
    {
        <B><FONT COLOR="#228B22">int</FONT></B> N = System.Convert.ToInt32(args[0]);
        <B><FONT COLOR="#A020F0">if</FONT></B>(N &lt; 1)
	{
	    N = 1;
	}

        Console.WriteLine( fib(N) );
    }
}</pre></td></tr></table>
