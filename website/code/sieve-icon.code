<table width="100%" border="0" cellpadding="5" cellspacing="0"><tr><td bgcolor="#FFFFFF"><pre>
<I><FONT COLOR="#B22222"># -*- mode: icon -*-
</FONT></I><I><FONT COLOR="#B22222"># $Id: sieve-icon.code,v 1.1 2004-11-02 08:53:25 bfulgham Exp $
</FONT></I><I><FONT COLOR="#B22222"># http://www.bagley.org/~doug/shootout/
</FONT></I>
<B><FONT COLOR="#A020F0">procedure </FONT></B><B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">main</FONT></I></B></FONT></B>(argv)
    n <B><FONT COLOR="#5F9EA0">:=</FONT></B> argv[1] <B><FONT COLOR="#5F9EA0">|</FONT></B> 1
    <B><FONT COLOR="#A020F0">every</FONT></B> i <B><FONT COLOR="#5F9EA0">:=</FONT></B> 1 <B><FONT COLOR="#A020F0">to</FONT></B> n <B><FONT COLOR="#A020F0">do</FONT></B> count <B><FONT COLOR="#5F9EA0">:=</FONT></B> sieve()
    <B><FONT COLOR="#A020F0">write</FONT></B>(<B><FONT COLOR="#BC8F8F">&quot;Count: &quot;</FONT></B>, count)
<B><FONT COLOR="#A020F0">end</FONT></B>

<I><FONT COLOR="#B22222"># algorithm from a test program that is distributed with
</FONT></I><I><FONT COLOR="#B22222"># the icon source
</FONT></I>
<B><FONT COLOR="#A020F0">procedure </FONT></B><B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">sieve</FONT></I></B></FONT></B>()
   <B><FONT COLOR="#5F9EA0">local</FONT></B> limit, s, i
   limit <B><FONT COLOR="#5F9EA0">:=</FONT></B> 8192
   s <B><FONT COLOR="#5F9EA0">:=</FONT></B> <B><FONT COLOR="#A020F0">set</FONT></B>()
   <B><FONT COLOR="#A020F0">every</FONT></B> <B><FONT COLOR="#A020F0">insert</FONT></B>(s,1 <B><FONT COLOR="#A020F0">to</FONT></B> limit)
   <B><FONT COLOR="#A020F0">every</FONT></B> <B><FONT COLOR="#A020F0">member</FONT></B>(s,i <B><FONT COLOR="#5F9EA0">:=</FONT></B> 2 <B><FONT COLOR="#A020F0">to</FONT></B> limit) <B><FONT COLOR="#A020F0">do</FONT></B>
      <B><FONT COLOR="#A020F0">every</FONT></B> <B><FONT COLOR="#A020F0">delete</FONT></B>(s,i <B><FONT COLOR="#5F9EA0">+</FONT></B> i <B><FONT COLOR="#A020F0">to</FONT></B> limit <B><FONT COLOR="#A020F0">by</FONT></B> i)
   <B><FONT COLOR="#A020F0">delete</FONT></B>(s,1)
   <B><FONT COLOR="#A020F0">return</FONT></B>(<B><FONT COLOR="#5F9EA0">*</FONT></B>s);
<B><FONT COLOR="#A020F0">end</FONT></B></pre></td></tr></table>
