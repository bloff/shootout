<I><FONT COLOR="#B22222">#!/usr/bin/perl 
</FONT></I><I><FONT COLOR="#B22222"># $Id: lists2-perl.code,v 1.1 2004-11-02 08:52:36 bfulgham Exp $
</FONT></I>
<B><FONT COLOR="#A020F0">use</FONT></B> strict;

<B><FONT COLOR="#A020F0">sub</FONT></B> main {
    <B><FONT COLOR="#A020F0">my</FONT></B> $N = ($ARGV[0] &lt; 1) ? 1 : <B><FONT COLOR="#A020F0">int</FONT></B>($ARGV[0]);
    <B><FONT COLOR="#A020F0">my</FONT></B> @A = (1..$N);
    <B><FONT COLOR="#A020F0">my</FONT></B> @B = ();
    <B><FONT COLOR="#A020F0">while</FONT></B> (@A) {
	<B><FONT COLOR="#A020F0">push</FONT></B>(@B, <B><FONT COLOR="#A020F0">shift</FONT></B>(@A));
	<B><FONT COLOR="#A020F0">push</FONT></B>(@B, <B><FONT COLOR="#A020F0">pop</FONT></B>(@A));
    }
    <B><FONT COLOR="#A020F0">while</FONT></B> (@B) {
	<B><FONT COLOR="#A020F0">unshift</FONT></B>(@A, <B><FONT COLOR="#A020F0">pop</FONT></B>(@B));
	<B><FONT COLOR="#A020F0">unshift</FONT></B>(@A, <B><FONT COLOR="#A020F0">shift</FONT></B>(@B));
    }
    <B><FONT COLOR="#A020F0">print</FONT></B> <B><FONT COLOR="#A020F0">scalar</FONT></B>(@A), <B><FONT COLOR="#BC8F8F">&quot;\n&quot;</FONT></B>;
    <B><FONT COLOR="#A020F0">print</FONT></B> <B><FONT COLOR="#BC8F8F">&quot;@A[0..9]\n&quot;</FONT></B>;
}

main();</pre></td></tr></table>
