<B><FONT COLOR="#A020F0">public</FONT></B> <B><FONT COLOR="#A020F0">class</FONT></B> ackermann {
    <B><FONT COLOR="#A020F0">public</FONT></B> <B><FONT COLOR="#A020F0">static</FONT></B> <B><FONT COLOR="#A020F0">void</FONT></B> main(String[] args) {
	<B><FONT COLOR="#A020F0">int</FONT></B> num = Integer.parseInt(args[0]);
	System.out.println(<B><FONT COLOR="#BC8F8F">&quot;Ack(3,&quot;</FONT></B> + num + <B><FONT COLOR="#BC8F8F">&quot;): &quot;</FONT></B> + Ack(3, num));
    }
    <B><FONT COLOR="#A020F0">public</FONT></B> <B><FONT COLOR="#A020F0">static</FONT></B> <B><FONT COLOR="#A020F0">int</FONT></B> Ack(<B><FONT COLOR="#A020F0">int</FONT></B> m, <B><FONT COLOR="#A020F0">int</FONT></B> n) {
	<B><FONT COLOR="#A020F0">return</FONT></B> (m == 0) ? (n + 1) : ((n == 0) ? Ack(m-1, 1) :
				     Ack(m-1, Ack(m, n - 1)));
    }
