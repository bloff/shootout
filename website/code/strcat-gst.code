| n stream hello |
n := Smalltalk arguments first asInteger.

stream := WriteStream on: String <B><FONT COLOR="#A020F0">new</FONT></B>.
hello := <B><FONT COLOR="#BC8F8F">'hello'</FONT></B>, Character nl asString. 
n timesRepeat: [stream nextPutAll: hello].
