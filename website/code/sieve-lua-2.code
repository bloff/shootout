<table width="100%" border="0" cellpadding="5" cellspacing="0"><tr><td bgcolor="#FFFFFF"><pre>
<I><FONT COLOR="#B22222">-- $Id: sieve-lua-2.code,v 1.1 2004-11-08 14:59:29 bfulgham Exp $
</FONT></I><I><FONT COLOR="#B22222">-- http://www.bagley.org/~doug/shootout/
</FONT></I><I><FONT COLOR="#B22222">--
</FONT></I><I><FONT COLOR="#B22222">-- Roberto Ierusalimschy pointed out the for loop is much
</FONT></I><I><FONT COLOR="#B22222">-- faster for our purposes here than using a while loop.
</FONT></I>
<B><FONT COLOR="#A020F0">function </FONT></B><B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">main</FONT></I></B></FONT></B> <B><FONT COLOR="#A020F0">(num)</FONT></B>
    <B><FONT COLOR="#A020F0">for</FONT></B> num<B><FONT COLOR="#5F9EA0">=</FONT></B>num,1,<B><FONT COLOR="#5F9EA0">-</FONT></B>1 <B><FONT COLOR="#A020F0">do</FONT></B>
        <B><FONT COLOR="#A020F0">local</FONT></B> flags <B><FONT COLOR="#5F9EA0">=</FONT></B> {}
        count <B><FONT COLOR="#5F9EA0">=</FONT></B> 0
        <B><FONT COLOR="#A020F0">for</FONT></B> i<B><FONT COLOR="#5F9EA0">=</FONT></B>2,8192 <B><FONT COLOR="#A020F0">do</FONT></B>
            <B><FONT COLOR="#A020F0">if</FONT></B> <B><FONT COLOR="#A020F0">not</FONT></B> flags[i] <B><FONT COLOR="#A020F0">then</FONT></B>
                <B><FONT COLOR="#A020F0">for</FONT></B> k<B><FONT COLOR="#5F9EA0">=</FONT></B>i<B><FONT COLOR="#5F9EA0">+</FONT></B>i, 8192, i <B><FONT COLOR="#A020F0">do</FONT></B>
                    flags[k] <B><FONT COLOR="#5F9EA0">=</FONT></B> 1
                <B><FONT COLOR="#A020F0">end</FONT></B>
                count <B><FONT COLOR="#5F9EA0">=</FONT></B> count <B><FONT COLOR="#5F9EA0">+</FONT></B> 1
            <B><FONT COLOR="#A020F0">end</FONT></B>
        <B><FONT COLOR="#A020F0">end</FONT></B>
    <B><FONT COLOR="#A020F0">end</FONT></B>
<B><FONT COLOR="#A020F0">end</FONT></B>

NUM <B><FONT COLOR="#5F9EA0">=</FONT></B> <B><FONT COLOR="#228B22">tonumber</FONT></B>((arg <B><FONT COLOR="#A020F0">and</FONT></B> arg[1])) <B><FONT COLOR="#A020F0">or</FONT></B> 1
count <B><FONT COLOR="#5F9EA0">=</FONT></B> 0
main(NUM)
io.write(<B><FONT COLOR="#BC8F8F">&quot;Count: &quot;</FONT></B>, count, <B><FONT COLOR="#BC8F8F">&quot;\n&quot;</FONT></B>)
</pre></td></tr></table>
