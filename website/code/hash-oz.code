</span><span class="line">   19 </span><span class="num"></span>        M <span class="sym">=</span> I mod <span class="num">16
</span><span class="line">   20 </span><span class="num"></span>        <span class="kwb">if</span> M <span class="sym">&lt;</span> <span class="num">10</span> <span class="kwb">then
</span><span class="line">   21 </span><span class="kwb"></span>           <span class="sym">{</span>IntToHexString D M<span class="sym">+&amp;</span><span class="num">0</span><span class="sym">|</span>Hex<span class="sym">}
</span><span class="line">   22 </span><span class="sym"></span>        <span class="kwb">else
</span><span class="line">   23 </span><span class="kwb"></span>           <span class="sym">{</span>IntToHexString D <span class="sym">(</span>M<span class="num">-10</span><span class="sym">)+&amp;</span>a<span class="sym">|</span>Hex<span class="sym">}
</span><span class="line">   24 </span><span class="sym"></span>        <span class="kwb">end
</span><span class="line">   25 </span><span class="kwb">     end
