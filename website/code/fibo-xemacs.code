<I><FONT COLOR="#B22222">;(setq max-lisp-eval-depth 8000)
</FONT></I><I><FONT COLOR="#B22222">;(setq max-specpdl-size 5000)
</FONT></I>
(<B><FONT COLOR="#A020F0">defun</FONT></B> <B><FONT COLOR="#0000FF">fib</FONT></B> (n)
  (<B><FONT COLOR="#A020F0">cond</FONT></B> ((&lt; n 2) 1)
	(t (+ (fib (- n 2)) (fib (- n 1))))))

(<B><FONT COLOR="#A020F0">defun</FONT></B> <B><FONT COLOR="#0000FF">main</FONT></B> ()
  (<B><FONT COLOR="#A020F0">let</FONT></B>* ((arg (car command-line-args-left))
	 (num (<B><FONT COLOR="#A020F0">if</FONT></B> arg (string-to-number arg) 1)))
    (princ (format <B><FONT COLOR="#BC8F8F">&quot;%d\n&quot;</FONT></B> (fib num)))))</pre></td></tr></table>
