using System;
using System.Collections;

class hash2 {
    public <B><FONT COLOR="#228B22">static</FONT></B> <B><FONT COLOR="#228B22">int</FONT></B> Main(String[] args) {        
        Hashtable hash1 = new Hashtable();
        Hashtable hash2 = new Hashtable();
        
        <B><FONT COLOR="#228B22">int</FONT></B> n = System.Convert.ToInt32(args[0]);
        <B><FONT COLOR="#A020F0">if</FONT></B>(n &lt; 1) n = 1;
                
        <B><FONT COLOR="#A020F0">for</FONT></B>(<B><FONT COLOR="#228B22">int</FONT></B> i=0; i&lt;=9999; i++) {
            hash1.Add( <B><FONT COLOR="#BC8F8F">&quot;foo_&quot;</FONT></B> + i.ToString(), i);
        }
        
        <B><FONT COLOR="#A020F0">for</FONT></B>(<B><FONT COLOR="#228B22">int</FONT></B> i = 0; i &lt; n; i++) {
            IDictionaryEnumerator it = hash1.GetEnumerator();
            <B><FONT COLOR="#A020F0">while</FONT></B>(it.MoveNext()) {
                <B><FONT COLOR="#A020F0">if</FONT></B>(hash2.ContainsKey(it.Key)) {
                    <B><FONT COLOR="#228B22">int</FONT></B> v1 = (<B><FONT COLOR="#228B22">int</FONT></B>) hash1[it.Key];
                    <B><FONT COLOR="#228B22">int</FONT></B> v2 = (<B><FONT COLOR="#228B22">int</FONT></B>) hash2[it.Key];                
                    hash2[it.Key] = v1 + v2;
                } <B><FONT COLOR="#A020F0">else</FONT></B> {
                    hash2.Add(it.Key, hash1[it.Key]);
                }
            }
        }
        Console.WriteLine(<B><FONT COLOR="#BC8F8F">&quot;{0} {1} {2} {3}&quot;</FONT></B>, hash1[<B><FONT COLOR="#BC8F8F">&quot;foo_1&quot;</FONT></B>], hash1[<B><FONT COLOR="#BC8F8F">&quot;foo_9999&quot;</FONT></B>],
		hash2[<B><FONT COLOR="#BC8F8F">&quot;foo_1&quot;</FONT></B>], hash2[<B><FONT COLOR="#BC8F8F">&quot;foo_9999&quot;</FONT></B>]);
        <B><FONT COLOR="#A020F0">return</FONT></B>(0);
    }
}</pre></td></tr></table>
