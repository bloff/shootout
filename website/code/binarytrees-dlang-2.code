    <span class="sym">}</span>

    <span class="kwd">writefln</span><span class="sym">(</span><span class="str">&quot;long lived tree of depth &quot;</span><span class="sym">,</span>maxDepth<span class="sym">,</span><span class="str">&quot;</span><span class="esc">\t</span> <span class="str">check: &quot;</span><span class="sym">,</span>longLivedTree<span class="sym">.</span><span class="kwb">I</span>tem<span class="kwb">C</span>heck<span class="sym">);</span>

