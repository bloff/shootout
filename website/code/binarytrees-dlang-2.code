            <span class="kwa">return</span> <span class="kwb">T</span>ree<span class="kwb">N</span>ode<span class="sym">(</span>item
                           <span class="sym">,</span><span class="kwb">B</span>ottom<span class="kwb">U</span>p<span class="kwb">T</span>ree<span class="sym">(</span><span class="num">2</span> <span class="sym">*</span> item <span class="sym">-</span> <span class="num">1</span><span class="sym">,</span> depth <span class="sym">-</span> <span class="num">1</span><span class="sym">)</span>
                           <span class="sym">,</span><span class="kwb">B</span>ottom<span class="kwb">U</span>p<span class="kwb">T</span>ree<span class="sym">(</span><span class="num">2</span> <span class="sym">*</span> item<span class="sym">,</span> depth <span class="sym">-</span> <span class="num">1</span><span class="sym">));</span>
        <span class="kwa">return</span> <span class="kwb">T</span>ree<span class="kwb">N</span>ode<span class="sym">(</span>item<span class="sym">);</span>
    <span class="sym">}</span>

    <span class="kwa">int</span> <span class="kwb">I</span>tem<span class="kwb">C</span>heck<span class="sym">()</span>
    <span class="sym">{</span>
        <span class="kwa">if</span><span class="sym">(</span>left<span class="sym">)</span>
            <span class="kwa">return</span> item <span class="sym">+</span> left<span class="sym">.</span><span class="kwb">I</span>tem<span class="kwb">C</span>heck<span class="sym">() -</span> right<span class="sym">.</span><span class="kwb">I</span>tem<span class="kwb">C</span>heck<span class="sym">();</span>
        <span class="kwa">return</span> item<span class="sym">;</span>
    <span class="sym">}</span>

<span class="kwa">private</span><span class="sym">:</span>
    <span class="kwb">T</span>ree<span class="kwb">N</span>ode<span class="sym">*</span>           left<span class="sym">,</span> right<span class="sym">;</span>
    <span class="kwa">int</span>                 item<span class="sym">;</span>

    <span class="kwa">static</span> <span class="kwb">T</span>ree<span class="kwb">N</span>ode<span class="sym">*</span> <span class="kwd">opCall</span><span class="sym">(</span><span class="kwa">int</span> item<span class="sym">,</span> <span class="kwb">T</span>ree<span class="kwb">N</span>ode<span class="sym">*</span> left <span class="sym">=</span> <span class="kwa">null</span><span class="sym">,</span> <span class="kwb">T</span>ree<span class="kwb">N</span>ode<span class="sym">*</span> right <span class="sym">=</span> <span class="kwa">null</span><span class="sym">)</span>
    <span class="sym">{</span>
        <span class="kwb">T</span>ree<span class="kwb">N</span>ode<span class="sym">*</span> t <span class="sym">=</span> <span class="kwa">new</span> <span class="kwb">T</span>ree<span class="kwb">N</span>ode<span class="sym">;</span>
        t<span class="sym">.</span>left <span class="sym">=</span> left<span class="sym">;</span>
        t<span class="sym">.</span>right <span class="sym">=</span> right<span class="sym">;</span>
        t<span class="sym">.</span>item <span class="sym">=</span> item<span class="sym">;</span>
        <span class="kwa">return</span> t<span class="sym">;</span>
    <span class="sym">}</span>
<span class="sym">}</span>
