#!/<B><FONT COLOR="#5F9EA0">usr/bin/pike</FONT></B>
<I><FONT COLOR="#B22222">// -*- mode: pike -*-
</FONT></I><I><FONT COLOR="#B22222">// $Id: sieve-pike.code,v 1.2 2004-11-08 08:15:52 bfulgham Exp $
</FONT></I><I><FONT COLOR="#B22222">// http://www.bagley.org/~doug/shootout/
</FONT></I><I><FONT COLOR="#B22222">// from: Per Hedbor
</FONT></I>
<B><FONT COLOR="#228B22">void</FONT></B> <B><FONT COLOR="#0000FF">main</FONT></B>(<B><FONT COLOR="#228B22">int</FONT></B> argc, array(string) argv)
{
    array(<B><FONT COLOR="#228B22">int</FONT></B>) flags;
    <B><FONT COLOR="#228B22">int</FONT></B> i, k, count, num;
  
    num = (<B><FONT COLOR="#228B22">int</FONT></B>)argv[-1];
    <B><FONT COLOR="#A020F0">if</FONT></B> (num &lt; 1)
	num = 1;
      
    <B><FONT COLOR="#A020F0">while</FONT></B> (num--) {
	count = 0;
	flags = ({ 1 })*8193;
	<B><FONT COLOR="#A020F0">for</FONT></B> (i=2; i &lt;= 8192; i++) {
	    <B><FONT COLOR="#A020F0">if</FONT></B> (flags[i]) {
		<B><FONT COLOR="#A020F0">for</FONT></B> (k=i+i; k &lt;= 8192; k+=i)
		    flags[k] = 0;
		count++;
	    }
	}
    }
    write(<B><FONT COLOR="#BC8F8F">&quot;Count: %d\n&quot;</FONT></B>, count);
}</pre></td></tr></table>
