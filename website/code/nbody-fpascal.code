    begin
</span>      dx <span class="sym">:=</span> b<span class="sym">[</span>i<span class="sym">]</span>.x <span class="sym">-</span> b<span class="sym">[</span>j<span class="sym">]</span>.x<span class="sym">;
</span>      dy <span class="sym">:=</span> b<span class="sym">[</span>i<span class="sym">]</span>.y <span class="sym">-</span> b<span class="sym">[</span>j<span class="sym">]</span>.y<span class="sym">;
</span>      dz <span class="sym">:=</span> b<span class="sym">[</span>i<span class="sym">]</span>.z <span class="sym">-</span> b<span class="sym">[</span>j<span class="sym">]</span>.z<span class="sym">;
</span>      mag <span class="sym">:=</span> dt <span class="sym">/</span> power<span class="sym">(</span>sqrt<span class="sym">(</span>sqr<span class="sym">(</span>dx<span class="sym">)+</span>sqr<span class="sym">(</span>dy<span class="sym">)+</span>sqr<span class="sym">(</span>dz<span class="sym">)),</span> <span class="num">3</span><span class="sym">);
</span>      <span class="kwa">with</span> b<span class="sym">[</span>i<span class="sym">]</span> <span class="kwa">do
      begin
</span>        vx <span class="sym">:=</span> vx <span class="sym">-</span> dx <span class="sym">*</span> b<span class="sym">[</span>j<span class="sym">]</span>.mass <span class="sym">*</span> mag<span class="sym">;
</span>        vy <span class="sym">:=</span> vy <span class="sym">-</span> dy <span class="sym">*</span> b<span class="sym">[</span>j<span class="sym">]</span>.mass <span class="sym">*</span> mag<span class="sym">;
</span>        vz <span class="sym">:=</span> vz <span class="sym">-</span> dz <span class="sym">*</span> b<span class="sym">[</span>j<span class="sym">]</span>.mass <span class="sym">*</span> mag<span class="sym">;
</span>      <span class="kwa">end</span><span class="sym">;
</span>      <span class="kwa">with</span> b<span class="sym">[</span>j<span class="sym">]</span> <span class="kwa">do
      begin
</span>        vx <span class="sym">:=</span> vx <span class="sym">+</span> dx <span class="sym">*</span> b<span class="sym">[</span>i<span class="sym">]</span>.mass <span class="sym">*</span> mag<span class="sym">;
</span>        vy <span class="sym">:=</span> vy <span class="sym">+</span> dy <span class="sym">*</span> b<span class="sym">[</span>i<span class="sym">]</span>.mass <span class="sym">*</span> mag<span class="sym">;
</span>        vz <span class="sym">:=</span> vz <span class="sym">+</span> dz <span class="sym">*</span> b<span class="sym">[</span>i<span class="sym">]</span>.mass <span class="sym">*</span> mag<span class="sym">;
</span>      <span class="kwa">end</span><span class="sym">;
</span>    <span class="kwa">end</span><span class="sym">;
</span>  <span class="kwa">for</span> i <span class="sym">:=</span> low<span class="sym">(</span>b<span class="sym">)</span> <span class="kwa">to</span> high<span class="sym">(</span>b<span class="sym">)</span> <span class="kwa">do
    with</span> b<span class="sym">[</span>i<span class="sym">]</span> <span class="kwa">do
