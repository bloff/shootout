</span>      px <span class="sym">:=</span> px <span class="sym">-</span> vx <span class="sym">*</span> mass<span class="sym">;
</span>      py <span class="sym">:=</span> py <span class="sym">-</span> vy <span class="sym">*</span> mass<span class="sym">;
</span>      pz <span class="sym">:=</span> pz <span class="sym">-</span> vz <span class="sym">*</span> mass<span class="sym">;
</span>    <span class="kwa">end</span><span class="sym">;
