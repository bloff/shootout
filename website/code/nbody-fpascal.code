</span>    vy<span class="sym">:</span>   <span class="num">4.99852801234917238e</span><span class="sym">-</span><span class="num">03</span> <span class="sym">*</span> daysPerYear<span class="sym">;
</span>    vz<span class="sym">:</span>   <span class="num">2.30417297573763929e</span><span class="sym">-</span><span class="num">05</span> <span class="sym">*</span> daysPerYear<span class="sym">;
</span>    mass<span class="sym">:</span> <span class="num">2.85885980666130812e</span><span class="sym">-</span><span class="num">04</span> <span class="sym">*</span> solarMass <span class="sym">),
</span>  <span class="com">{ Uranus }</span>
