</span>      FOR i <span class="sym">:=</span> <span class="num">1</span> <span class="kwa">TO</span> m <span class="kwa">DO</span> Out<span class="sym">.</span>Char<span class="sym">(</span> SelectRandom<span class="sym">(</span>a<span class="sym">) );</span> <span class="kwa">END</span><span class="sym">;</span> Out<span class="sym">.</span>Ln<span class="sym">;
