    b<span class="sym">.</span>buf <span class="sym">=</span> <span class="kwa">cast</span><span class="sym">(</span><span class="kwa">char</span><span class="sym">*)</span><span class="kwd">realloc</span><span class="sym">(</span>b<span class="sym">.</span>buf<span class="sym">,</span> b<span class="sym">.</span>size<span class="sym">);</span>
