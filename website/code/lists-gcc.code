</span>	l<span class="sym">[</span>j<span class="sym">].</span>prev <span class="sym">= &amp;</span>l<span class="sym">[</span>i<span class="sym">];
</span>	l<span class="sym">[</span>i<span class="sym">].</span>val <span class="sym">=</span> xp<span class="sym">-&gt;</span>val<span class="sym">;
