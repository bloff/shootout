</span>    printf<span class="sym">(</span><span class="str">&quot;[last entry points to list head]</span><span class="esc">\n</span><span class="str">&quot;</span><span class="sym">);
</span>    printf<span class="sym">(</span><span class="str">&quot;[val of next of tail is:  %d]</span><span class="esc">\n</span><span class="str">&quot;</span><span class="sym">,</span> xp<span class="sym">-&gt;</span>next<span class="sym">-&gt;</span>val<span class="sym">);
