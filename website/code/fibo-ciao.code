%%% http:<I><FONT COLOR="#B22222">//shootout.alioth.debian.org/
</FONT></I>:- module(fibo,[main/1]).

:- use_module(<B><FONT COLOR="#A020F0">library</FONT></B>(<B><FONT COLOR="#A020F0">write</FONT></B>)).

fibo(0,1).
fibo(1,1).
fibo(N,Val) :- N &gt; 0, N1 <B><FONT COLOR="#A020F0">is</FONT></B> N - 1, fibo(N1,Val1),
		N2 <B><FONT COLOR="#A020F0">is</FONT></B> N - 2, fibo(N2,Val2),
		Val <B><FONT COLOR="#A020F0">is</FONT></B> Val1 + Val2.

run_list([Arg|Args]) :-
	<B><FONT COLOR="#A020F0">name</FONT></B>(Arg,ArgStr), <B><FONT COLOR="#A020F0">name</FONT></B>(ArgN,ArgStr),
	fibo(ArgN,Val),
	<B><FONT COLOR="#A020F0">write</FONT></B>(Val), nl, halt.

main(Argv) :-
	run_list(Argv).
</pre></td></tr></table>
