</FONT></I>
(<B><FONT COLOR="#A020F0">define</FONT></B> (<B><FONT COLOR="#0000FF">main</FONT></B> args)
  (<B><FONT COLOR="#A020F0">let</FONT></B> ((lines '()))
    (<B><FONT COLOR="#A020F0">do</FONT></B> ((line (read-line) (read-line)))
	((eof-object? line) (map write-line lines))
      (<B><FONT COLOR="#A020F0">set!</FONT></B> lines (cons line lines)))))</pre></td></tr></table>
