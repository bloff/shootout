<table width="100%" border="0" cellpadding="5" cellspacing="0"><tr><td bgcolor="#FFFFFF"><pre>
<I><FONT COLOR="#B22222">%%% -*- mode: erlang -*-
</FONT></I><I><FONT COLOR="#B22222">%%% $Id: nestedloop-erlang.code,v 1.1 2004-11-02 08:52:37 bfulgham Exp $
</FONT></I><I><FONT COLOR="#B22222">%%% http://www.bagley.org/~doug/shootout/
</FONT></I>
<B><FONT COLOR="#5F9EA0">-module</FONT></B>(nestedloop).
<B><FONT COLOR="#5F9EA0">-export</FONT></B>([main/1]).

<B><FONT COLOR="#0000FF">main</FONT></B>([<FONT COLOR="#B8860B">Arg</FONT>]) -&gt;
    <FONT COLOR="#B8860B">Num</FONT> = <B><FONT COLOR="#A020F0">list_to_integer</FONT></B>(<B><FONT COLOR="#A020F0">atom_to_list</FONT></B>(<FONT COLOR="#B8860B">Arg</FONT>)),
    io:fwrite(<B><FONT COLOR="#BC8F8F">&quot;~w\n&quot;</FONT></B>, [loopA(<FONT COLOR="#B8860B">Num</FONT>, <FONT COLOR="#B8860B">Num</FONT>, 0)]),
    <B><FONT COLOR="#A020F0">halt</FONT></B>(0).


<B><FONT COLOR="#0000FF">loopA</FONT></B>(0, <FONT COLOR="#B8860B">M</FONT>, <FONT COLOR="#B8860B">N</FONT>) -&gt; <FONT COLOR="#B8860B">N</FONT>;
<B><FONT COLOR="#0000FF">loopA</FONT></B>(<FONT COLOR="#B8860B">I</FONT>, <FONT COLOR="#B8860B">M</FONT>, <FONT COLOR="#B8860B">N</FONT>) -&gt; loopA(<FONT COLOR="#B8860B">I</FONT> - 1, <FONT COLOR="#B8860B">M</FONT>, loopB(<FONT COLOR="#B8860B">M</FONT>, <FONT COLOR="#B8860B">M</FONT>, <FONT COLOR="#B8860B">N</FONT>)).

<B><FONT COLOR="#0000FF">loopB</FONT></B>(0, <FONT COLOR="#B8860B">M</FONT>, <FONT COLOR="#B8860B">N</FONT>) -&gt; <FONT COLOR="#B8860B">N</FONT>;
<B><FONT COLOR="#0000FF">loopB</FONT></B>(<FONT COLOR="#B8860B">I</FONT>, <FONT COLOR="#B8860B">M</FONT>, <FONT COLOR="#B8860B">N</FONT>) -&gt; loopB(<FONT COLOR="#B8860B">I</FONT> - 1, <FONT COLOR="#B8860B">M</FONT>, loopC(<FONT COLOR="#B8860B">M</FONT>, <FONT COLOR="#B8860B">M</FONT>, <FONT COLOR="#B8860B">N</FONT>)).

<B><FONT COLOR="#0000FF">loopC</FONT></B>(0, <FONT COLOR="#B8860B">M</FONT>, <FONT COLOR="#B8860B">N</FONT>) -&gt; <FONT COLOR="#B8860B">N</FONT>;
<B><FONT COLOR="#0000FF">loopC</FONT></B>(<FONT COLOR="#B8860B">I</FONT>, <FONT COLOR="#B8860B">M</FONT>, <FONT COLOR="#B8860B">N</FONT>) -&gt; loopC(<FONT COLOR="#B8860B">I</FONT> - 1, <FONT COLOR="#B8860B">M</FONT>, loopD(<FONT COLOR="#B8860B">M</FONT>, <FONT COLOR="#B8860B">M</FONT>, <FONT COLOR="#B8860B">N</FONT>)).

<B><FONT COLOR="#0000FF">loopD</FONT></B>(0, <FONT COLOR="#B8860B">M</FONT>, <FONT COLOR="#B8860B">N</FONT>) -&gt; <FONT COLOR="#B8860B">N</FONT>;
<B><FONT COLOR="#0000FF">loopD</FONT></B>(<FONT COLOR="#B8860B">I</FONT>, <FONT COLOR="#B8860B">M</FONT>, <FONT COLOR="#B8860B">N</FONT>) -&gt; loopD(<FONT COLOR="#B8860B">I</FONT> - 1, <FONT COLOR="#B8860B">M</FONT>, loopE(<FONT COLOR="#B8860B">M</FONT>, <FONT COLOR="#B8860B">M</FONT>, <FONT COLOR="#B8860B">N</FONT>)).

<B><FONT COLOR="#0000FF">loopE</FONT></B>(0, <FONT COLOR="#B8860B">M</FONT>, <FONT COLOR="#B8860B">N</FONT>) -&gt; <FONT COLOR="#B8860B">N</FONT>;
<B><FONT COLOR="#0000FF">loopE</FONT></B>(<FONT COLOR="#B8860B">I</FONT>, <FONT COLOR="#B8860B">M</FONT>, <FONT COLOR="#B8860B">N</FONT>) -&gt; loopE(<FONT COLOR="#B8860B">I</FONT> - 1, <FONT COLOR="#B8860B">M</FONT>, loopF(<FONT COLOR="#B8860B">M</FONT>, <FONT COLOR="#B8860B">N</FONT>)).

<B><FONT COLOR="#0000FF">loopF</FONT></B>(0, <FONT COLOR="#B8860B">N</FONT>) -&gt; <FONT COLOR="#B8860B">N</FONT>;
<B><FONT COLOR="#0000FF">loopF</FONT></B>(<FONT COLOR="#B8860B">I</FONT>, <FONT COLOR="#B8860B">N</FONT>) -&gt; loopF(<FONT COLOR="#B8860B">I</FONT> - 1, 1 + <FONT COLOR="#B8860B">N</FONT>).</pre></td></tr></table>
