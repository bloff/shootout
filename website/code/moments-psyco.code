<span class="line">    1 </span><span class="slc">#!/usr/bin/python
</span><span class="line">    2 </span><span class="slc"></span><span class="slc"># $Id: moments-psyco.code,v 1.3 2004-11-11 07:14:01 bfulgham Exp $
</span><span class="line">    3 </span><span class="slc"></span><span class="slc"># http://www.bagley.org/~doug/shootout/
</span><span class="line">    4 </span><span class="slc"></span>
<span class="line">    5 </span><span class="kwc">import</span> sys<span class="sym">,</span> string<span class="sym">,</span> math<span class="sym">,</span> operator<span class="sym">,</span> psyco
<span class="line">    6 </span>psyco<span class="sym">.</span>full<span class="sym">()
</span><span class="line">    7 </span><span class="sym">
</span><span class="line">    8 </span><span class="sym"></span><span class="kwb">def</span> main<span class="sym">():
</span><span class="line">    9 </span><span class="sym"></span>    sum <span class="sym">=</span> <span class="num">0
</span><span class="line">   10 </span><span class="num"></span>    nums <span class="sym">= []
</span><span class="line">   11 </span><span class="sym"></span>    atof <span class="sym">=</span> string<span class="sym">.</span>atof
<span class="line">   12 </span>
<span class="line">   13 </span>    nums <span class="sym">=</span> <span class="kwa">map</span><span class="sym">(</span>atof<span class="sym">,</span> sys<span class="sym">.</span>stdin<span class="sym">.</span>readlines<span class="sym">())
</span><span class="line">   14 </span><span class="sym"></span>    sum <span class="sym">=</span> <span class="kwa">reduce</span><span class="sym">(</span>operator<span class="sym">.</span>add<span class="sym">,</span> nums<span class="sym">)
</span><span class="line">   15 </span><span class="sym">
</span><span class="line">   16 </span><span class="sym"></span>    n <span class="sym">=</span> <span class="kwa">len</span><span class="sym">(</span>nums<span class="sym">)
</span><span class="line">   17 </span><span class="sym"></span>    mean <span class="sym">=</span> sum<span class="sym">/</span>n
<span class="line">   18 </span>    average_deviation <span class="sym">=</span> <span class="num">0
</span><span class="line">   19 </span><span class="num"></span>    standard_deviation <span class="sym">=</span> <span class="num">0
</span><span class="line">   20 </span><span class="num"></span>    variance <span class="sym">=</span> <span class="num">0
</span><span class="line">   21 </span><span class="num"></span>    skew <span class="sym">=</span> <span class="num">0
</span><span class="line">   22 </span><span class="num"></span>    kurtosis <span class="sym">=</span> <span class="num">0
</span><span class="line">   23 </span><span class="num">
</span><span class="line">   24 </span><span class="num"></span>    <span class="kwb">for</span> num <span class="kwb">in</span> nums<span class="sym">:
</span><span class="line">   25 </span><span class="sym"></span>        deviation <span class="sym">=</span> num <span class="sym">-</span> mean
<span class="line">   26 </span>        average_deviation <span class="sym">+=</span> <span class="kwc">abs</span><span class="sym">(</span>deviation<span class="sym">)
</span><span class="line">   27 </span><span class="sym"></span>        variance <span class="sym">+=</span> deviation<span class="sym">**</span><span class="num">2</span><span class="sym">;
</span><span class="line">   28 </span><span class="sym"></span>        skew <span class="sym">+=</span> deviation<span class="sym">**</span><span class="num">3</span><span class="sym">;
</span><span class="line">   29 </span><span class="sym"></span>        kurtosis <span class="sym">+=</span> deviation<span class="sym">**</span><span class="num">4
</span><span class="line">   30 </span><span class="num">
</span><span class="line">   31 </span><span class="num"></span>    average_deviation <span class="sym">/=</span> n
<span class="line">   32 </span>    variance <span class="sym">/= (</span>n <span class="num">- 1</span><span class="sym">)
</span><span class="line">   33 </span><span class="sym"></span>    standard_deviation <span class="sym">=</span> math<span class="sym">.</span>sqrt<span class="sym">(</span>variance<span class="sym">)
</span><span class="line">   34 </span><span class="sym">
</span><span class="line">   35 </span><span class="sym"></span>    <span class="kwb">if</span> variance <span class="sym">&gt;</span> <span class="num">0.0</span><span class="sym">:
</span><span class="line">   36 </span><span class="sym"></span>        skew <span class="sym">/= (</span>n <span class="sym">*</span> variance <span class="sym">*</span> standard_deviation<span class="sym">)
</span><span class="line">   37 </span><span class="sym"></span>        kurtosis <span class="sym">=</span> kurtosis<span class="sym">/(</span>n <span class="sym">*</span> variance <span class="sym">*</span> variance<span class="sym">) -</span> <span class="num">3.0
</span><span class="line">   38 </span><span class="num">
</span><span class="line">   39 </span><span class="num"></span>    nums<span class="sym">.</span>sort<span class="sym">()
</span><span class="line">   40 </span><span class="sym"></span>    mid <span class="sym">=</span> n <span class="sym">/</span> <span class="num">2
</span><span class="line">   41 </span><span class="num">
</span><span class="line">   42 </span><span class="num"></span>    <span class="kwb">if</span> <span class="sym">(</span>n <span class="sym">%</span> <span class="num">2</span><span class="sym">) ==</span> <span class="num">0</span><span class="sym">:
</span><span class="line">   43 </span><span class="sym"></span>        median <span class="sym">= (</span>nums<span class="sym">[</span>mid<span class="sym">] +</span> nums<span class="sym">[</span>mid<span class="num">-1</span><span class="sym">])/</span><span class="num">2
</span><span class="line">   44 </span><span class="num"></span>    <span class="kwb">else</span><span class="sym">:
</span><span class="line">   45 </span><span class="sym"></span>        median <span class="sym">=</span> nums<span class="sym">[</span>mid<span class="sym">]
</span><span class="line">   46 </span><span class="sym">
</span><span class="line">   47 </span><span class="sym"></span>    <span class="kwb">print</span> <span class="str">&quot;n:                  %d&quot;</span> <span class="sym">%</span> n
<span class="line">   48 </span>    <span class="kwb">print</span> <span class="str">&quot;median:             %f&quot;</span> <span class="sym">%</span> median
<span class="line">   49 </span>    <span class="kwb">print</span> <span class="str">&quot;mean:               %f&quot;</span> <span class="sym">%</span> mean
<span class="line">   50 </span>    <span class="kwb">print</span> <span class="str">&quot;average_deviation:  %f&quot;</span> <span class="sym">%</span> average_deviation
<span class="line">   51 </span>    <span class="kwb">print</span> <span class="str">&quot;standard_deviation: %f&quot;</span> <span class="sym">%</span> standard_deviation
<span class="line">   52 </span>    <span class="kwb">print</span> <span class="str">&quot;variance:           %f&quot;</span> <span class="sym">%</span> variance
<span class="line">   53 </span>    <span class="kwb">print</span> <span class="str">&quot;skew:               %f&quot;</span> <span class="sym">%</span> skew
<span class="line">   54 </span>    <span class="kwb">print</span> <span class="str">&quot;kurtosis:           %f&quot;</span> <span class="sym">%</span> kurtosis
<span class="line">   55 </span>
<span class="line">   56 </span>main<span class="sym">()
