<B><FONT COLOR="#A020F0">import</FONT></B> sys, psyco
psyco.full()

IM = 139968
IA = 3877
IC = 29573

LAST = 42
<B><FONT COLOR="#A020F0">def</FONT></B> <B><FONT COLOR="#0000FF">gen_random</FONT></B>(max):
    <B><FONT COLOR="#A020F0">global</FONT></B> LAST
    LAST = (LAST * IA + IC) % IM
    <B><FONT COLOR="#A020F0">return</FONT></B>( (max * LAST) / IM )

<B><FONT COLOR="#A020F0">def</FONT></B> <B><FONT COLOR="#0000FF">main</FONT></B>():
    N = int(sys.argv[1])
    <B><FONT COLOR="#A020F0">if</FONT></B> N &lt; 1:
        N = 1
    gr = gen_random
    <B><FONT COLOR="#A020F0">for</FONT></B> i <B><FONT COLOR="#A020F0">in</FONT></B> xrange(1,N):
        gr(100.0)
    <B><FONT COLOR="#A020F0">print</FONT></B> <B><FONT COLOR="#BC8F8F">&quot;%.9f&quot;</FONT></B> % gr(100.0)

main()</pre></td></tr></table>
