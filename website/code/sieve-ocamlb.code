<table width="100%" border="0" cellpadding="5" cellspacing="0"><tr><td bgcolor="#FFFFFF"><pre>
<I><FONT COLOR="#B22222">(*
 * $Id: sieve-ocamlb.code,v 1.2 2004-11-08 08:15:52 bfulgham Exp $
 * http://www.bagley.org/~doug/shootout/
 * based on code from Markus Mottl
 * slightly improved by Claude Marche and Jean-Christophe Filliatre
 *)</FONT></I>

<B><FONT COLOR="#A020F0">let</FONT></B> init_flags <B><FONT COLOR="#5F9EA0">=</FONT></B> String.make 8193 <B><FONT COLOR="#BC8F8F">'t'</FONT></B>
<B><FONT COLOR="#A020F0">let</FONT></B> flags <B><FONT COLOR="#5F9EA0">=</FONT></B> String.make 8193 <B><FONT COLOR="#BC8F8F">'t'</FONT></B>

<B><FONT COLOR="#A020F0">let</FONT></B> <B><FONT COLOR="#A020F0">rec</FONT></B> inner_loop f i k <B><FONT COLOR="#5F9EA0">=</FONT></B>
  <B><FONT COLOR="#A020F0">if</FONT></B> k &lt; 8193 <B><FONT COLOR="#A020F0">then</FONT></B> begin f.[k] &lt;<B><FONT COLOR="#5F9EA0">-</FONT></B> <B><FONT COLOR="#BC8F8F">'f'</FONT></B>; inner_loop f i (k <B><FONT COLOR="#5F9EA0">+</FONT></B> i) <B><FONT COLOR="#A020F0">end</FONT></B>

<B><FONT COLOR="#A020F0">let</FONT></B> <B><FONT COLOR="#A020F0">rec</FONT></B> middle_loop f i cnt <B><FONT COLOR="#5F9EA0">=</FONT></B>
  <B><FONT COLOR="#A020F0">if</FONT></B> i &lt; 8193 <B><FONT COLOR="#A020F0">then</FONT></B>
    <B><FONT COLOR="#A020F0">if</FONT></B> f.[i] <B><FONT COLOR="#5F9EA0">=</FONT></B> <B><FONT COLOR="#BC8F8F">'t'</FONT></B> <B><FONT COLOR="#A020F0">then</FONT></B> begin
      inner_loop f i (i <B><FONT COLOR="#5F9EA0">+</FONT></B> i);
      middle_loop f (i <B><FONT COLOR="#5F9EA0">+</FONT></B> 1) (cnt <B><FONT COLOR="#5F9EA0">+</FONT></B> 1) <B><FONT COLOR="#A020F0">end</FONT></B>
    <B><FONT COLOR="#A020F0">else</FONT></B> middle_loop f (i <B><FONT COLOR="#5F9EA0">+</FONT></B> 1) cnt
  <B><FONT COLOR="#A020F0">else</FONT></B> cnt

<B><FONT COLOR="#A020F0">let</FONT></B> n <B><FONT COLOR="#5F9EA0">=</FONT></B> <B><FONT COLOR="#A020F0">if</FONT></B> Array.length Sys.argv <B><FONT COLOR="#5F9EA0">&gt;</FONT></B> 1 <B><FONT COLOR="#A020F0">then</FONT></B> int_of_string Sys.argv.(1) <B><FONT COLOR="#A020F0">else</FONT></B> 1;;
for iter <B><FONT COLOR="#5F9EA0">=</FONT></B> 2 to n <B><FONT COLOR="#A020F0">do</FONT></B>
  String.blit init_flags 0 flags 0 8193;
  ignore (middle_loop flags 2 0);
done;
Printf.printf <B><FONT COLOR="#BC8F8F">&quot;Count: %d\n&quot;</FONT></B> (middle_loop flags 2 0)</pre></td></tr></table>
