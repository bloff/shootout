n = <span class="sym">(</span><span class="kwa">ARGV</span>.shift <span class="sym">||</span> <span class="num">1</span><span class="sym">)</span>.to_i

hash = <span class="sym">{}
</span><span class="kwa">for</span> i <span class="kwa">in</span> <span class="num">1</span>..n
    hash<span class="sym">[</span><span class="str">'%x'</span> % i<span class="sym">]</span> = <span class="num">1
</span><span class="kwa">end

</span>c = <span class="num">0
</span>n.downto <span class="num">1</span> <span class="kwa">do</span> <span class="sym">|</span>i<span class="sym">|
</span>    c <span class="sym">+</span>= <span class="num">1</span> <span class="kwa">if</span> hash.has_key? i.to_s
<span class="kwa">end

