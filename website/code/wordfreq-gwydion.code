             words[x] <B><FONT COLOR="#5F9EA0">:</FONT></B>= count + 1;
           <B><FONT COLOR="#A020F0">end</FONT></B> <B><FONT COLOR="#A020F0">unless</FONT></B>;
       <B><FONT COLOR="#A020F0">end</FONT></B> <B><FONT COLOR="#5F9EA0">method</FONT></B>, <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">split</FONT></I></B></FONT></B>(<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">read-line</FONT></I></B></FONT></B>(*standard-input*), <B><FONT COLOR="#BC8F8F">' '</FONT></B>));
