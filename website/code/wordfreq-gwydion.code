  <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">for</FONT></I></B></FONT></B>(w keyed-by k <B><FONT COLOR="#A020F0">in</FONT></B> words)
    <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">add!</FONT></I></B></FONT></B>(results, <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">pair</FONT></I></B></FONT></B>(k, w));
  <B><FONT COLOR="#A020F0">end</FONT></B> <B><FONT COLOR="#A020F0">for</FONT></B>;
