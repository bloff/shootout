using System;

class sieve {
    public <B><FONT COLOR="#228B22">static</FONT></B> <B><FONT COLOR="#228B22">void</FONT></B> Main(String[] args) {        
        <B><FONT COLOR="#228B22">int</FONT></B> NUM = System.Convert.ToInt32(args[0]);
        <B><FONT COLOR="#A020F0">if</FONT></B>(NUM &lt; 1) NUM = 1;
        
        <B><FONT COLOR="#228B22">long</FONT></B> i, k;
        bool[] flags = new bool[8193];
        <B><FONT COLOR="#228B22">int</FONT></B> count = 0;
        <B><FONT COLOR="#A020F0">while</FONT></B> (NUM-- &gt; 0) {
            count = 0;
            <B><FONT COLOR="#A020F0">for</FONT></B>(i=2; i &lt;= 8192; i++) {
                flags[i] = true;
            }
            <B><FONT COLOR="#A020F0">for</FONT></B>(i=2; i &lt;= 8192; i++) {
                <B><FONT COLOR="#A020F0">if</FONT></B>(flags[i]) {
                    <I><FONT COLOR="#B22222">// remove all multiples of prime: i
</FONT></I>                    <B><FONT COLOR="#A020F0">for</FONT></B>(k=i+i; k &lt;= 8192; k+=i) {
                        flags[k] = false;
                    }
                    count++;
                }
            }
        }
        
        Console.WriteLine(<B><FONT COLOR="#BC8F8F">&quot;Count: {0}&quot;</FONT></B>, count);
    }
}</pre></td></tr></table>
