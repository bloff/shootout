<B><FONT COLOR="#A020F0">BEGIN</FONT></B> {
    n = (<B><FONT COLOR="#A020F0">ARGV</FONT></B>[1] &lt; 1) ? 1 : <B><FONT COLOR="#A020F0">ARGV</FONT></B>[1];

    <B><FONT COLOR="#A020F0">for</FONT></B> (i = 0; i &lt; n; i++)
	x[i] = i + 1
    <B><FONT COLOR="#A020F0">for</FONT></B> (k = 0; k &lt; 1000; k++) {
	<B><FONT COLOR="#A020F0">for</FONT></B> (j = n-1; j &gt;= 0; j--)
	    y[j] += x[j]
    }

    <B><FONT COLOR="#A020F0">print</FONT></B> y[0], y[n-1]
}</pre></td></tr></table>
