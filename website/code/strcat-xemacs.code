(<B><FONT COLOR="#A020F0">defun</FONT></B> <B><FONT COLOR="#0000FF">main</FONT></B> ()
  (<B><FONT COLOR="#A020F0">let</FONT></B>* ((arg (car command-line-args-left))
	 (num (<B><FONT COLOR="#A020F0">if</FONT></B> arg (string-to-number arg) 1)))
    (<B><FONT COLOR="#A020F0">let</FONT></B> ((buf (get-buffer-create <B><FONT COLOR="#BC8F8F">&quot;mybuf&quot;</FONT></B>))
	  (str <B><FONT COLOR="#BC8F8F">&quot;&quot;</FONT></B>))
      (dotimes (iter num)
	(insert-string <B><FONT COLOR="#BC8F8F">&quot;hello\n&quot;</FONT></B> buf))
      (princ (format <B><FONT COLOR="#BC8F8F">&quot;%d\n&quot;</FONT></B> (length (buffer-substring nil nil buf)))))))</pre></td></tr></table>
