<B><FONT COLOR="#A020F0">let</FONT></B> <B><FONT COLOR="#A020F0">rec</FONT></B> fib n <B><FONT COLOR="#5F9EA0">=</FONT></B>
   <B><FONT COLOR="#A020F0">if</FONT></B> n &lt; 2 <B><FONT COLOR="#A020F0">then</FONT></B> 1 <B><FONT COLOR="#A020F0">else</FONT></B> fib (n <B><FONT COLOR="#5F9EA0">-</FONT></B> 2) <B><FONT COLOR="#5F9EA0">+</FONT></B> fib (n <B><FONT COLOR="#5F9EA0">-</FONT></B> 1);;

<B><FONT COLOR="#A020F0">let</FONT></B> n <B><FONT COLOR="#5F9EA0">=</FONT></B> <B><FONT COLOR="#A020F0">if</FONT></B> Array.length Sys.argv <B><FONT COLOR="#5F9EA0">&gt;</FONT></B> 1 <B><FONT COLOR="#A020F0">then</FONT></B> int_of_string Sys.argv.(1) <B><FONT COLOR="#A020F0">else</FONT></B> 1;;
Printf.printf <B><FONT COLOR="#BC8F8F">&quot;%d\n&quot;</FONT></B> (fib n)
</pre></td></tr></table>
