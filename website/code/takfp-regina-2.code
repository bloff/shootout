arg n <span class="sym">.</span>
<span class="kwa">if</span> n <span class="sym">=</span> <span class="str">''</span> then n <span class="sym">=</span> <span class="num">1</span>

say <span class="kwd">format</span><span class="sym">(</span><span class="kwd">tak</span><span class="sym">(</span>n<span class="sym">*</span><span class="num">3.0</span><span class="sym">,</span> n<span class="sym">*</span><span class="num">2.0</span><span class="sym">,</span> n<span class="sym">*</span><span class="num">1.0</span><span class="sym">),  ,</span> <span class="num">1</span><span class="sym">)</span>
