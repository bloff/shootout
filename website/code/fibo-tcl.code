<table width="100%" border="0" cellpadding="5" cellspacing="0"><tr><td bgcolor="#FFFFFF"><pre>
<I><FONT COLOR="#B22222">#!/usr/bin/tclsh
</FONT></I><I><FONT COLOR="#B22222"># $Id: fibo-tcl.code,v 1.2 2004-11-08 08:15:13 bfulgham Exp $
</FONT></I><I><FONT COLOR="#B22222"># http://www.bagley.org/~doug/shootout/
</FONT></I>
<I><FONT COLOR="#B22222"># with help from: Kristoffer Lawson
</FONT></I>
<B><FONT COLOR="#A020F0">proc</FONT></B> <B><FONT COLOR="#0000FF">fib</FONT></B> {<FONT COLOR="#B8860B">n</FONT>} {
    <B><FONT COLOR="#A020F0">if</FONT></B> {$<FONT COLOR="#B8860B">n</FONT> &lt; 2} {
	<B><FONT COLOR="#A020F0">return</FONT></B> 1
    } else {
	<B><FONT COLOR="#A020F0">return</FONT></B> [<B><FONT COLOR="#A020F0">expr</FONT></B> {[fib [<B><FONT COLOR="#A020F0">expr</FONT></B> {$<FONT COLOR="#B8860B">n</FONT>-2}]] + [fib [<B><FONT COLOR="#A020F0">expr</FONT></B> {$<FONT COLOR="#B8860B">n</FONT>-1}]]}]
    }
}

<B><FONT COLOR="#A020F0">set</FONT></B> N [<B><FONT COLOR="#A020F0">lindex</FONT></B> $<FONT COLOR="#B8860B">argv</FONT> 0]
<B><FONT COLOR="#A020F0">if</FONT></B> {$<FONT COLOR="#B8860B">N</FONT> &lt; 1} { <B><FONT COLOR="#A020F0">set</FONT></B> N 1 }
<B><FONT COLOR="#A020F0">puts</FONT></B> [fib $<FONT COLOR="#B8860B">N</FONT>]</pre></td></tr></table>
