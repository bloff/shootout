</span>
<span class="kwa">proc</span> fib <span class="sym">{</span>n<span class="sym">} {
</span>    <span class="kwa">if</span> <span class="sym">{</span><span class="kwb">$n</span> <span class="sym">&lt;</span> <span class="num">2</span><span class="sym">} {
</span>	<span class="kwa">return</span> <span class="kwb">$n
</span>    <span class="sym">}</span> else <span class="sym">{
</span>	<span class="kwa">return</span> <span class="sym">[</span><span class="kwa">expr</span> <span class="sym">{[</span>fib <span class="sym">[</span><span class="kwa">incr</span> n <span class="num">-2</span><span class="sym">]] + [</span>fib <span class="sym">[</span><span class="kwa">incr</span> n<span class="sym">]]}]
    }
}

</span><span class="kwa">interp</span> recursionlimit <span class="sym">{}</span> <span class="num">10000
</span><span class="kwa">puts</span> <span class="sym">[</span>fib <span class="kwb">$argv</span><span class="sym">]</span>
