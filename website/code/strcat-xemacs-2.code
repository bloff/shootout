(<B><FONT COLOR="#A020F0">defun</FONT></B> <B><FONT COLOR="#0000FF">main</FONT></B> ()
  (<B><FONT COLOR="#A020F0">let</FONT></B>* ((arg (car command-line-args-left))
	 (num (<B><FONT COLOR="#A020F0">if</FONT></B> arg (string-to-number arg) 1)))
    (<B><FONT COLOR="#A020F0">let</FONT></B> ((str <B><FONT COLOR="#BC8F8F">&quot;&quot;</FONT></B>))
      (dotimes (iter num)
	(setq str (concat str <B><FONT COLOR="#BC8F8F">&quot;hello\n&quot;</FONT></B>)))
      (princ (format <B><FONT COLOR="#BC8F8F">&quot;%d\n&quot;</FONT></B> (length str))))))</pre></td></tr></table>
