<B><FONT COLOR="#A020F0">public</FONT></B> <B><FONT COLOR="#A020F0">class</FONT></B> fibo {
    <B><FONT COLOR="#A020F0">public</FONT></B> <B><FONT COLOR="#A020F0">static</FONT></B> <B><FONT COLOR="#A020F0">void</FONT></B> main(String args[]) {
	<B><FONT COLOR="#A020F0">int</FONT></B> N = Integer.parseInt(args[0]);
	System.out.println(fib(N));
    }
    <B><FONT COLOR="#A020F0">public</FONT></B> <B><FONT COLOR="#A020F0">static</FONT></B> <B><FONT COLOR="#A020F0">int</FONT></B> fib(<B><FONT COLOR="#A020F0">int</FONT></B> n) {
	<B><FONT COLOR="#A020F0">if</FONT></B> (n &lt; 2) <B><FONT COLOR="#A020F0">return</FONT></B>(1);
	<B><FONT COLOR="#A020F0">return</FONT></B>( fib(n-2) + fib(n-1) );
    }
