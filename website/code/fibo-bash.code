<B><FONT COLOR="#A020F0">function</FONT></B> fib {
    <B><FONT COLOR="#A020F0">if</FONT></B> [ $1 -lt 2 ] ; <B><FONT COLOR="#A020F0">then</FONT></B>
	<B><FONT COLOR="#DA70D6">echo</FONT></B> 1
    <B><FONT COLOR="#A020F0">else</FONT></B>
	<B><FONT COLOR="#DA70D6">echo</FONT></B> $[ `fib $[ $1 - 2 ]` + `fib $[ $1 - 1 ]` ]
    <B><FONT COLOR="#A020F0">fi</FONT></B>
}

N=${1:-1}
fib $N</pre></td></tr></table>
