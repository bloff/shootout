<span class="line">    1 </span>#<span class="sym">!/</span>usr<span class="sym">/</span>bin<span class="sym">/</span>newlisp
<span class="line">    2 </span>
<span class="line">    3 </span>#
<span class="line">    4 </span># hash <span class="sym">-</span> benchmark
<span class="line">    5 </span># create i<span class="sym">=</span><span class="num">1</span><span class="sym">-&gt;</span>n symbols with value i
<span class="line">    6 </span># count all exsiting symbols in <span class="kwa">reverse
</span><span class="line">    7 </span><span class="kwa"></span>#
<span class="line">    8 </span># newLISP does <span class="kwa">not</span> have hashtables but can
<span class="line">    9 </span># simlate hashes with symbols
<span class="line">   10 </span>#
<span class="line">   11 </span># Contributed by Lutz Mueller
<span class="line">   12 </span>
<span class="line">   13 </span><span class="sym">(</span>context <span class="str">'HASH) ; create hash container
</span><span class="line">   14 </span><span class="str">(context '</span>MAIN<span class="sym">)</span> <span class="slc">; go back to MAIN context
</span><span class="line">   15 </span><span class="slc"></span>
<span class="line">   16 </span><span class="sym">(</span><span class="kwa">set</span> <span class="str">'n (integer (nth 2 (main-args))))
</span><span class="line">   17 </span><span class="str">
</span><span class="line">   18 </span><span class="str">(for (i 1 n)
</span><span class="line">   19 </span><span class="str">        (set (symbol (format &quot;_%x&quot; (integer i)) HASH) i) )
</span><span class="line">   20 </span><span class="str">
</span><span class="line">   21 </span><span class="str">(set '</span>cnt <span class="num">0</span><span class="sym">)
</span><span class="line">   22 </span><span class="sym">(</span>for <span class="sym">(</span>i n <span class="num">1</span><span class="sym">)
</span><span class="line">   23 </span><span class="sym">        (</span><span class="kwa">if</span> <span class="sym">(</span>symbol <span class="sym">(</span>format <span class="str">&quot;_%d&quot;</span> <span class="sym">(</span>integer i<span class="sym">))</span> HASH nil<span class="sym">) (</span>inc <span class="str">'cnt)))
</span><span class="line">   24 </span><span class="str">
</span><span class="line">   25 </span><span class="str">(println cnt)
</span><span class="line">   26 </span><span class="str">
</span><span class="line">   27 </span><span class="str">(exit)
</span><span class="line">   28 </span><span class="str"></span>
