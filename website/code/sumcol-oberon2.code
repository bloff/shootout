<B><FONT COLOR="#228B22">MODULE</FONT></B> Sumcol;
<B><FONT COLOR="#5F9EA0">IMPORT</FONT></B> In, IntConv, Out;

<B><FONT COLOR="#5F9EA0">CONST</FONT></B>
   maxLineSize <B><FONT COLOR="#5F9EA0">=</FONT></B> 120;
<B><FONT COLOR="#A020F0">VAR</FONT></B> 
   line: <B><FONT COLOR="#228B22">ARRAY</FONT></B> maxLineSize <B><FONT COLOR="#A020F0">OF</FONT></B> <B><FONT COLOR="#228B22">CHAR</FONT></B>;
   sum: <B><FONT COLOR="#228B22">LONGINT</FONT></B>;
<B><FONT COLOR="#A020F0">BEGIN</FONT></B>
   sum <B><FONT COLOR="#5F9EA0">:=</FONT></B> 0;
   <B><FONT COLOR="#A020F0">LOOP</FONT></B>
      In.Line(line);           
      <B><FONT COLOR="#A020F0">IF</FONT></B> line[0] <B><FONT COLOR="#5F9EA0">=</FONT></B> 0X <B><FONT COLOR="#A020F0">THEN</FONT></B> <B><FONT COLOR="#A020F0">EXIT</FONT></B>; <B><FONT COLOR="#A020F0">END</FONT></B>;    
      <B><FONT COLOR="#A020F0">INC</FONT></B>(sum, IntConv.ValueInt(line)); 
   <B><FONT COLOR="#A020F0">END</FONT></B>;
   Out.Int(sum, 0); Out.Ln;
<B><FONT COLOR="#A020F0">END </FONT></B><B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">Sumcol</FONT></I></B></FONT></B>.</pre></td></tr></table>
