<B><FONT COLOR="#A020F0">define</FONT></B> <B><FONT COLOR="#5F9EA0">sealed</FONT></B> <B><FONT COLOR="#5F9EA0">domain</FONT></B> <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">make </FONT></I></B></FONT></B>(<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">subclass</FONT></I></B></FONT></B>(<B><FONT COLOR="#228B22">&lt;toggle&gt;</FONT></B>));
<B><FONT COLOR="#A020F0">define</FONT></B> <B><FONT COLOR="#5F9EA0">sealed</FONT></B> <B><FONT COLOR="#5F9EA0">domain</FONT></B> <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">initialize </FONT></I></B></FONT></B>(<B><FONT COLOR="#228B22">&lt;toggle&gt;</FONT></B>);


<B><FONT COLOR="#A020F0">define</FONT></B> <B><FONT COLOR="#5F9EA0">class</FONT></B> <B><FONT COLOR="#228B22">&lt;toggle&gt;</FONT></B> (<B><FONT COLOR="#228B22">&lt;object&gt;</FONT></B>)
  <B><FONT COLOR="#5F9EA0">slot</FONT></B> value <B><FONT COLOR="#5F9EA0">::</FONT></B> <B><FONT COLOR="#228B22">&lt;boolean&gt;</FONT></B>, <B><FONT COLOR="#5F9EA0">required-init-keyword:</FONT></B> <B><FONT COLOR="#5F9EA0">start-state:</FONT></B>;
<B><FONT COLOR="#A020F0">end</FONT></B> <B><FONT COLOR="#5F9EA0">class</FONT></B>;


<B><FONT COLOR="#A020F0">define</FONT></B> <B><FONT COLOR="#5F9EA0">class</FONT></B> <B><FONT COLOR="#228B22">&lt;nth-toggle&gt;</FONT></B> (<B><FONT COLOR="#228B22">&lt;toggle&gt;</FONT></B>)
  <B><FONT COLOR="#5F9EA0">slot</FONT></B> counter <B><FONT COLOR="#5F9EA0">::</FONT></B> <B><FONT COLOR="#228B22">&lt;integer&gt;</FONT></B> = 0;
  <B><FONT COLOR="#5F9EA0">slot</FONT></B> counter-maxiumum <B><FONT COLOR="#5F9EA0">::</FONT></B> <B><FONT COLOR="#228B22">&lt;integer&gt;</FONT></B>, <B><FONT COLOR="#5F9EA0">required-init-keyword:</FONT></B> <B><FONT COLOR="#5F9EA0">counter-maxiumum:</FONT></B>;
<B><FONT COLOR="#A020F0">end</FONT></B> <B><FONT COLOR="#5F9EA0">class</FONT></B>;


<B><FONT COLOR="#A020F0">define</FONT></B> <B><FONT COLOR="#5F9EA0">inline</FONT></B> <B><FONT COLOR="#5F9EA0">method</FONT></B> <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">activate </FONT></I></B></FONT></B>(t <B><FONT COLOR="#5F9EA0">::</FONT></B> <B><FONT COLOR="#228B22">&lt;toggle&gt;</FONT></B>) <B><FONT COLOR="#A020F0">=&gt;</FONT></B> t <B><FONT COLOR="#5F9EA0">::</FONT></B> <B><FONT COLOR="#228B22">&lt;toggle&gt;</FONT></B>;
  t.value <B><FONT COLOR="#5F9EA0">:</FONT></B>= ~t.value;
  t;
<B><FONT COLOR="#A020F0">end</FONT></B> <B><FONT COLOR="#5F9EA0">method</FONT></B>;


<B><FONT COLOR="#A020F0">define</FONT></B> <B><FONT COLOR="#5F9EA0">inline</FONT></B> <B><FONT COLOR="#5F9EA0">method</FONT></B> <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">activate </FONT></I></B></FONT></B>(t <B><FONT COLOR="#5F9EA0">::</FONT></B> <B><FONT COLOR="#228B22">&lt;nth-toggle&gt;</FONT></B>) <B><FONT COLOR="#A020F0">=&gt;</FONT></B> t <B><FONT COLOR="#5F9EA0">::</FONT></B> <B><FONT COLOR="#228B22">&lt;nth-toggle&gt;</FONT></B>;
  t.counter <B><FONT COLOR="#5F9EA0">:</FONT></B>= t.counter + 1;
  <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">if </FONT></I></B></FONT></B>(t.counter &gt;= t.counter-maxiumum)
    t.value <B><FONT COLOR="#5F9EA0">:</FONT></B>= ~t.value;
    t.counter <B><FONT COLOR="#5F9EA0">:</FONT></B>= 0;
  <B><FONT COLOR="#A020F0">end</FONT></B>;
  t;
<B><FONT COLOR="#A020F0">end</FONT></B> <B><FONT COLOR="#5F9EA0">method</FONT></B>;


<B><FONT COLOR="#A020F0">begin</FONT></B>
  <B><FONT COLOR="#A020F0">let</FONT></B> arg = <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">string-to-integer</FONT></I></B></FONT></B>(<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">element</FONT></I></B></FONT></B>(<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">application-arguments</FONT></I></B></FONT></B>(), 0, <B><FONT COLOR="#5F9EA0">default:</FONT></B> <B><FONT COLOR="#BC8F8F">&quot;1&quot;</FONT></B>));

  <B><FONT COLOR="#A020F0">let</FONT></B> val = #t;
