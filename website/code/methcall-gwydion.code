  <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">for </FONT></I></B></FONT></B>(i <B><FONT COLOR="#A020F0">from</FONT></B> 1 <B><FONT COLOR="#A020F0">to</FONT></B> arg)
    val <B><FONT COLOR="#5F9EA0">:</FONT></B>= toggle.activate.value;
  <B><FONT COLOR="#A020F0">end</FONT></B>;

  <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">format-out</FONT></I></B></FONT></B>(<B><FONT COLOR="#BC8F8F">&quot;%s\n&quot;</FONT></B>, <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">if </FONT></I></B></FONT></B>(val) <B><FONT COLOR="#BC8F8F">&quot;true&quot;</FONT></B> <B><FONT COLOR="#A020F0">else</FONT></B> <B><FONT COLOR="#BC8F8F">&quot;false&quot;</FONT></B> <B><FONT COLOR="#A020F0">end</FONT></B>);
     
  val <B><FONT COLOR="#5F9EA0">:</FONT></B>= #t;
  <B><FONT COLOR="#A020F0">let</FONT></B> nth-toggle = <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">make</FONT></I></B></FONT></B>(<B><FONT COLOR="#228B22">&lt;nth-toggle&gt;</FONT></B>, <B><FONT COLOR="#5F9EA0">start-state:</FONT></B> val, <B><FONT COLOR="#5F9EA0">counter-maxiumum:</FONT></B> 3);
  
  <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">for </FONT></I></B></FONT></B>(i <B><FONT COLOR="#A020F0">from</FONT></B> 1 <B><FONT COLOR="#A020F0">to</FONT></B> arg)
    val <B><FONT COLOR="#5F9EA0">:</FONT></B>= nth-toggle.activate.value;
  <B><FONT COLOR="#A020F0">end</FONT></B>;

  <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">format-out</FONT></I></B></FONT></B>(<B><FONT COLOR="#BC8F8F">&quot;%s\n&quot;</FONT></B>, <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">if </FONT></I></B></FONT></B>(val) <B><FONT COLOR="#BC8F8F">&quot;true&quot;</FONT></B> <B><FONT COLOR="#A020F0">else</FONT></B> <B><FONT COLOR="#BC8F8F">&quot;false&quot;</FONT></B> <B><FONT COLOR="#A020F0">end</FONT></B>);
<B><FONT COLOR="#A020F0">end</FONT></B></pre></td></tr></table>
