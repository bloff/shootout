dict = Hash.new
l = <B><FONT COLOR="#BC8F8F">&quot;&quot;</FONT></B>

IO.foreach(<B><FONT COLOR="#BC8F8F">&quot;Usr.Dict.Words&quot;</FONT></B>) <B><FONT COLOR="#A020F0">do</FONT></B> |l|
  dict[l.chomp!] = 1
<B><FONT COLOR="#A020F0">end</FONT></B> 

STDIN.each <B><FONT COLOR="#A020F0">do</FONT></B> |l|
  <B><FONT COLOR="#A020F0">unless</FONT></B> dict.has_key? l.chomp!
    puts l
  <B><FONT COLOR="#A020F0">end</FONT></B>
<B><FONT COLOR="#A020F0">end</FONT></B></pre></td></tr></table>
