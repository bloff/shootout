<table width="100%" border="0" cellpadding="5" cellspacing="0"><tr><td bgcolor="#FFFFFF"><pre>
<B><FONT COLOR="#A020F0">Program</FONT></B> reversefile;
<B><FONT COLOR="#A020F0">uses</FONT></B> SysUtils, Classes;

<B><FONT COLOR="#A020F0">var</FONT></B>
	i, N : longint;
	list : TList;
	line : <B><FONT COLOR="#A020F0">string</FONT></B>;
	pline : pointer;	
<B><FONT COLOR="#A020F0">begin</FONT></B>
	list := TList.Create;
	<B><FONT COLOR="#A020F0">While</FONT></B> <B><FONT COLOR="#A020F0">Not</FONT></B> Eof(input) <B><FONT COLOR="#A020F0">do</FONT></B>
	<B><FONT COLOR="#A020F0">begin</FONT></B>
		Readln(input, line);
		Getmem(pline, Length(line)+1);
		Move(line, pline^, Length(line)+1);
		list.Add( pline );
	<B><FONT COLOR="#A020F0">end</FONT></B>;
	N := list.Count;
	<B><FONT COLOR="#A020F0">For</FONT></B> i := N-1 <B><FONT COLOR="#A020F0">Downto</FONT></B> 0 <B><FONT COLOR="#A020F0">do</FONT></B> WriteLn( <B><FONT COLOR="#A020F0">string</FONT></B>(list.items[i]^) );
<B><FONT COLOR="#A020F0">end</FONT></B>.</pre></td></tr></table>
