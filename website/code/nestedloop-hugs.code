<span class="kwa">import</span> System<span class="sym">(</span>getArgs<span class="sym">)

</span>main <span class="sym">=</span> <span class="kwa">do</span> <span class="sym">[</span>number<span class="sym">] &lt;-</span> getArgs
	  <span class="kwa">let</span> n <span class="sym">=</span> read number
          print <span class="sym">(</span>loopA n n <span class="num">0</span><span class="sym">)
</span>    <span class="kwa">where</span> loopA <span class="sym">::</span> <span class="kwb">Int</span> <span class="sym">-&gt;</span> <span class="kwb">Int</span> <span class="sym">-&gt;</span> <span class="kwb">Int</span> <span class="sym">-&gt;</span> <span class="kwb">Int
</span>	  loopA <span class="num">0</span> m x <span class="sym">=</span> x
	  loopA n m x <span class="sym">=</span> loopA <span class="sym">(</span>n<span class="num">-1</span><span class="sym">)</span> m <span class="sym">(</span>loopB m m x<span class="sym">)

</span>	  loopB <span class="num">0</span> m x <span class="sym">=</span> x
	  loopB n m x <span class="sym">=</span> loopB <span class="sym">(</span>n<span class="num">-1</span><span class="sym">)</span> m <span class="sym">(</span>loopC m m x<span class="sym">)

</span>	  loopC <span class="num">0</span> m x <span class="sym">=</span> x
	  loopC n m x <span class="sym">=</span> loopC <span class="sym">(</span>n<span class="num">-1</span><span class="sym">)</span> m <span class="sym">(</span>loopD m m x<span class="sym">)

</span>	  loopD <span class="num">0</span> m x <span class="sym">=</span> x
	  loopD n m x <span class="sym">=</span> loopD <span class="sym">(</span>n<span class="num">-1</span><span class="sym">)</span> m <span class="sym">(</span>loopE m m x<span class="sym">)

</span>	  loopE <span class="num">0</span> m x <span class="sym">=</span> x
	  loopE n m x <span class="sym">=</span> loopE <span class="sym">(</span>n<span class="num">-1</span><span class="sym">)</span> m <span class="sym">(</span>loopF m x<span class="sym">)

</span>	  loopF <span class="num">0</span> x <span class="sym">=</span> x
	  loopF n x <span class="sym">=</span> loopF <span class="sym">(</span>n<span class="num">-1</span><span class="sym">) (</span>x<span class="sym">+</span><span class="num">1</span><span class="sym">)
</span>
