<B><FONT COLOR="#A020F0">import</FONT></B> java.io.*;
<B><FONT COLOR="#A020F0">import</FONT></B> java.util.*;

<B><FONT COLOR="#A020F0">public</FONT></B> <B><FONT COLOR="#A020F0">class</FONT></B> ary {
    <B><FONT COLOR="#A020F0">public</FONT></B> <B><FONT COLOR="#A020F0">static</FONT></B> <B><FONT COLOR="#A020F0">void</FONT></B> main(String args[]) {
	<B><FONT COLOR="#A020F0">int</FONT></B> i, j, k, n = Integer.parseInt(args[0]);
	<B><FONT COLOR="#A020F0">int</FONT></B> x[] = <B><FONT COLOR="#A020F0">new</FONT></B> <B><FONT COLOR="#A020F0">int</FONT></B>[n];
	<B><FONT COLOR="#A020F0">int</FONT></B> y[] = <B><FONT COLOR="#A020F0">new</FONT></B> <B><FONT COLOR="#A020F0">int</FONT></B>[n];

	<B><FONT COLOR="#A020F0">for</FONT></B> (i = 0; i &lt; n; i++)
	    x[i] = i + 1;
	<B><FONT COLOR="#A020F0">for</FONT></B> (k = 0; k &lt; 1000; k++ )
	    <B><FONT COLOR="#A020F0">for</FONT></B> (j = n-1; j &gt;= 0; j--)
		y[j] += x[j];

	System.out.println(y[0] + <B><FONT COLOR="#BC8F8F">&quot; &quot;</FONT></B> + y[n-1]);
    }
}</pre></td></tr></table>
