</span><span class="line">    3 </span><span class="slc"></span>
<span class="line">    4 </span><span class="slc">%%%
</span><span class="line">    5 </span><span class="slc"></span><span class="slc">%%% contributed by Isaac Gouy
</span><span class="line">    6 </span><span class="slc"></span>
<span class="line">    7 </span><span class="slc">%%  Usage: start from command line with
</span><span class="line">    8 </span><span class="slc"></span><span class="slc">%%     ozc -x sieve.oz -o sieve.oz.exe
</span><span class="line">    9 </span><span class="slc"></span><span class="slc">%%     sieve.oz.exe 900
</span><span class="line">   10 </span><span class="slc"></span>
<span class="line">   11 </span><span class="kwa">functor
</span><span class="line">   12 </span><span class="kwa">import</span> System Application
<span class="line">   13 </span>
<span class="line">   14 </span><span class="kwb">define</span> Args N Flags Start Stop <span class="kwb">in
</span><span class="line">   15 </span><span class="kwb">
</span><span class="line">   16 </span><span class="kwb"></span>    <span class="sym">[</span>Args<span class="sym">] = {</span>Application.getArgs plain<span class="sym">}
</span><span class="line">   17 </span><span class="sym"></span>    N <span class="sym">= {</span>String.toInt Args<span class="sym">}
</span><span class="line">   18 </span><span class="sym">
</span><span class="line">   19 </span><span class="sym"></span>    Start <span class="sym">=</span> <span class="num">2
</span><span class="line">   20 </span><span class="num"></span>    Stop <span class="sym">=</span> <span class="num">8192
</span><span class="line">   21 </span><span class="num">
</span><span class="line">   22 </span><span class="num"></span>    Flags <span class="sym">= {</span>BitArray.new Start Stop<span class="sym">}
</span><span class="line">   23 </span><span class="sym"></span>    <span class="kwb">for</span> I <span class="kwb">in</span> Start..Stop <span class="kwb">do</span> <span class="sym">{</span>BitArray.set Flags I<span class="sym">}</span> <span class="kwb">end
</span><span class="line">   24 </span><span class="kwb">
</span><span class="line">   25 </span><span class="kwb">    for</span> I <span class="kwb">in</span> <span class="num">1</span>..N <span class="kwb">do
</span><span class="line">   26 </span><span class="kwb">           for</span> J <span class="kwb">in</span> Start..Stop <span class="kwb">do
</span><span class="line">   27 </span><span class="kwb">            if</span> <span class="sym">{</span>BitArray.test Flags J<span class="sym">}</span> <span class="kwb">then
</span><span class="line">   28 </span><span class="kwb">                for</span> K <span class="kwb">in</span> J<span class="sym">+</span>J..Stop<span class="sym">;</span>J <span class="kwb">do</span> <span class="sym">{</span>BitArray.clear Flags K<span class="sym">}</span> <span class="kwb">end
</span><span class="line">   29 </span><span class="kwb">            end
</span><span class="line">   30 </span><span class="kwb">        end
</span><span class="line">   31 </span><span class="kwb">    end
</span><span class="line">   32 </span><span class="kwb">
</span><span class="line">   33 </span><span class="kwb"></span>   <span class="sym">{</span>System.showInfo <span class="str">&quot;Count: &quot;</span>#<span class="sym">{</span>BitArray.card Flags<span class="sym">}}
</span><span class="line">   34 </span><span class="sym">
</span><span class="line">   35 </span><span class="sym">   {</span>Application.exit <span class="num">0</span><span class="sym">}
</span><span class="line">   36 </span><span class="sym"></span><span class="kwb">end</span>
