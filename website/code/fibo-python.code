<span class="kwc">import</span> sys

<span class="kwb">def</span> <span class="kwd">fib</span><span class="sym">(</span>n<span class="sym">):</span>
    <span class="kwb">if</span> <span class="sym">(</span>n <span class="sym">&lt;</span> <span class="num">2</span><span class="sym">):</span>
        <span class="kwb">return</span><span class="sym">(</span><span class="num">1</span><span class="sym">)</span>
