</span><span class="line">    9 </span><span class="str">(defun matmul (a b c n m k)
</span><span class="line">   10 </span><span class="str">  (declare (optimize (speed 3) (safety 0) (debug 0))
</span><span class="line">   11 </span><span class="str">           (type (simple-array (unsigned-byte 32) (*)) a b c)
</span><span class="line">   12 </span><span class="str">           (fixnum n m k))
</span><span class="line">   13 </span><span class="str">  (let ((sum 0)
</span><span class="line">   14 </span><span class="str">        (i1 (- m))
</span><span class="line">   15 </span><span class="str">        (k2 0))
</span><span class="line">   16 </span><span class="str">    (declare (type (unsigned-byte 32) sum) (type fixnum i1 k2))
</span><span class="line">   17 </span><span class="str">    (dotimes (i n c)
</span><span class="line">   18 </span><span class="str">      (declare (fixnum i))
</span><span class="line">   19 </span><span class="str">      (setf i1 (+ i1 m)) ;; i1=i*m
</span><span class="line">   20 </span><span class="str">      (dotimes (j k)
</span><span class="line">   21 </span><span class="str">        (declare (fixnum j))
</span><span class="line">   22 </span><span class="str">        (setf sum 0)
</span><span class="line">   23 </span><span class="str">        (setf k2 (- k))
</span><span class="line">   24 </span><span class="str">        (dotimes (l m)
</span><span class="line">   25 </span><span class="str">          (declare (fixnum l))
</span><span class="line">   26 </span><span class="str">          (setf k2 (+ k2 k)) ;; k2= l*k
</span><span class="line">   27 </span><span class="str">          (setf sum (the (unsigned-byte 32) (+ (the (unsigned-byte 32) sum)
</span><span class="line">   28 </span><span class="str">                                               (the (unsigned-byte 32) (* (aref a (+ i1 l))
</span><span class="line">   29 </span><span class="str">                                                                          (aref b (+ k2 j))))))))
</span><span class="line">   30 </span><span class="str">        (setf (aref c (+ i1 j)) sum)))))
</span><span class="line">   31 </span><span class="str">
</span><span class="line">   32 </span><span class="str">(defun main ()
</span><span class="line">   33 </span><span class="str">  (let ((n (parse-integer (or (car (last #+sbcl sb-ext:*posix-argv*
</span><span class="line">   34 </span><span class="str">                                         #+cmu  extensions:*command-line-strings*)) &quot;1&quot;))))
</span><span class="line">   35 </span><span class="str">    (declare (fixnum n)
</span><span class="line">   36 </span><span class="str">             (optimize (speed 3) (debug 0) (safety 0)))
</span><span class="line">   37 </span><span class="str">    (let* ((m1 (make-matrix 30 30))
</span><span class="line">   38 </span><span class="str">           (m2 (make-matrix 30 30))
</span><span class="line">   39 </span><span class="str">           (m3 (make-matrix 30 30))
</span><span class="line">   40 </span><span class="str">           (mm (make-array '</span><span class="sym">(</span><span class="num">30 30</span><span class="sym">) :</span>element<span class="sym">-</span><span class="kwa">type</span> <span class="str">'(unsigned-byte 32) :displaced-to m3)))
</span><span class="line">   41 </span><span class="str">      (loop repeat n do (matmul m1 m2 m3 30 30 30))
</span><span class="line">   42 </span><span class="str">      (format t &quot;~A ~A ~A ~A~%&quot;
</span><span class="line">   43 </span><span class="str">              (aref mm 0 0) (aref mm 2 3) (aref mm 3 2) (aref mm 4 4)))))
</span><span class="line">   44 </span><span class="str">
</span><span class="line">   45 </span><span class="str">(defun make-matrix (rows cols)
</span><span class="line">   46 </span><span class="str">  (declare (type (unsigned-byte 32) rows cols)
</span><span class="line">   47 </span><span class="str">           (optimize (speed 3) (safety 0))); (hcl:fixnum-safety 0)))
</span><span class="line">   48 </span><span class="str">  (let* ((space (* rows cols))
</span><span class="line">   49 </span><span class="str">         (matrix (make-array space
</span><span class="line">   50 </span><span class="str">                             :element-type '</span><span class="sym">(</span>unsigned<span class="sym">-</span>byte <span class="num">32</span><span class="sym">))))
</span><span class="line">   51 </span><span class="sym">    (</span>declare <span class="sym">(</span><span class="kwa">type</span> <span class="sym">(</span>simple<span class="sym">-</span>array <span class="sym">(</span>unsigned<span class="sym">-</span>byte <span class="num">32</span><span class="sym">) (*))</span> matrix<span class="sym">)
</span><span class="line">   52 </span><span class="sym">             (</span>fixnum space<span class="sym">))
</span><span class="line">   53 </span><span class="sym">    (</span>loop <span class="sym">:</span>for i <span class="sym">:</span>of<span class="sym">-</span><span class="kwa">type</span> fixnum <span class="sym">:</span>from <span class="num">0</span> <span class="sym">:</span>below space
<span class="line">   54 </span>          <span class="sym">:</span>do <span class="sym">(</span>setf <span class="sym">(</span>aref matrix i<span class="sym">) (</span><span class="num">1</span><span class="sym">+</span> i<span class="sym">)))
</span><span class="line">   55 </span><span class="sym"></span>    matrix<span class="sym">))</span>
