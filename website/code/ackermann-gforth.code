<I><FONT COLOR="#B22222">\ if x = 0 		A(x,y) = y + 1
</FONT></I><I><FONT COLOR="#B22222">\ if y = 0		A(x,y) = A(x-1,1)
</FONT></I><I><FONT COLOR="#B22222">\ A(x,y) = A(x-1, A(x, y-1))
</FONT></I>
<I><FONT COLOR="#B22222">\ read NUM from last command line argument
</FONT></I>0. argc @ 1<B><FONT COLOR="#5F9EA0">-</FONT></B> arg <B><FONT COLOR="#5F9EA0">&gt;</FONT></B>number <B><FONT COLOR="#5F9EA0">2drop</FONT></B> <B><FONT COLOR="#5F9EA0">drop</FONT></B> <B><FONT COLOR="#A020F0">constant</FONT></B> NUM

<B><FONT COLOR="#A020F0">: </FONT></B><B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">-ack</FONT></I></B></FONT></B> ( y x <B><FONT COLOR="#5F9EA0">-</FONT></B><B><FONT COLOR="#5F9EA0">-</FONT></B> n )
   <B><FONT COLOR="#5F9EA0">dup</FONT></B> 0= <B><FONT COLOR="#A020F0">if</FONT></B>
     <B><FONT COLOR="#5F9EA0">drop</FONT></B> 1<B><FONT COLOR="#5F9EA0">+</FONT></B>
   <B><FONT COLOR="#A020F0">else</FONT></B>
     <B><FONT COLOR="#5F9EA0">over</FONT></B> <B><FONT COLOR="#A020F0">if</FONT></B>
	   <B><FONT COLOR="#5F9EA0">swap</FONT></B> 1<B><FONT COLOR="#5F9EA0">-</FONT></B> <B><FONT COLOR="#5F9EA0">over</FONT></B> recurse <B><FONT COLOR="#5F9EA0">swap</FONT></B> 1<B><FONT COLOR="#5F9EA0">-</FONT></B> recurse
	 <B><FONT COLOR="#A020F0">else</FONT></B>
	   1 <B><FONT COLOR="#5F9EA0">-</FONT></B>1 d<B><FONT COLOR="#5F9EA0">+</FONT></B> recurse
	 <B><FONT COLOR="#A020F0">then</FONT></B>
   <B><FONT COLOR="#A020F0">then</FONT></B> ;

<I><FONT COLOR="#B22222">\ run ack(3, NUM) and print result from stack
</FONT></I>.<B><FONT COLOR="#BC8F8F">&quot; Ack(3,&quot;</FONT></B> NUM 1 u.r .<B><FONT COLOR="#BC8F8F">&quot; ): &quot;</FONT></B> NUM 3 <B><FONT COLOR="#5F9EA0">-</FONT></B>ack 1 u.r cr

