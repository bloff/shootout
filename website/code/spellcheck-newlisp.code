#!/usr/bin/newlisp

<I><FONT COLOR="#B22222">;; spellcheck.newlisp - benchmark
</FONT></I>
(context 'Dictionary)
(context 'MAIN)

(<B><FONT COLOR="#A020F0">define</FONT></B> (<B><FONT COLOR="#0000FF">main</FONT></B>)
	(set 'infile (open <B><FONT COLOR="#BC8F8F">&quot;Usr.Dict.Words&quot;</FONT></B> <B><FONT COLOR="#BC8F8F">&quot;read&quot;</FONT></B>))
	(while (set 'word (read-line infile))
		(symbol (append <B><FONT COLOR="#BC8F8F">&quot;_&quot;</FONT></B> word) 'Dictionary))
	(close infile)

	(while (set 'word (read-line))
		(<B><FONT COLOR="#A020F0">if</FONT></B> (not (symbol (append <B><FONT COLOR="#BC8F8F">&quot;_&quot;</FONT></B> word) 'Dictionary nil))
			(println word))))

(main)

