using System;

class random {

    public <B><FONT COLOR="#228B22">static</FONT></B> <B><FONT COLOR="#228B22">int</FONT></B> IM = 139968;
    public <B><FONT COLOR="#228B22">static</FONT></B> <B><FONT COLOR="#228B22">int</FONT></B> IA = 3877;
    public <B><FONT COLOR="#228B22">static</FONT></B> <B><FONT COLOR="#228B22">int</FONT></B> IC = 29573;
    public <B><FONT COLOR="#228B22">static</FONT></B> <B><FONT COLOR="#228B22">long</FONT></B> last = 42;

    public <B><FONT COLOR="#228B22">static</FONT></B> <B><FONT COLOR="#228B22">double</FONT></B> gen_random(<B><FONT COLOR="#228B22">double</FONT></B> max)
    {
        last = (last * IA + IC) % IM;
        <B><FONT COLOR="#A020F0">return</FONT></B>( max * last / IM );
    }

    public <B><FONT COLOR="#228B22">static</FONT></B> <B><FONT COLOR="#228B22">void</FONT></B> Main(String[] args)
    {
        <B><FONT COLOR="#228B22">double</FONT></B> result = 0;

        <B><FONT COLOR="#228B22">int</FONT></B> n = System.Convert.ToInt32(args[0]);
        <B><FONT COLOR="#A020F0">if</FONT></B>(n &lt; 1) n = 1;

        <B><FONT COLOR="#A020F0">while</FONT></B> (n-- &gt; 0) {
            result = gen_random(100.0);
        }

        Console.WriteLine(<B><FONT COLOR="#BC8F8F">&quot;{0:f9}&quot;</FONT></B>, result);
    }
}</pre></td></tr></table>
