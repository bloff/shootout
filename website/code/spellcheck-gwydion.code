<table width="100%" border="0" cellpadding="5" cellspacing="0"><tr><td bgcolor="#FFFFFF"><pre>
module:         spellcheck
synopsis:       implementation of <B><FONT COLOR="#BC8F8F">&quot;Spell Checker&quot;</FONT></B> benchmark
author:         Peter Hinely
copyright:      public <B><FONT COLOR="#5F9EA0">domain</FONT></B>
use-libraries:  common-dylan, io, table-extensions, format-out
use-modules:    common-dylan, standard-io, streams, table-extensions, format-out


<B><FONT COLOR="#A020F0">define</FONT></B> <B><FONT COLOR="#5F9EA0">function</FONT></B> <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">spellcheck </FONT></I></B></FONT></B>() <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">=&gt;</FONT></I></B></FONT></B> ()
  <B><FONT COLOR="#A020F0">let</FONT></B> dictionary = <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">make</FONT></I></B></FONT></B>(<B><FONT COLOR="#228B22">&lt;case-insensitive-string-table&gt;</FONT></B>);
  
  <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">with-open-file </FONT></I></B></FONT></B>(file = <B><FONT COLOR="#BC8F8F">&quot;Usr.Dict.Words&quot;</FONT></B>)
    <B><FONT COLOR="#A020F0">let</FONT></B> line = #f;
    <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">while </FONT></I></B></FONT></B>(line <B><FONT COLOR="#5F9EA0">:</FONT></B>= <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">read-line</FONT></I></B></FONT></B>(file, <B><FONT COLOR="#5F9EA0">on-end-of-stream:</FONT></B> #f))
      dictionary[line] <B><FONT COLOR="#5F9EA0">:</FONT></B>= #t;
    <B><FONT COLOR="#A020F0">end</FONT></B> <B><FONT COLOR="#A020F0">while</FONT></B>;
  <B><FONT COLOR="#A020F0">end</FONT></B> with-open-file;
  
  <B><FONT COLOR="#A020F0">let</FONT></B> word-to-check = #f;

  <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">while </FONT></I></B></FONT></B>(word-to-check <B><FONT COLOR="#5F9EA0">:</FONT></B>= <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">read-line</FONT></I></B></FONT></B>(*standard-input*, <B><FONT COLOR="#5F9EA0">on-end-of-stream:</FONT></B> #f))
    <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">unless </FONT></I></B></FONT></B>(<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">element</FONT></I></B></FONT></B>(dictionary, word-to-check, <B><FONT COLOR="#5F9EA0">default:</FONT></B> #f))
      <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">format-out</FONT></I></B></FONT></B>(<B><FONT COLOR="#BC8F8F">&quot;%s\n&quot;</FONT></B>, word-to-check);
    <B><FONT COLOR="#A020F0">end</FONT></B>;
  <B><FONT COLOR="#A020F0">end</FONT></B> <B><FONT COLOR="#A020F0">while</FONT></B>;
<B><FONT COLOR="#A020F0">end</FONT></B> <B><FONT COLOR="#5F9EA0">function</FONT></B>;


<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">spellcheck</FONT></I></B></FONT></B>();</pre></td></tr></table>
