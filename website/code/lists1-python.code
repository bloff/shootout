<B><FONT COLOR="#A020F0">import</FONT></B> sys

<B><FONT COLOR="#A020F0">def</FONT></B> <B><FONT COLOR="#0000FF">main</FONT></B>():
    N = int(sys.argv[1])
    <B><FONT COLOR="#A020F0">if</FONT></B> N &lt; 1:
        N = 1

    <I><FONT COLOR="#B22222"># create list (A) of integers from 1 through N
</FONT></I>    A = range(1, N + 1)

    <I><FONT COLOR="#B22222"># create empty list B
</FONT></I>    B = []

    <I><FONT COLOR="#B22222"># move each individual item from head of A to head of B, in a loop
</FONT></I>    <I><FONT COLOR="#B22222"># the result should be in reverse order
</FONT></I>    <B><FONT COLOR="#A020F0">while</FONT></B> A:
        B.append(A.pop())

    <I><FONT COLOR="#B22222"># print first 2 elements of B
</FONT></I>    <B><FONT COLOR="#A020F0">print</FONT></B> B[0], B[1]

    <I><FONT COLOR="#B22222"># reverse B (can be done in place)
</FONT></I>    B.reverse()

    <I><FONT COLOR="#B22222"># is 0 a member of B?
</FONT></I>    <B><FONT COLOR="#A020F0">if</FONT></B> 0 <B><FONT COLOR="#A020F0">in</FONT></B> B:
        <B><FONT COLOR="#A020F0">print</FONT></B> <B><FONT COLOR="#BC8F8F">&quot;true&quot;</FONT></B>
    <B><FONT COLOR="#A020F0">else</FONT></B>:
        <B><FONT COLOR="#A020F0">print</FONT></B> <B><FONT COLOR="#BC8F8F">&quot;false&quot;</FONT></B>

    <I><FONT COLOR="#B22222"># is N a member of B?
</FONT></I>    <B><FONT COLOR="#A020F0">if</FONT></B> N <B><FONT COLOR="#A020F0">in</FONT></B> B:
        <B><FONT COLOR="#A020F0">print</FONT></B> <B><FONT COLOR="#BC8F8F">&quot;true&quot;</FONT></B>
    <B><FONT COLOR="#A020F0">else</FONT></B>:
        <B><FONT COLOR="#A020F0">print</FONT></B> <B><FONT COLOR="#BC8F8F">&quot;false&quot;</FONT></B>

    <I><FONT COLOR="#B22222"># filter values from B to A that are less than N/2, preserving order
</FONT></I>    mid = N/2
    <I><FONT COLOR="#B22222">#A = filter(lambda item: item &lt; mid, B)
</FONT></I>    <B><FONT COLOR="#A020F0">for</FONT></B> item <B><FONT COLOR="#A020F0">in</FONT></B> B:
        <B><FONT COLOR="#A020F0">if</FONT></B> item &lt; mid:
            A.append(item)

    <I><FONT COLOR="#B22222"># print first ten items of A
</FONT></I>    <B><FONT COLOR="#A020F0">print</FONT></B> (<B><FONT COLOR="#BC8F8F">&quot; &quot;</FONT></B>).join(map(<B><FONT COLOR="#A020F0">lambda</FONT></B> x: <B><FONT COLOR="#BC8F8F">&quot;%d&quot;</FONT></B> % x, A[0:10]))

    <I><FONT COLOR="#B22222"># print sum of items in A that are less than 1000
</FONT></I>    <B><FONT COLOR="#A020F0">print</FONT></B> reduce(<B><FONT COLOR="#A020F0">lambda</FONT></B> x, y: x + y, (filter(<B><FONT COLOR="#A020F0">lambda</FONT></B> z: z &lt; 1000, A)))

    <I><FONT COLOR="#B22222"># append B to end of A
</FONT></I>    A = A + B

    <I><FONT COLOR="#B22222"># print length and last element of A
</FONT></I>    <B><FONT COLOR="#A020F0">print</FONT></B> len(A), A[-1]

main()</pre></td></tr></table>
