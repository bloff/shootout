<B><FONT COLOR="#A020F0">procedure </FONT></B><B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">main</FONT></I></B></FONT></B>(argv)
    <B><FONT COLOR="#5F9EA0">local</FONT></B> n, a, b, c, d, e, f, x
    n <B><FONT COLOR="#5F9EA0">:=</FONT></B> argv[1] <B><FONT COLOR="#5F9EA0">|</FONT></B> 1;
    x <B><FONT COLOR="#5F9EA0">:=</FONT></B> 0
    <B><FONT COLOR="#A020F0">every</FONT></B> a <B><FONT COLOR="#5F9EA0">:=</FONT></B> 1 <B><FONT COLOR="#A020F0">to</FONT></B> n <B><FONT COLOR="#A020F0">do</FONT></B>
	<B><FONT COLOR="#A020F0">every</FONT></B> b <B><FONT COLOR="#5F9EA0">:=</FONT></B> 1 <B><FONT COLOR="#A020F0">to</FONT></B> n <B><FONT COLOR="#A020F0">do</FONT></B>
	    <B><FONT COLOR="#A020F0">every</FONT></B> c <B><FONT COLOR="#5F9EA0">:=</FONT></B> 1 <B><FONT COLOR="#A020F0">to</FONT></B> n <B><FONT COLOR="#A020F0">do</FONT></B>
		<B><FONT COLOR="#A020F0">every</FONT></B> d <B><FONT COLOR="#5F9EA0">:=</FONT></B> 1 <B><FONT COLOR="#A020F0">to</FONT></B> n <B><FONT COLOR="#A020F0">do</FONT></B>
		    <B><FONT COLOR="#A020F0">every</FONT></B> e <B><FONT COLOR="#5F9EA0">:=</FONT></B> 1 <B><FONT COLOR="#A020F0">to</FONT></B> n <B><FONT COLOR="#A020F0">do</FONT></B>
			<B><FONT COLOR="#A020F0">every</FONT></B> f <B><FONT COLOR="#5F9EA0">:=</FONT></B> 1 <B><FONT COLOR="#A020F0">to</FONT></B> n <B><FONT COLOR="#A020F0">do</FONT></B>
			    x <B><FONT COLOR="#5F9EA0">+</FONT></B><B><FONT COLOR="#5F9EA0">:=</FONT></B> 1
    <B><FONT COLOR="#A020F0">write</FONT></B>(x)
<B><FONT COLOR="#A020F0">end</FONT></B></pre></td></tr></table>
