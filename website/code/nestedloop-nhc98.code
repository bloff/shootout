<B><FONT COLOR="#A020F0">import</FONT></B> System(getArgs)

main = do [number] &lt;- getArgs
	  <B><FONT COLOR="#A020F0">let</FONT></B> n = read number
          print (loopA n n 0)
    <B><FONT COLOR="#A020F0">where</FONT></B> loopA <B><FONT COLOR="#228B22">:: Int -&gt; Int -&gt; Int -&gt; Int
</FONT></B>	  loopA 0 m x = x
	  loopA n m x = loopA (n-1) m (loopB m m x)

	  loopB 0 m x = x
	  loopB n m x = loopB (n-1) m (loopC m m x)

	  loopC 0 m x = x
	  loopC n m x = loopC (n-1) m (loopD m m x)

	  loopD 0 m x = x
	  loopD n m x = loopD (n-1) m (loopE m m x)

	  loopE 0 m x = x
	  loopE n m x = loopE (n-1) m (loopF m x)

	  loopF 0 x = x
	  loopF n x = loopF (n-1) (x+1)
</pre></td></tr></table>
