<B><FONT COLOR="#A020F0">procedure </FONT></B><B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">main</FONT></I></B></FONT></B>(argv)
    <I><FONT COLOR="#B22222"># local n, i, k, x, y, last
</FONT></I>    n <B><FONT COLOR="#5F9EA0">:=</FONT></B> argv[1] <B><FONT COLOR="#5F9EA0">|</FONT></B> 1
    x <B><FONT COLOR="#5F9EA0">:=</FONT></B> <B><FONT COLOR="#A020F0">list</FONT></B>(n,0)
    y <B><FONT COLOR="#5F9EA0">:=</FONT></B> <B><FONT COLOR="#A020F0">list</FONT></B>(n,0)
    <B><FONT COLOR="#A020F0">every</FONT></B> i <B><FONT COLOR="#5F9EA0">:=</FONT></B> 1 <B><FONT COLOR="#A020F0">to</FONT></B> n <B><FONT COLOR="#A020F0">do</FONT></B> {
	x[i] <B><FONT COLOR="#5F9EA0">:=</FONT></B> i
    }
    <B><FONT COLOR="#A020F0">every</FONT></B> k <B><FONT COLOR="#5F9EA0">:=</FONT></B> 0 <B><FONT COLOR="#A020F0">to</FONT></B> 999 <B><FONT COLOR="#A020F0">do</FONT></B> {
	<B><FONT COLOR="#A020F0">every</FONT></B> i <B><FONT COLOR="#5F9EA0">:=</FONT></B> n <B><FONT COLOR="#A020F0">to</FONT></B> 1 <B><FONT COLOR="#A020F0">by</FONT></B> <B><FONT COLOR="#5F9EA0">-</FONT></B>1 <B><FONT COLOR="#A020F0">do</FONT></B> {
	    y[i] <B><FONT COLOR="#5F9EA0">+</FONT></B><B><FONT COLOR="#5F9EA0">:=</FONT></B> x[i]
	}
    }
    <B><FONT COLOR="#A020F0">write</FONT></B>(y[1], <B><FONT COLOR="#BC8F8F">&quot; &quot;</FONT></B>, y[n])
<B><FONT COLOR="#A020F0">end</FONT></B></pre></td></tr></table>
