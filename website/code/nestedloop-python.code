<B><FONT COLOR="#A020F0">import</FONT></B> sys

<B><FONT COLOR="#A020F0">def</FONT></B> <B><FONT COLOR="#0000FF">main</FONT></B>():
    x = 0
    iter = int(sys.argv[1])
    <B><FONT COLOR="#A020F0">if</FONT></B> iter &lt; 1:
        iter = 1
    
    i_r = range(iter)
    <B><FONT COLOR="#A020F0">for</FONT></B> a <B><FONT COLOR="#A020F0">in</FONT></B> i_r:
        <B><FONT COLOR="#A020F0">for</FONT></B> b <B><FONT COLOR="#A020F0">in</FONT></B> i_r:
            <B><FONT COLOR="#A020F0">for</FONT></B> c <B><FONT COLOR="#A020F0">in</FONT></B> i_r:
                <B><FONT COLOR="#A020F0">for</FONT></B> d <B><FONT COLOR="#A020F0">in</FONT></B> i_r:
                    <B><FONT COLOR="#A020F0">for</FONT></B> e <B><FONT COLOR="#A020F0">in</FONT></B> i_r:
                        <B><FONT COLOR="#A020F0">for</FONT></B> f <B><FONT COLOR="#A020F0">in</FONT></B> i_r:
                            x += 1
    <B><FONT COLOR="#A020F0">print</FONT></B> x

main()</pre></td></tr></table>
