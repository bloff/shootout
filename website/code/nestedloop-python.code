<span class="kwc">import</span> sys

<span class="kwb">def</span> <span class="kwd">main</span><span class="sym">():</span>
    x <span class="sym">=</span> <span class="num">0</span>
    iter <span class="sym">=</span> <span class="kwa">int</span><span class="sym">(</span>sys<span class="sym">.</span>argv<span class="sym">[</span><span class="num">1</span><span class="sym">])</span>
    <span class="kwb">if</span> iter <span class="sym">&lt;</span> <span class="num">1</span><span class="sym">:</span>
        iter <span class="sym">=</span> <span class="num">1</span>

    i_r <span class="sym">=</span> <span class="kwa">range</span><span class="sym">(</span>iter<span class="sym">)</span>
    <span class="kwb">for</span> a <span class="kwb">in</span> i_r<span class="sym">:</span>
        <span class="kwb">for</span> b <span class="kwb">in</span> i_r<span class="sym">:</span>
            <span class="kwb">for</span> c <span class="kwb">in</span> i_r<span class="sym">:</span>
                <span class="kwb">for</span> d <span class="kwb">in</span> i_r<span class="sym">:</span>
                    <span class="kwb">for</span> e <span class="kwb">in</span> i_r<span class="sym">:</span>
                        <span class="kwb">for</span> f <span class="kwb">in</span> i_r<span class="sym">:</span>
                            x <span class="sym">+=</span> <span class="num">1</span>
    <span class="kwb">print</span> x

<span class="kwd">main</span><span class="sym">()</span>
