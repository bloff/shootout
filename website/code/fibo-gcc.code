#<B><FONT COLOR="#5F9EA0">include</FONT></B> <B><FONT COLOR="#BC8F8F">&lt;stdio.h&gt;</FONT></B>
#<B><FONT COLOR="#5F9EA0">include</FONT></B> <B><FONT COLOR="#BC8F8F">&lt;stdlib.h&gt;</FONT></B>

<B><FONT COLOR="#228B22">unsigned</FONT></B> <B><FONT COLOR="#228B22">long</FONT></B> <B><FONT COLOR="#0000FF">fib</FONT></B>(<B><FONT COLOR="#228B22">unsigned</FONT></B> <B><FONT COLOR="#228B22">long</FONT></B> n) __attribute__ ((regparm(3)));

<B><FONT COLOR="#228B22">unsigned</FONT></B> <B><FONT COLOR="#228B22">long</FONT></B>
<B><FONT COLOR="#0000FF">fib</FONT></B>(<B><FONT COLOR="#228B22">unsigned</FONT></B> <B><FONT COLOR="#228B22">long</FONT></B> n) {
    <B><FONT COLOR="#A020F0">return</FONT></B>( (n &lt; 2) ? 1 : (fib(n-2) + fib(n-1)) );
}

<B><FONT COLOR="#228B22">int</FONT></B>
<B><FONT COLOR="#0000FF">main</FONT></B>(<B><FONT COLOR="#228B22">int</FONT></B> argc, <B><FONT COLOR="#228B22">char</FONT></B> *argv[]) {
    <B><FONT COLOR="#228B22">int</FONT></B> N = ((argc == 2) ? atoi(argv[1]) : 1);
    printf(<B><FONT COLOR="#BC8F8F">&quot;%ld\n&quot;</FONT></B>, fib(N));
    <B><FONT COLOR="#A020F0">return</FONT></B>(0);
}</pre></td></tr></table>
