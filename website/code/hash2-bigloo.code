<table width="100%" border="0" cellpadding="5" cellspacing="0"><tr><td bgcolor="#FFFFFF"><pre>
<I><FONT COLOR="#B22222">;;; -*- mode: scheme -*-
</FONT></I><I><FONT COLOR="#B22222">;;; $Id: hash2-bigloo.code,v 1.2 2004-11-08 08:15:14 bfulgham Exp $
</FONT></I><I><FONT COLOR="#B22222">;;; http://www.bagley.org/~doug/shootout/
</FONT></I>
(module hash
	(option (<B><FONT COLOR="#A020F0">set!</FONT></B> *genericity* #f))
	(main main))

(<B><FONT COLOR="#A020F0">define</FONT></B> (<B><FONT COLOR="#0000FF">main</FONT></B> args)
  (<B><FONT COLOR="#A020F0">let*</FONT></B> ((n (<B><FONT COLOR="#A020F0">or</FONT></B> (<B><FONT COLOR="#A020F0">and</FONT></B> (= (length args) 2) (string-&gt;number (cadr args))) 1))
	 (max (<B><FONT COLOR="#A020F0">do</FONT></B> ((max 1 (* max 2))) ((&gt; max 10000) max)))
	 (x (make-hash-table max
			     (<B><FONT COLOR="#A020F0">lambda</FONT></B> (o) (string-&gt;0..2^x-1 o 14))
			     (<B><FONT COLOR="#A020F0">lambda</FONT></B> (c) (car c))
			     string=?
			     max))
	 (y (make-hash-table max
			     (<B><FONT COLOR="#A020F0">lambda</FONT></B> (o) (string-&gt;0..2^x-1 o 14))
			     (<B><FONT COLOR="#A020F0">lambda</FONT></B> (c) (car c))
			     string=?
			     max)))
    (<B><FONT COLOR="#A020F0">do</FONT></B> ((i 0 (+ i 1)))
	((= i 10000))
      (put-hash! (cons (string-append <B><FONT COLOR="#BC8F8F">&quot;foo_&quot;</FONT></B> (number-&gt;string i 10)) i) x))
    (<B><FONT COLOR="#A020F0">do</FONT></B> ((i 0 (+ i 1)))
	((= i n))
      (for-each-hash (<B><FONT COLOR="#A020F0">lambda</FONT></B> (cell)
		       (<B><FONT COLOR="#A020F0">let*</FONT></B> ((x-key (car cell))
			      (x-val (cdr cell))
			      (old-y-cell (get-hash x-key y))
			      (old-y-val (<B><FONT COLOR="#A020F0">cond</FONT></B> (old-y-cell
						(rem-key-hash! x-key y)
						(cdr old-y-cell))
					       (<B><FONT COLOR="#A020F0">else</FONT></B>
						0))))
			 (put-hash! (cons x-key (+ x-val old-y-val)) y)))
		     x))
    (print (cdr (get-hash <B><FONT COLOR="#BC8F8F">&quot;foo_1&quot;</FONT></B> x)) <B><FONT COLOR="#BC8F8F">&quot; &quot;</FONT></B>
	   (cdr (get-hash <B><FONT COLOR="#BC8F8F">&quot;foo_9999&quot;</FONT></B> x)) <B><FONT COLOR="#BC8F8F">&quot; &quot;</FONT></B>
	   (cdr (get-hash <B><FONT COLOR="#BC8F8F">&quot;foo_1&quot;</FONT></B> y)) <B><FONT COLOR="#BC8F8F">&quot; &quot;</FONT></B>
	   (cdr (get-hash <B><FONT COLOR="#BC8F8F">&quot;foo_9999&quot;</FONT></B> y)))))</pre></td></tr></table>
