(setq max-lisp-eval-depth 8000)
(setq max-specpdl-size 5000)

(<B><FONT COLOR="#A020F0">defun</FONT></B> <B><FONT COLOR="#0000FF">Ack</FONT></B> (M N)
  (<B><FONT COLOR="#A020F0">cond</FONT></B> ((= M 0) (+ N 1))
	((= N 0) (Ack (- M 1) 1))
	(t (Ack (- M 1) (Ack M (- N 1))))))

(<B><FONT COLOR="#A020F0">defun</FONT></B> <B><FONT COLOR="#0000FF">main</FONT></B> ()
  (<B><FONT COLOR="#A020F0">let</FONT></B>* ((arg (car command-line-args-left))
	 (num (<B><FONT COLOR="#A020F0">if</FONT></B> arg (string-to-number arg) 1)))
    (princ (format <B><FONT COLOR="#BC8F8F">&quot;Ack(3,%d): %d\n&quot;</FONT></B> num (Ack 3 num)))))</pre></td></tr></table>
