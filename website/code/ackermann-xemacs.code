    (</span><span class="kwa">princ</span> <span class="sym">(</span>format <span class="str">&quot;Ack(3,%d): %d</span><span class="esc">\n</span><span class="str">&quot;</span> num <span class="sym">(</span>Ack <span class="num">3</span> num<span class="sym">)))))</span>
