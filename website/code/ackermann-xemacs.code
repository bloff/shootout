<span class="sym">(</span><span class="kwa">setq max</span><span class="sym">-</span>lisp<span class="sym">-</span><span class="kwa">eval</span><span class="sym">-</span>depth <span class="num">8000</span><span class="sym">)
(</span><span class="kwa">setq max</span><span class="sym">-</span>specpdl<span class="sym">-</span>size <span class="num">5000</span><span class="sym">)

(</span><span class="kwa">defun</span> Ack <span class="sym">(</span>M N<span class="sym">)
