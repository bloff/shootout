define <B><FONT COLOR="#0000FF">fib</FONT></B> ();
define <B><FONT COLOR="#0000FF">fib</FONT></B>(n)
{
   <B><FONT COLOR="#A020F0">if</FONT></B> (n &lt; 2)
     <B><FONT COLOR="#A020F0">return</FONT></B> 1;
   fib(n-2) + fib(n-1);
}
define <B><FONT COLOR="#0000FF">main</FONT></B> ()
{
   variable n = 1;
   <B><FONT COLOR="#A020F0">if</FONT></B> (__argc &gt; 1) n = integer (__argv[1]);
   fprintf (stdout, <B><FONT COLOR="#BC8F8F">&quot;%d\n&quot;</FONT></B>, fib(n));
}
<B><FONT COLOR="#0000FF">main</FONT></B> ();</pre></td></tr></table>
