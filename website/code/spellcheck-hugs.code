<I><FONT COLOR="#B22222">-- compile with: ghc -O -package data
</FONT></I>
<B><FONT COLOR="#A020F0">import</FONT></B> Data.Set

main = do
	d &lt;- readFile <B><FONT COLOR="#BC8F8F">&quot;Usr.Dict.Words&quot;</FONT></B>
	<B><FONT COLOR="#A020F0">let</FONT></B> words = mkSet (lines d)
	<B><FONT COLOR="#A020F0">let</FONT></B> misspelled x = not $ x `elementOf` words
	interact $ unlines . filter misspelled . lines</pre></td></tr></table>
