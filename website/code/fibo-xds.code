<span class="kwa">MODULE</span> fibo<span class="sym">;
</span><span class="kwa">IMPORT</span> Shootout<span class="sym">,</span> Out<span class="sym">;


</span><span class="kwa">PROCEDURE</span> Fibo <span class="sym">(</span>n<span class="sym">:</span> <span class="kwb">LONGINT</span><span class="sym">):</span> <span class="kwb">LONGINT</span><span class="sym">;
</span><span class="kwa">BEGIN
   IF</span> n<span class="sym">&lt;</span><span class="num">2</span> <span class="kwa">THEN
      RETURN</span> <span class="num">1</span><span class="sym">;
</span>   <span class="kwa">ELSE
