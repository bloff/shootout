</span>   <span class="kwa">END</span><span class="sym">;
</span><span class="kwa">END</span> Fibo<span class="sym">;


</span><span class="kwa">BEGIN
</span>   Out<span class="sym">.</span>Int<span class="sym">(</span> Fibo<span class="sym">(</span> Shootout<span class="sym">.</span>Argi<span class="sym">() ),</span><span class="num">0</span><span class="sym">);</span> Out<span class="sym">.</span>Ln<span class="sym">;
</span><span class="kwa">END</span> fibo<span class="sym">.</span>
