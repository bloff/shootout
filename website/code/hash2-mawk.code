<B><FONT COLOR="#A020F0">BEGIN</FONT></B> {
    n = (<B><FONT COLOR="#A020F0">ARGV</FONT></B>[1] &lt; 1) ? 1 : <B><FONT COLOR="#A020F0">ARGV</FONT></B>[1];

    <B><FONT COLOR="#A020F0">for</FONT></B> (i=0; i&lt;10000; i++)
	hash1[<B><FONT COLOR="#A020F0">sprintf</FONT></B>(<B><FONT COLOR="#BC8F8F">&quot;foo_%d&quot;</FONT></B>, i)] = i
    <B><FONT COLOR="#A020F0">for</FONT></B> (i=0; i&lt;n; i++)
	<B><FONT COLOR="#A020F0">for</FONT></B> (k <B><FONT COLOR="#A020F0">in</FONT></B> hash1)
	    hash2[k] += hash1[k]
    <B><FONT COLOR="#A020F0">print</FONT></B> hash1[<B><FONT COLOR="#BC8F8F">&quot;foo_1&quot;</FONT></B>], hash1[<B><FONT COLOR="#BC8F8F">&quot;foo_9999&quot;</FONT></B>], hash2[<B><FONT COLOR="#BC8F8F">&quot;foo_1&quot;</FONT></B>], hash2[<B><FONT COLOR="#BC8F8F">&quot;foo_9999&quot;</FONT></B>]
}</pre></td></tr></table>
