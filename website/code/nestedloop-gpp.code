#<B><FONT COLOR="#5F9EA0">include</FONT></B> <B><FONT COLOR="#BC8F8F">&lt;iostream&gt;</FONT></B>
#<B><FONT COLOR="#5F9EA0">include</FONT></B> <B><FONT COLOR="#BC8F8F">&lt;stdlib.h&gt;</FONT></B>

using namespace std;

<B><FONT COLOR="#228B22">int</FONT></B> <B><FONT COLOR="#0000FF">main</FONT></B>(<B><FONT COLOR="#228B22">int</FONT></B> argc, <B><FONT COLOR="#228B22">char</FONT></B> *argv[]) {
    <B><FONT COLOR="#228B22">int</FONT></B> n = ((argc == 2) ? atoi(argv[1]) : 1);
    <B><FONT COLOR="#228B22">int</FONT></B> a, b, c, d, e, f, x=0;
	
    <B><FONT COLOR="#A020F0">for</FONT></B> (a=0; a&lt;n; a++)
	<B><FONT COLOR="#A020F0">for</FONT></B> (b=0; b&lt;n; b++)
	    <B><FONT COLOR="#A020F0">for</FONT></B> (c=0; c&lt;n; c++)
		<B><FONT COLOR="#A020F0">for</FONT></B> (d=0; d&lt;n; d++)
		    <B><FONT COLOR="#A020F0">for</FONT></B> (e=0; e&lt;n; e++)
			<B><FONT COLOR="#A020F0">for</FONT></B> (f=0; f&lt;n; f++)
			    x++;

    cout &lt;&lt; x &lt;&lt; endl;
    <B><FONT COLOR="#A020F0">return</FONT></B>(0);
}</pre></td></tr></table>
