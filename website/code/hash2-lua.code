<B><FONT COLOR="#A020F0">local</FONT></B> n <B><FONT COLOR="#5F9EA0">=</FONT></B> <B><FONT COLOR="#228B22">tonumber</FONT></B>((arg <B><FONT COLOR="#A020F0">and</FONT></B> arg[1]) <B><FONT COLOR="#A020F0">or</FONT></B> 1)

<B><FONT COLOR="#A020F0">local</FONT></B> hash1<B><FONT COLOR="#5F9EA0">=</FONT></B>{}
<B><FONT COLOR="#A020F0">for</FONT></B> i<B><FONT COLOR="#5F9EA0">=</FONT></B>0,10000 <B><FONT COLOR="#A020F0">do</FONT></B>
    hash1[<B><FONT COLOR="#BC8F8F">&quot;foo_&quot;</FONT></B>..i] <B><FONT COLOR="#5F9EA0">=</FONT></B> i
<B><FONT COLOR="#A020F0">end</FONT></B>
<B><FONT COLOR="#A020F0">local</FONT></B> hash2<B><FONT COLOR="#5F9EA0">=</FONT></B>{}
<B><FONT COLOR="#A020F0">for</FONT></B> i<B><FONT COLOR="#5F9EA0">=</FONT></B>1,n <B><FONT COLOR="#A020F0">do</FONT></B>
    <B><FONT COLOR="#A020F0">for</FONT></B> k,v <B><FONT COLOR="#A020F0">in</FONT></B> hash1 <B><FONT COLOR="#A020F0">do</FONT></B>
	hash2[k] <B><FONT COLOR="#5F9EA0">=</FONT></B> v <B><FONT COLOR="#5F9EA0">+</FONT></B> (hash2[k] <B><FONT COLOR="#A020F0">or</FONT></B> 0)
    <B><FONT COLOR="#A020F0">end</FONT></B>
<B><FONT COLOR="#A020F0">end</FONT></B>

io.write(string.format(<B><FONT COLOR="#BC8F8F">&quot;%d %d %d %d\n&quot;</FONT></B>, hash1[<B><FONT COLOR="#BC8F8F">&quot;foo_1&quot;</FONT></B>], hash1[<B><FONT COLOR="#BC8F8F">&quot;foo_9999&quot;</FONT></B>],
	     hash2[<B><FONT COLOR="#BC8F8F">&quot;foo_1&quot;</FONT></B>], hash2[<B><FONT COLOR="#BC8F8F">&quot;foo_9999&quot;</FONT></B>]))</pre></td></tr></table>
