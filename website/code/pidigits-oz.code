	       <span class="sym">{</span>System.printInfo <span class="str">&quot;</span><span class="esc">\t</span><span class="str">:&quot;</span>#<span class="sym">(</span><span class="num">10</span><span class="sym">+</span>Row<span class="sym">)</span>#<span class="str">&quot;</span><span class="esc">\n</span><span class="str">&quot;</span>#Y<span class="sym">}</span>
	       <span class="sym">{</span>Digit K <span class="sym">{</span>Prod Z Y<span class="sym">}</span> N<span class="sym">-</span><span class="num">1 10</span><span class="sym">+</span>Row <span class="num">1</span><span class="sym">}</span>
	    <span class="kwb">else</span>
