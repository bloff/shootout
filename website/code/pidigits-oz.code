      <span class="kwb">end</span>
   <span class="kwb">end</span>

   <span class="kwb">fun</span> <span class="sym">{</span>Prod Z N<span class="sym">} {</span>Comp <span class="sym">[</span><span class="num">10</span> ~<span class="num">10</span><span class="sym">*</span>N <span class="num">0 1</span><span class="sym">]</span> Z<span class="sym">}</span> <span class="kwb">end</span>

   <span class="kwb">fun</span> <span class="sym">{</span>Cons Z K<span class="sym">} {</span>Comp Z <span class="sym">[</span>K <span class="num">4</span><span class="sym">*</span>K<span class="sym">+</span><span class="num">2 0 2</span><span class="sym">*</span>K<span class="sym">+</span><span class="num">1</span><span class="sym">]}</span> <span class="kwb">end</span>

   <span class="kwb">proc</span> <span class="sym">{</span>PrintNSpace N<span class="sym">}</span>
