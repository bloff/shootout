<I><FONT COLOR="#B22222">%%% $Id: nestedloop-oz.code,v 1.1 2004-11-02 08:52:37 bfulgham Exp $
</FONT></I><I><FONT COLOR="#B22222">%%% http://dada.perl.it/shootout/
</FONT></I><I><FONT COLOR="#B22222">%%% 
</FONT></I><I><FONT COLOR="#B22222">%%% Isaac Gouy suggested the use of a cell
</FONT></I>
<B><FONT COLOR="#228B22">functor</FONT></B>
<B><FONT COLOR="#5F9EA0">import</FONT></B> System Application
<B><FONT COLOR="#A020F0">define</FONT></B>

    <B><FONT COLOR="#5F9EA0">local</FONT></B> P X A B C D E F N <B><FONT COLOR="#A020F0">in</FONT></B>
        [P] <B><FONT COLOR="#5F9EA0">=</FONT></B> {<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">Application</FONT></I></B></FONT></B>.getArgs plain}
        N <B><FONT COLOR="#5F9EA0">=</FONT></B> {<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">String</FONT></I></B></FONT></B>.toInt P}
        X <B><FONT COLOR="#5F9EA0">=</FONT></B> {<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">NewCell</FONT></I></B></FONT></B> 0}
        <B><FONT COLOR="#A020F0">for</FONT></B> A <B><FONT COLOR="#A020F0">in</FONT></B> 1<B><FONT COLOR="#5F9EA0">..</FONT></B>N <B><FONT COLOR="#A020F0">do</FONT></B>
            <B><FONT COLOR="#A020F0">for</FONT></B> B <B><FONT COLOR="#A020F0">in</FONT></B> 1<B><FONT COLOR="#5F9EA0">..</FONT></B>N <B><FONT COLOR="#A020F0">do</FONT></B>
                <B><FONT COLOR="#A020F0">for</FONT></B> C <B><FONT COLOR="#A020F0">in</FONT></B> 1<B><FONT COLOR="#5F9EA0">..</FONT></B>N <B><FONT COLOR="#A020F0">do</FONT></B>
                    <B><FONT COLOR="#A020F0">for</FONT></B> D <B><FONT COLOR="#A020F0">in</FONT></B> 1<B><FONT COLOR="#5F9EA0">..</FONT></B>N <B><FONT COLOR="#A020F0">do</FONT></B>
                        <B><FONT COLOR="#A020F0">for</FONT></B> E <B><FONT COLOR="#A020F0">in</FONT></B> 1<B><FONT COLOR="#5F9EA0">..</FONT></B>N <B><FONT COLOR="#A020F0">do</FONT></B>
                            <B><FONT COLOR="#A020F0">for</FONT></B> F <B><FONT COLOR="#A020F0">in</FONT></B> 1<B><FONT COLOR="#5F9EA0">..</FONT></B>N <B><FONT COLOR="#A020F0">do</FONT></B>
                                {<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">Assign</FONT></I></B></FONT></B> X {<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">Access</FONT></I></B></FONT></B> X}+1}
                            <B><FONT COLOR="#A020F0">end</FONT></B>
                        <B><FONT COLOR="#A020F0">end</FONT></B>
                    <B><FONT COLOR="#A020F0">end</FONT></B>
                <B><FONT COLOR="#A020F0">end</FONT></B>
            <B><FONT COLOR="#A020F0">end</FONT></B>
        <B><FONT COLOR="#A020F0">end</FONT></B>
        {<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">System</FONT></I></B></FONT></B>.show {<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">Access</FONT></I></B></FONT></B> X}}
    <B><FONT COLOR="#A020F0">end</FONT></B>
    {<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">Application</FONT></I></B></FONT></B>.exit 0}
<B><FONT COLOR="#A020F0">end</FONT></B></pre></td></tr></table>
