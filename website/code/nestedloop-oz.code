<span class="line">    1 </span><span class="slc">%%% $Id: nestedloop-oz.code,v 1.4 2004-11-13 07:42:21 bfulgham Exp $
</span><span class="line">    2 </span><span class="slc"></span><span class="slc">%%% http://dada.perl.it/shootout/
</span><span class="line">    3 </span><span class="slc"></span><span class="slc">%%%
</span><span class="line">    4 </span><span class="slc"></span><span class="slc">%%% Isaac Gouy suggested the use of a cell
</span><span class="line">    5 </span><span class="slc"></span>
<span class="line">    6 </span><span class="kwa">functor
</span><span class="line">    7 </span><span class="kwa">import</span> System Application
<span class="line">    8 </span><span class="kwb">define
</span><span class="line">    9 </span><span class="kwb">
</span><span class="line">   10 </span><span class="kwb"></span>    <span class="kwa">local</span> P X A B C D E F N <span class="kwb">in
</span><span class="line">   11 </span><span class="kwb"></span>        <span class="sym">[</span>P<span class="sym">] = {</span>Application.getArgs plain<span class="sym">}
</span><span class="line">   12 </span><span class="sym"></span>        N <span class="sym">= {</span>String.toInt P<span class="sym">}
</span><span class="line">   13 </span><span class="sym"></span>        X <span class="sym">= {</span>NewCell <span class="num">0</span><span class="sym">}
</span><span class="line">   14 </span><span class="sym"></span>        <span class="kwb">for</span> A <span class="kwb">in</span> <span class="num">1</span>..N <span class="kwb">do
</span><span class="line">   15 </span><span class="kwb">            for</span> B <span class="kwb">in</span> <span class="num">1</span>..N <span class="kwb">do
</span><span class="line">   16 </span><span class="kwb">                for</span> C <span class="kwb">in</span> <span class="num">1</span>..N <span class="kwb">do
</span><span class="line">   17 </span><span class="kwb">                    for</span> D <span class="kwb">in</span> <span class="num">1</span>..N <span class="kwb">do
</span><span class="line">   18 </span><span class="kwb">                        for</span> E <span class="kwb">in</span> <span class="num">1</span>..N <span class="kwb">do
</span><span class="line">   19 </span><span class="kwb">                            for</span> F <span class="kwb">in</span> <span class="num">1</span>..N <span class="kwb">do
</span><span class="line">   20 </span><span class="kwb"></span>                                <span class="sym">{</span>Assign X <span class="sym">{</span>Access X<span class="sym">}+</span><span class="num">1</span><span class="sym">}
</span><span class="line">   21 </span><span class="sym"></span>                            <span class="kwb">end
</span><span class="line">   22 </span><span class="kwb">                        end
</span><span class="line">   23 </span><span class="kwb">                    end
</span><span class="line">   24 </span><span class="kwb">                end
</span><span class="line">   25 </span><span class="kwb">            end
</span><span class="line">   26 </span><span class="kwb">        end
</span><span class="line">   27 </span><span class="kwb"></span>        <span class="sym">{</span>System.show <span class="sym">{</span>Access X<span class="sym">}}
</span><span class="line">   28 </span><span class="sym"></span>    <span class="kwb">end
</span><span class="line">   29 </span><span class="kwb"></span>    <span class="sym">{</span>Application.exit <span class="num">0</span><span class="sym">}
</span><span class="line">   30 </span><span class="sym"></span><span class="kwb">end</span>
