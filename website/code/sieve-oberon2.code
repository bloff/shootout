*)</FONT></I>

<B><FONT COLOR="#228B22">MODULE</FONT></B> Sieve;
<B><FONT COLOR="#5F9EA0">IMPORT</FONT></B> LangShootout, Out;

<B><FONT COLOR="#5F9EA0">CONST</FONT></B>
   start <B><FONT COLOR="#5F9EA0">=</FONT></B> 2;
   stop <B><FONT COLOR="#5F9EA0">=</FONT></B> 8192;
   size <B><FONT COLOR="#5F9EA0">=</FONT></B> stop<B><FONT COLOR="#5F9EA0">+</FONT></B>1;

<B><FONT COLOR="#A020F0">VAR</FONT></B>
   n, count, i, k: <B><FONT COLOR="#228B22">LONGINT</FONT></B>;
   isPrimeNumber: <B><FONT COLOR="#228B22">ARRAY</FONT></B> size <B><FONT COLOR="#A020F0">OF</FONT></B> <B><FONT COLOR="#228B22">BOOLEAN</FONT></B>;

<B><FONT COLOR="#A020F0">BEGIN</FONT></B>
   n <B><FONT COLOR="#5F9EA0">:=</FONT></B> LangShootout.Argi();
   <B><FONT COLOR="#A020F0">WHILE</FONT></B> n <B><FONT COLOR="#5F9EA0">&gt;</FONT></B> 0 <B><FONT COLOR="#A020F0">DO</FONT></B>
      <B><FONT COLOR="#A020F0">DEC</FONT></B>(n);
      count <B><FONT COLOR="#5F9EA0">:=</FONT></B> 0;      
      <B><FONT COLOR="#A020F0">FOR</FONT></B> i <B><FONT COLOR="#5F9EA0">:=</FONT></B> start <B><FONT COLOR="#A020F0">TO</FONT></B> stop <B><FONT COLOR="#A020F0">DO</FONT></B> isPrimeNumber[i] <B><FONT COLOR="#5F9EA0">:=</FONT></B> <B><FONT COLOR="#A020F0">TRUE</FONT></B>; <B><FONT COLOR="#A020F0">END</FONT></B>;
      <B><FONT COLOR="#A020F0">FOR</FONT></B> i <B><FONT COLOR="#5F9EA0">:=</FONT></B> start <B><FONT COLOR="#A020F0">TO</FONT></B> stop <B><FONT COLOR="#A020F0">DO</FONT></B>
         <B><FONT COLOR="#A020F0">IF</FONT></B> isPrimeNumber[i] <B><FONT COLOR="#A020F0">THEN</FONT></B>
            <B><FONT COLOR="#A020F0">INC</FONT></B>(count);
            k <B><FONT COLOR="#5F9EA0">:=</FONT></B> i<B><FONT COLOR="#5F9EA0">+</FONT></B>i;
            <B><FONT COLOR="#A020F0">WHILE</FONT></B> k <B><FONT COLOR="#5F9EA0">&lt;=</FONT></B> stop <B><FONT COLOR="#A020F0">DO</FONT></B>
               isPrimeNumber[k] <B><FONT COLOR="#5F9EA0">:=</FONT></B> <B><FONT COLOR="#A020F0">FALSE</FONT></B>;
               <B><FONT COLOR="#A020F0">INC</FONT></B>(k, i);
            <B><FONT COLOR="#A020F0">END</FONT></B>;
         <B><FONT COLOR="#A020F0">END</FONT></B>;
      <B><FONT COLOR="#A020F0">END</FONT></B>;
   <B><FONT COLOR="#A020F0">END</FONT></B>;  	

   Out.String(<B><FONT COLOR="#BC8F8F">&quot;Count: &quot;</FONT></B>); Out.Int(count,0); Out.Ln;
<B><FONT COLOR="#A020F0">END </FONT></B><B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">Sieve</FONT></I></B></FONT></B>.</pre></td></tr></table>
