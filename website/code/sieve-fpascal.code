<table width="100%" border="0" cellpadding="5" cellspacing="0"><tr><td bgcolor="#FFFFFF"><pre>
<B><FONT COLOR="#A020F0">program</FONT></B> sieve;
<B><FONT COLOR="#A020F0">uses</FONT></B> SysUtils;

<B><FONT COLOR="#A020F0">var</FONT></B> 
	NUM, i, k, count : integer;
	flags : <B><FONT COLOR="#A020F0">array</FONT></B>[0..8192] <B><FONT COLOR="#A020F0">of</FONT></B> integer;

<B><FONT COLOR="#A020F0">begin</FONT></B>
	<B><FONT COLOR="#A020F0">if</FONT></B> ParamCount = 0 <B><FONT COLOR="#A020F0">then</FONT></B>
		NUM := 1
	<B><FONT COLOR="#A020F0">else</FONT></B>
		NUM := StrToInt(ParamStr(1));
		
	<B><FONT COLOR="#A020F0">if</FONT></B> NUM &lt; 1 <B><FONT COLOR="#A020F0">then</FONT></B> NUM := 1;

	<B><FONT COLOR="#A020F0">while</FONT></B> NUM &gt; 0 <B><FONT COLOR="#A020F0">do</FONT></B>
	<B><FONT COLOR="#A020F0">begin</FONT></B>
		Dec(NUM);
		count := 0;
		<B><FONT COLOR="#A020F0">for</FONT></B> i := 0 <B><FONT COLOR="#A020F0">to</FONT></B> 8192 <B><FONT COLOR="#A020F0">do</FONT></B>
		<B><FONT COLOR="#A020F0">begin</FONT></B>
			flags[i] := i;
		<B><FONT COLOR="#A020F0">end</FONT></B>;
		<B><FONT COLOR="#A020F0">for</FONT></B> i := 2 <B><FONT COLOR="#A020F0">to</FONT></B> 8192 <B><FONT COLOR="#A020F0">do</FONT></B>
		<B><FONT COLOR="#A020F0">begin</FONT></B>
			<B><FONT COLOR="#A020F0">if</FONT></B> flags[i] &lt;&gt; -1 <B><FONT COLOR="#A020F0">then</FONT></B>
			<B><FONT COLOR="#A020F0">begin</FONT></B>
				k := i+i;
				<B><FONT COLOR="#A020F0">while</FONT></B> k &lt;= 8192 <B><FONT COLOR="#A020F0">do</FONT></B>
				<B><FONT COLOR="#A020F0">begin</FONT></B>
					flags[k] := -1;
					Inc(k, i);
				<B><FONT COLOR="#A020F0">end</FONT></B>;
				Inc(count);
			<B><FONT COLOR="#A020F0">end</FONT></B>;
		<B><FONT COLOR="#A020F0">end</FONT></B>;
	<B><FONT COLOR="#A020F0">end</FONT></B>;
	WriteLn(<B><FONT COLOR="#BC8F8F">'Count: '</FONT></B> + IntToStr(Count));
<B><FONT COLOR="#A020F0">end</FONT></B>.</pre></td></tr></table>
