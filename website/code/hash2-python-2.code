<B><FONT COLOR="#A020F0">import</FONT></B> sys

n = int(sys.argv[1])
hash1 = {}
hash2 = {}
<B><FONT COLOR="#A020F0">for</FONT></B> i <B><FONT COLOR="#A020F0">in</FONT></B> xrange(10000):
    key = <B><FONT COLOR="#BC8F8F">'foo_%d'</FONT></B> % i
    hash1[key] = i
    hash2[key] = 0

<B><FONT COLOR="#A020F0">for</FONT></B> i <B><FONT COLOR="#A020F0">in</FONT></B> xrange(n):
    <B><FONT COLOR="#A020F0">for</FONT></B> key <B><FONT COLOR="#A020F0">in</FONT></B> hash1.keys():
        hash2[key] += hash1[key]

<B><FONT COLOR="#A020F0">print</FONT></B> hash1[<B><FONT COLOR="#BC8F8F">'foo_1'</FONT></B>], hash1[<B><FONT COLOR="#BC8F8F">'foo_9999'</FONT></B>], hash2[<B><FONT COLOR="#BC8F8F">'foo_1'</FONT></B>], hash2[<B><FONT COLOR="#BC8F8F">'foo_9999'</FONT></B>]</pre></td></tr></table>
