<table width="100%" border="0" cellpadding="5" cellspacing="0"><tr><td bgcolor="#FFFFFF"><pre>
<B><FONT COLOR="#A020F0">import</FONT></B> Control.Concurrent
<B><FONT COLOR="#A020F0">import</FONT></B> Control.Monad
<B><FONT COLOR="#A020F0">import</FONT></B> System

procinst v = forkIO (takeMVar v)

main = do [num] &lt;- getArgs
          v &lt;- newEmptyMVar
	  replicateM_ (read num) (procinst v)
	  putStrLn num</pre></td></tr></table>
