<B><FONT COLOR="#A020F0">import</FONT></B> sys
<B><FONT COLOR="#A020F0">from</FONT></B> numarray <B><FONT COLOR="#A020F0">import</FONT></B> *

size = 30

<B><FONT COLOR="#A020F0">def</FONT></B> <B><FONT COLOR="#0000FF">mkmatrix</FONT></B>(rows,cols):
    m = array([[i+(j-1)*rows <B><FONT COLOR="#A020F0">for</FONT></B> i <B><FONT COLOR="#A020F0">in</FONT></B> xrange(1,rows+1)] <B><FONT COLOR="#A020F0">for</FONT></B> j <B><FONT COLOR="#A020F0">in</FONT></B> xrange(1,cols+1)])
    <B><FONT COLOR="#A020F0">return</FONT></B> m


<B><FONT COLOR="#A020F0">def</FONT></B> <B><FONT COLOR="#0000FF">mmult</FONT></B>(m1,m2):
    <B><FONT COLOR="#A020F0">return</FONT></B> dot(m1,m2)


<B><FONT COLOR="#A020F0">def</FONT></B> <B><FONT COLOR="#0000FF">main</FONT></B>():
    <B><FONT COLOR="#A020F0">try</FONT></B>: iter = int(sys.argv[1])
    <B><FONT COLOR="#A020F0">except</FONT></B>: iter = 1

    m1 = mkmatrix(size,size)
    m2 = mkmatrix(size,size)
    <B><FONT COLOR="#A020F0">for</FONT></B> i <B><FONT COLOR="#A020F0">in</FONT></B> xrange(iter):
        mm = mmult(m1,m2)
    <B><FONT COLOR="#A020F0">print</FONT></B> mm[0,0], mm[2,3], mm[3,2], mm[4,4]

main()
