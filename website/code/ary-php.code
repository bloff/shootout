$n = ($argc == 2) ? $argv[1] : 1;

<B><FONT COLOR="#A020F0">for</FONT></B> ($i=0; $i&lt;$n; $i++) $x[$i] = $i + 1;
    
$y = array_pad(array(),$n,0);
<B><FONT COLOR="#A020F0">for</FONT></B> ($k=0; $k&lt;1000; $k++){
   $j = $n;
   <B><FONT COLOR="#A020F0">while</FONT></B> ($j--) $y[$j] += $x[$j];
}

<B><FONT COLOR="#A020F0">printf</FONT></B>(<B><FONT COLOR="#BC8F8F">&quot;%d %d\n&quot;</FONT></B>, $y[0], $y[$n-1]);

