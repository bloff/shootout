<span class="line">    1 </span><span class="slc">-- $Id: moments-lua.code,v 1.4 2004-11-13 07:42:19 bfulgham Exp $
</span><span class="line">    2 </span><span class="slc"></span><span class="slc">-- http://www.bagley.org/~doug/shootout/
</span><span class="line">    3 </span><span class="slc"></span><span class="slc">-- implemented by: Roberto Ierusalimschy
</span><span class="line">    4 </span><span class="slc"></span>
<span class="line">    5 </span><span class="kwa">local</span> nums <span class="sym">= {}
</span><span class="line">    6 </span><span class="sym"></span><span class="kwa">local</span> n <span class="sym">=</span> <span class="num">0
</span><span class="line">    7 </span><span class="num"></span><span class="kwa">local</span> sum <span class="sym">=</span> <span class="num">0
</span><span class="line">    8 </span><span class="num"></span><span class="kwa">while</span> <span class="num">1</span> <span class="kwa">do
</span><span class="line">    9 </span><span class="kwa">  local</span> line <span class="sym">=</span> io.<span class="kwa">read</span><span class="sym">()
</span><span class="line">   10 </span><span class="sym"></span>  <span class="kwa">if</span> line <span class="sym">==</span> <span class="kwa">nil then break end
</span><span class="line">   11 </span><span class="kwa"></span>  line <span class="sym">=</span> line<span class="sym">+</span><span class="num">0</span>        <span class="slc">-- convert line to number
</span><span class="line">   12 </span><span class="slc"></span>  sum <span class="sym">=</span> sum <span class="sym">+</span> line
<span class="line">   13 </span>  n <span class="sym">=</span> n <span class="sym">+</span> <span class="num">1
</span><span class="line">   14 </span><span class="num"></span>  nums<span class="sym">[</span>n<span class="sym">] =</span> line
<span class="line">   15 </span><span class="kwa">end
</span><span class="line">   16 </span><span class="kwa">
</span><span class="line">   17 </span><span class="kwa">local</span> mean <span class="sym">=</span> sum<span class="sym">/</span>n
<span class="line">   18 </span>
<span class="line">   19 </span><span class="kwa">local</span> average_deviation<span class="sym">,</span> variance<span class="sym">,</span> skew<span class="sym">,</span> kurtosis <span class="sym">=</span> <span class="num">0</span><span class="sym">,</span> <span class="num">0</span><span class="sym">,</span> <span class="num">0</span><span class="sym">,</span> <span class="num">0
</span><span class="line">   20 </span><span class="num">
</span><span class="line">   21 </span><span class="num"></span><span class="kwa">for</span> i <span class="sym">=</span> <span class="num">1</span><span class="sym">,</span> n <span class="kwa">do
</span><span class="line">   22 </span><span class="kwa">  local</span> deviation <span class="sym">=</span> nums<span class="sym">[</span>i<span class="sym">] -</span> mean
<span class="line">   23 </span>  average_deviation <span class="sym">=</span> average_deviation <span class="sym">+</span> math.<span class="kwa">abs</span><span class="sym">(</span>deviation<span class="sym">)
</span><span class="line">   24 </span><span class="sym"></span>  variance <span class="sym">=</span> variance <span class="sym">+</span> deviation^<span class="num">2
</span><span class="line">   25 </span><span class="num"></span>  skew <span class="sym">=</span> skew <span class="sym">+</span> deviation^<span class="num">3
</span><span class="line">   26 </span><span class="num"></span>  kurtosis <span class="sym">=</span> kurtosis <span class="sym">+</span> deviation^<span class="num">4
</span><span class="line">   27 </span><span class="num"></span><span class="kwa">end
</span><span class="line">   28 </span><span class="kwa">
</span><span class="line">   29 </span><span class="kwa"></span>average_deviation <span class="sym">=</span> average_deviation<span class="sym">/</span>n
<span class="line">   30 </span>variance <span class="sym">=</span> variance<span class="sym">/(</span>n<span class="num">-1</span><span class="sym">)
</span><span class="line">   31 </span><span class="sym"></span><span class="kwa">local</span> standard_deviation <span class="sym">=</span> math.<span class="kwa">sqrt</span><span class="sym">(</span>variance<span class="sym">)
</span><span class="line">   32 </span><span class="sym"></span><span class="kwa">if</span> variance ~<span class="sym">=</span> <span class="num">0</span> <span class="kwa">then
</span><span class="line">   33 </span><span class="kwa"></span>  skew <span class="sym">=</span> skew <span class="sym">/ (</span>n <span class="sym">*</span> variance <span class="sym">*</span> standard_deviation<span class="sym">)
</span><span class="line">   34 </span><span class="sym"></span>  kurtosis <span class="sym">=</span> kurtosis<span class="sym">/(</span>n <span class="sym">*</span> variance <span class="sym">*</span> variance<span class="sym">) -</span> <span class="num">3.0
</span><span class="line">   35 </span><span class="num"></span><span class="kwa">end
</span><span class="line">   36 </span><span class="kwa">
</span><span class="line">   37 </span><span class="kwa"></span><span class="kwb">table</span>.<span class="kwa">sort</span><span class="sym">(</span>nums<span class="sym">)
</span><span class="line">   38 </span><span class="sym"></span><span class="kwa">local</span> mid <span class="sym">=</span> math.<span class="kwa">floor</span><span class="sym">(</span>n<span class="sym">/</span><span class="num">2</span><span class="sym">)
</span><span class="line">   39 </span><span class="sym"></span><span class="kwa">local</span> median
<span class="line">   40 </span><span class="kwa">if</span> math.<span class="kwa">mod</span><span class="sym">(</span>n<span class="sym">,</span><span class="num">2</span><span class="sym">) ==</span> <span class="num">1</span> <span class="kwa">then
</span><span class="line">   41 </span><span class="kwa"></span>  median <span class="sym">=</span> nums<span class="sym">[</span>mid<span class="sym">+</span><span class="num">1</span><span class="sym">]
</span><span class="line">   42 </span><span class="sym"></span><span class="kwa">else
</span><span class="line">   43 </span><span class="kwa"></span>  median <span class="sym">= (</span>nums<span class="sym">[</span>mid<span class="sym">] +</span> nums<span class="sym">[</span>mid<span class="sym">+</span><span class="num">1</span><span class="sym">])/</span><span class="num">2
</span><span class="line">   44 </span><span class="num"></span><span class="kwa">end
</span><span class="line">   45 </span><span class="kwa">
</span><span class="line">   46 </span><span class="kwa"></span>io.<span class="kwa">write</span><span class="sym">(</span><span class="kwb">string</span>.<span class="kwa">format</span><span class="sym">(</span><span class="str">&quot;n:                  %d</span><span class="esc">\n</span><span class="str">&quot;</span><span class="sym">,</span> n<span class="sym">))
</span><span class="line">   47 </span><span class="sym"></span>io.<span class="kwa">write</span><span class="sym">(</span><span class="kwb">string</span>.<span class="kwa">format</span><span class="sym">(</span><span class="str">&quot;median:             %f</span><span class="esc">\n</span><span class="str">&quot;</span><span class="sym">,</span> median<span class="sym">))
</span><span class="line">   48 </span><span class="sym"></span>io.<span class="kwa">write</span><span class="sym">(</span><span class="kwb">string</span>.<span class="kwa">format</span><span class="sym">(</span><span class="str">&quot;mean:               %f</span><span class="esc">\n</span><span class="str">&quot;</span><span class="sym">,</span> mean<span class="sym">))
</span><span class="line">   49 </span><span class="sym"></span>io.<span class="kwa">write</span><span class="sym">(</span><span class="kwb">string</span>.<span class="kwa">format</span><span class="sym">(</span><span class="str">&quot;average_deviation:  %f</span><span class="esc">\n</span><span class="str">&quot;</span><span class="sym">,</span> average_deviation<span class="sym">))
</span><span class="line">   50 </span><span class="sym"></span>io.<span class="kwa">write</span><span class="sym">(</span><span class="kwb">string</span>.<span class="kwa">format</span><span class="sym">(</span><span class="str">&quot;standard_deviation: %f</span><span class="esc">\n</span><span class="str">&quot;</span><span class="sym">,</span> standard_deviation<span class="sym">))
</span><span class="line">   51 </span><span class="sym"></span>io.<span class="kwa">write</span><span class="sym">(</span><span class="kwb">string</span>.<span class="kwa">format</span><span class="sym">(</span><span class="str">&quot;variance:           %f</span><span class="esc">\n</span><span class="str">&quot;</span><span class="sym">,</span> variance<span class="sym">))
</span><span class="line">   52 </span><span class="sym"></span>io.<span class="kwa">write</span><span class="sym">(</span><span class="kwb">string</span>.<span class="kwa">format</span><span class="sym">(</span><span class="str">&quot;skew:               %f</span><span class="esc">\n</span><span class="str">&quot;</span><span class="sym">,</span> skew<span class="sym">))
</span><span class="line">   53 </span><span class="sym"></span>io.<span class="kwa">write</span><span class="sym">(</span><span class="kwb">string</span>.<span class="kwa">format</span><span class="sym">(</span><span class="str">&quot;kurtosis:           %f</span><span class="esc">\n</span><span class="str">&quot;</span><span class="sym">,</span> kurtosis<span class="sym">))</span>
