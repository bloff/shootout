</FONT></I>include <B><FONT COLOR="#BC8F8F">&quot;std&quot;</FONT></B>;
n := atoi(System::argv 1);
include <B><FONT COLOR="#BC8F8F">&quot;stl&quot;</FONT></B>;
open Stl;
open Stl::HashMap;

requires ext_string_hash;

body hexer <B><FONT COLOR="#BC8F8F">&quot;&quot;</FONT></B><B><FONT COLOR="#BC8F8F">&quot;
string hex(int x) { char buf[20]; sprintf(buf,&quot;</FONT></B>%x<B><FONT COLOR="#BC8F8F">&quot;,x); return buf; }
&quot;</FONT></B><B><FONT COLOR="#BC8F8F">&quot;&quot;</FONT></B>;

fun hex: <B><FONT COLOR="#228B22">int</FONT></B> -&gt; string requires hexer;

var hm = HashMap::create[string,<B><FONT COLOR="#228B22">int</FONT></B>]();

var i = n; until i == 0 <B><FONT COLOR="#A020F0">do</FONT></B>
  insert(hm,hex i,i);
  --i;
done;

var c = 0;
i = n; until i == 0 <B><FONT COLOR="#A020F0">do</FONT></B>
  <B><FONT COLOR="#A020F0">if</FONT></B> mem (hm,str i) <B><FONT COLOR="#A020F0">do</FONT></B> c++; done;
  --i;
done;

print c; endl;

</pre></td></tr></table>
