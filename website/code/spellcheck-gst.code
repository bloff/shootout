| dict stream newWord |
dict := Set <B><FONT COLOR="#A020F0">new</FONT></B>: 4096.
stream := (File name: <B><FONT COLOR="#BC8F8F">'Usr.Dict.Words'</FONT></B>) readStream.
[stream atEnd] whileFalse: [dict add: stream nextLine].
stream <B><FONT COLOR="#A020F0">close</FONT></B>.

stream := FileStream stdin bufferSize: 4096.
[stream atEnd] whileFalse: [
   (dict includes: (newWord := stream nextLine)) 
      ifFalse: [Transcript show: newWord; nl] ] !</pre></td></tr></table>
