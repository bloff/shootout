<span class="line">    1 </span><span class="slc">%% The Great Computer Language Shootout 
</span><span class="line">    2 </span><span class="slc"></span><span class="slc">%% contributed by Bengt Kleberg (Erlang Guru) 
</span><span class="line">    3 </span><span class="slc"></span><span class="slc">%% 
</span><span class="line">    4 </span><span class="slc"></span><span class="slc">%% http://shootout.alioth.debian.org/ 
</span><span class="line">    5 </span><span class="slc"></span><span class="slc">%% 
</span><span class="line">    6 </span><span class="slc"></span><span class="slc">%% Usage: start from command line with 
</span><span class="line">    7 </span><span class="slc"></span><span class="slc">%%     erlc moments.erl 
</span><span class="line">    8 </span><span class="slc"></span><span class="slc">%%     erl -noinput -s moments main &lt; InputFile 
</span><span class="line">    9 </span><span class="slc"></span>
<span class="line">   10 </span> <span class="sym">-</span>module<span class="sym">(</span>moments<span class="sym">)</span>.
<span class="line">   11 </span>  <span class="sym">-</span>export<span class="sym">([</span>main<span class="sym">/</span><span class="num">0</span><span class="sym">,</span> main<span class="sym">/</span><span class="num">1</span><span class="sym">])</span>.
<span class="line">   12 </span>
<span class="line">   13 </span>   <span class="slc">%% ignore program argument 
</span><span class="line">   14 </span><span class="slc"></span>main<span class="sym">() -&gt;</span> main<span class="sym">([</span><span class="str">'1'</span><span class="sym">])</span>.
<span class="line">   15 </span> main<span class="sym">(</span>_Args<span class="sym">) -&gt;
</span><span class="line">   16 </span><span class="sym"> </span>    Numbers <span class="sym">=</span> input_floats<span class="sym">(),
</span><span class="line">   17 </span><span class="sym"> </span>    How_many <span class="sym">=</span> erlang<span class="sym">:</span><span class="kwb">length</span><span class="sym">(</span> Numbers <span class="sym">),
</span><span class="line">   18 </span><span class="sym"> </span>    Median <span class="sym">=</span> median<span class="sym">(</span> Numbers<span class="sym">,</span> How_many <span class="sym">),
</span><span class="line">   19 </span><span class="sym"> </span>    Mean <span class="sym">=</span> lists<span class="sym">:</span>sum<span class="sym">(</span> Numbers <span class="sym">) /</span> How_many<span class="sym">,
</span><span class="line">   20 </span><span class="sym">     {</span>Ad<span class="sym">,</span> Av<span class="sym">,</span> As<span class="sym">,</span> Ak<span class="sym">} =</span> various_averages<span class="sym">(</span> Numbers<span class="sym">,</span> Mean<span class="sym">),
</span><span class="line">   21 </span><span class="sym"> </span>    Average_deviation <span class="sym">=</span> Ad <span class="sym">/</span> How_many<span class="sym">,
</span><span class="line">   22 </span><span class="sym"> </span>    Variance <span class="sym">=</span> Av <span class="sym">/ (</span>How_many <span class="num">- 1</span><span class="sym">),
</span><span class="line">   23 </span><span class="sym"> </span>    Standard_deviation <span class="sym">=</span> <span class="kwb">math</span><span class="sym">:</span><span class="kwb">sqrt</span><span class="sym">(</span>Variance<span class="sym">),
</span><span class="line">   24 </span><span class="sym"> </span>    Skew <span class="sym">=</span> skew<span class="sym">(</span>How_many<span class="sym">,</span> As<span class="sym">,</span> Variance<span class="sym">,</span> Standard_deviation<span class="sym">),
</span><span class="line">   25 </span><span class="sym"> </span>    Kurtosis <span class="sym">=</span> kurtosis<span class="sym">(</span>How_many<span class="sym">,</span> Ak<span class="sym">,</span> Variance<span class="sym">),
</span><span class="line">   26 </span><span class="sym"> </span>    print<span class="sym">(</span>How_many<span class="sym">,</span> Median<span class="sym">,</span> Mean<span class="sym">,</span> Average_deviation<span class="sym">,</span> Standard_deviation<span class="sym">,
</span><span class="line">   27 </span><span class="sym"> </span>            Variance<span class="sym">,</span> Skew<span class="sym">,</span> Kurtosis<span class="sym">),
</span><span class="line">   28 </span><span class="sym"> </span>    <span class="kwb">halt</span><span class="sym">()</span>.
<span class="line">   29 </span>
<span class="line">   30 </span>  input_floats<span class="sym">() -&gt;</span> <span class="slc">% &quot;list of (double precision floating point) numbers&quot;? no,integers. 
</span><span class="line">   31 </span><span class="slc"></span>    input_floats<span class="sym">(</span>io<span class="sym">:</span>fread<span class="sym">(</span><span class="str">''</span><span class="sym">,</span> <span class="str">&quot;~d&quot;</span><span class="sym">), [])</span>.
<span class="line">   32 </span> input_floats<span class="sym">(</span>eof<span class="sym">,</span> Numbers<span class="sym">) -&gt;
</span><span class="line">   33 </span><span class="sym"> </span>    Numbers<span class="sym">;
</span><span class="line">   34 </span><span class="sym"> </span>input_floats<span class="sym">({</span>ok<span class="sym">, [</span>Float<span class="sym">]},</span> Numbers<span class="sym">) -&gt;
</span><span class="line">   35 </span><span class="sym"> </span>    input_floats<span class="sym">(</span> io<span class="sym">:</span>fread<span class="sym">(</span><span class="str">''</span><span class="sym">,</span> <span class="str">&quot;~d&quot;</span><span class="sym">), [</span>Float<span class="sym">|</span>Numbers<span class="sym">] )</span>.
<span class="line">   36 </span>
<span class="line">   37 </span>  various_averages<span class="sym">(</span> Numbers<span class="sym">,</span> Mean <span class="sym">) -&gt;
</span><span class="line">   38 </span><span class="sym"> </span>    Fun <span class="sym">=</span> <span class="kwa">fun</span><span class="sym">(</span> Float<span class="sym">, {</span>Ad<span class="sym">,</span> Av<span class="sym">,</span> As<span class="sym">,</span> Ak<span class="sym">} ) -&gt;
</span><span class="line">   39 </span><span class="sym"> </span>            D <span class="sym">=</span> Float <span class="sym">-</span> Mean<span class="sym">,
</span><span class="line">   40 </span><span class="sym"> </span>            D2 <span class="sym">=</span> D <span class="sym">*</span> D<span class="sym">,
</span><span class="line">   41 </span><span class="sym">             {</span>Ad <span class="sym">+</span> erlang<span class="sym">:</span><span class="kwb">abs</span><span class="sym">(</span>D<span class="sym">),</span> Av <span class="sym">+</span> D2<span class="sym">,</span> As <span class="sym">+ (</span>D2 <span class="sym">*</span> D<span class="sym">),</span> Ak <span class="sym">+ (</span>D2 <span class="sym">*</span> D2<span class="sym">)}
</span><span class="line">   42 </span><span class="sym"> </span>          <span class="kwa">end</span><span class="sym">,
</span><span class="line">   43 </span><span class="sym"> </span>    lists<span class="sym">:</span>foldl<span class="sym">(</span> Fun<span class="sym">, {</span><span class="num">0.0</span><span class="sym">,</span> <span class="num">0.0</span><span class="sym">,</span> <span class="num">0.0</span><span class="sym">,</span> <span class="num">0.0</span><span class="sym">},</span> Numbers <span class="sym">)</span>.
<span class="line">   44 </span>
<span class="line">   45 </span>  median<span class="sym">(</span>L<span class="sym">,</span> N<span class="sym">) -&gt;</span> medianS<span class="sym">(</span>lists<span class="sym">:</span>sort<span class="sym">(</span>L<span class="sym">),</span> N<span class="sym">)</span>.
<span class="line">   46 </span>
<span class="line">   47 </span>  medianS<span class="sym">(</span>L<span class="sym">,</span> N<span class="sym">) -&gt;
</span><span class="line">   48 </span><span class="sym"> </span>    Mid <span class="sym">= (</span>N div <span class="num">2</span><span class="sym">) +</span> <span class="num">1</span><span class="sym">,</span> <span class="slc">% 1-based list access 
</span><span class="line">   49 </span><span class="slc"></span>    <span class="kwa">case</span> N rem <span class="num">2</span> <span class="kwa">of
</span><span class="line">   50 </span><span class="kwa"> </span>        <span class="num">0</span> <span class="sym">-&gt; (</span>lists<span class="sym">:</span>nth<span class="sym">(</span>Mid<span class="sym">,</span> L<span class="sym">) +</span> lists<span class="sym">:</span>nth<span class="sym">(</span>Mid <span class="num">- 1</span><span class="sym">,</span> L<span class="sym">)) /</span> <span class="num">2</span><span class="sym">;
</span><span class="line">   51 </span><span class="sym"> </span>        <span class="num">1</span> <span class="sym">-&gt;</span> lists<span class="sym">:</span>nth<span class="sym">(</span>Mid<span class="sym">,</span> L<span class="sym">)
</span><span class="line">   52 </span><span class="sym"> </span>    <span class="kwa">end</span>.
<span class="line">   53 </span>
<span class="line">   54 </span>  skew<span class="sym">(</span>N<span class="sym">,</span> As<span class="sym">,</span> V<span class="sym">,</span> SD<span class="sym">)</span> <span class="kwa">when</span> V <span class="sym">&gt;</span> <span class="num">0.0</span> <span class="sym">-&gt;</span> As <span class="sym">/ (</span>N <span class="sym">*</span> V <span class="sym">*</span> SD<span class="sym">);
</span><span class="line">   55 </span><span class="sym"> </span>skew<span class="sym">(</span>_N<span class="sym">,</span> _As<span class="sym">,</span> _V<span class="sym">,</span> _SD<span class="sym">) -&gt;</span> <span class="num">0</span>.
<span class="line">   56 </span>
<span class="line">   57 </span>  kurtosis<span class="sym">(</span>N<span class="sym">,</span> Ak<span class="sym">,</span> V<span class="sym">)</span> <span class="kwa">when</span> V <span class="sym">&gt;</span> <span class="num">0.0</span> <span class="sym">-&gt;</span> Ak <span class="sym">/ (</span>N <span class="sym">*</span> V <span class="sym">*</span> V<span class="sym">) -</span> <span class="num">3</span><span class="sym">;
</span><span class="line">   58 </span><span class="sym"> </span>kurtosis<span class="sym">(</span>_N<span class="sym">,</span> _Ak<span class="sym">,</span> _V<span class="sym">) -&gt;</span> <span class="num">0</span>.
<span class="line">   59 </span>
<span class="line">   60 </span>  print<span class="sym">(</span>How_many<span class="sym">,</span> Median<span class="sym">,</span> Mean<span class="sym">,</span> Average_deviation<span class="sym">,</span> Standard_deviation<span class="sym">,
</span><span class="line">   61 </span><span class="sym"> </span>        Variance<span class="sym">,</span> Skew<span class="sym">,</span> Kurtosis<span class="sym">) -&gt;
</span><span class="line">   62 </span><span class="sym"> </span>    io<span class="sym">:</span>fwrite<span class="sym">(</span><span class="str">&quot;n:                  ~w~n&quot;</span><span class="sym">, [</span>How_many<span class="sym">]),
</span><span class="line">   63 </span><span class="sym">  </span>    io<span class="sym">:</span>fwrite<span class="sym">(</span><span class="str">&quot;median:             ~f~n&quot;</span><span class="sym">, [</span>Median<span class="sym">]),
</span><span class="line">   64 </span><span class="sym">  </span>    io<span class="sym">:</span>fwrite<span class="sym">(</span><span class="str">&quot;mean:               ~f~n&quot;</span><span class="sym">, [</span>Mean<span class="sym">]),
</span><span class="line">   65 </span><span class="sym">  </span>    io<span class="sym">:</span>fwrite<span class="sym">(</span><span class="str">&quot;average_deviation:  ~f~n&quot;</span><span class="sym">, [</span>Average_deviation<span class="sym">]),
</span><span class="line">   66 </span><span class="sym">  </span>    io<span class="sym">:</span>fwrite<span class="sym">(</span><span class="str">&quot;standard_deviation: ~f~n&quot;</span><span class="sym">, [</span>Standard_deviation<span class="sym">]),
</span><span class="line">   67 </span><span class="sym">  </span>    io<span class="sym">:</span>fwrite<span class="sym">(</span><span class="str">&quot;variance:           ~f~n&quot;</span><span class="sym">, [</span>Variance<span class="sym">]),
</span><span class="line">   68 </span><span class="sym">  </span>    io<span class="sym">:</span>fwrite<span class="sym">(</span><span class="str">&quot;skew:               ~f~n&quot;</span><span class="sym">, [</span>Skew<span class="sym">]),
</span><span class="line">   69 </span><span class="sym">  </span>    io<span class="sym">:</span>fwrite<span class="sym">(</span><span class="str">&quot;kurtosis:           ~f~n&quot;</span><span class="sym">, [</span>Kurtosis<span class="sym">])</span>.  
