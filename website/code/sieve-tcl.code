<table width="100%" border="0" cellpadding="5" cellspacing="0"><tr><td bgcolor="#FFFFFF"><pre>
<I><FONT COLOR="#B22222">#!/usr/bin/tclsh
</FONT></I><I><FONT COLOR="#B22222"># $Id: sieve-tcl.code,v 1.1 2004-11-02 08:53:27 bfulgham Exp $
</FONT></I><I><FONT COLOR="#B22222"># http://www.bagley.org/~doug/shootout/
</FONT></I><I><FONT COLOR="#B22222"># with help from: Kristoffer Lawson
</FONT></I>
<B><FONT COLOR="#A020F0">proc</FONT></B> <B><FONT COLOR="#0000FF">sieve</FONT></B> {<FONT COLOR="#B8860B">num</FONT>} {
    <B><FONT COLOR="#A020F0">while</FONT></B> {$<FONT COLOR="#B8860B">num</FONT> &gt; 0} {
	<B><FONT COLOR="#A020F0">incr</FONT></B> num -1
	<B><FONT COLOR="#A020F0">set</FONT></B> count 0
	<B><FONT COLOR="#A020F0">for</FONT></B> {<B><FONT COLOR="#A020F0">set</FONT></B> i 2} {$<FONT COLOR="#B8860B">i</FONT> &lt;= 8192} {<B><FONT COLOR="#A020F0">incr</FONT></B> i 1} {
	    <B><FONT COLOR="#A020F0">set</FONT></B> flags($<FONT COLOR="#B8860B">i</FONT>) 1
	}
	<B><FONT COLOR="#A020F0">for</FONT></B> {<B><FONT COLOR="#A020F0">set</FONT></B> i 2} {$<FONT COLOR="#B8860B">i</FONT> &lt;= 8192} {<B><FONT COLOR="#A020F0">incr</FONT></B> i 1} {
	    <B><FONT COLOR="#A020F0">if</FONT></B> {$<FONT COLOR="#B8860B">flags</FONT>($<FONT COLOR="#B8860B">i</FONT>) == 1} {
		<I><FONT COLOR="#B22222"># remove all multiples of prime: i
</FONT></I>		<B><FONT COLOR="#A020F0">for</FONT></B> {<B><FONT COLOR="#A020F0">set</FONT></B> k [<B><FONT COLOR="#A020F0">expr</FONT></B> {$<FONT COLOR="#B8860B">i</FONT>+$<FONT COLOR="#B8860B">i</FONT>}]} {$<FONT COLOR="#B8860B">k</FONT> &lt;= 8192} {<B><FONT COLOR="#A020F0">incr</FONT></B> k $<FONT COLOR="#B8860B">i</FONT>} {
		    <B><FONT COLOR="#A020F0">set</FONT></B> flags($<FONT COLOR="#B8860B">k</FONT>) 0
		}
		<B><FONT COLOR="#A020F0">incr</FONT></B> count 1
	    }
	}
    }
    <B><FONT COLOR="#A020F0">return</FONT></B> $<FONT COLOR="#B8860B">count</FONT>
}

<B><FONT COLOR="#A020F0">set</FONT></B> NUM [<B><FONT COLOR="#A020F0">lindex</FONT></B> $<FONT COLOR="#B8860B">argv</FONT> 0]
<B><FONT COLOR="#A020F0">if</FONT></B> {$<FONT COLOR="#B8860B">NUM</FONT> &lt; 1} {
    <B><FONT COLOR="#A020F0">set</FONT></B> NUM 1
}

<B><FONT COLOR="#A020F0">set</FONT></B> count [sieve $<FONT COLOR="#B8860B">NUM</FONT>]
<B><FONT COLOR="#A020F0">puts</FONT></B> <B><FONT COLOR="#BC8F8F">&quot;Count: $count&quot;</FONT></B></pre></td></tr></table>
