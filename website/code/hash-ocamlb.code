<B><FONT COLOR="#A020F0">let</FONT></B> hexdigits <B><FONT COLOR="#5F9EA0">=</FONT></B>  [<B><FONT COLOR="#5F9EA0">|</FONT></B> <B><FONT COLOR="#BC8F8F">'0'</FONT></B>; <B><FONT COLOR="#BC8F8F">'1'</FONT></B>; <B><FONT COLOR="#BC8F8F">'2'</FONT></B>; <B><FONT COLOR="#BC8F8F">'3'</FONT></B>; <B><FONT COLOR="#BC8F8F">'4'</FONT></B>; <B><FONT COLOR="#BC8F8F">'5'</FONT></B>; <B><FONT COLOR="#BC8F8F">'6'</FONT></B>; <B><FONT COLOR="#BC8F8F">'7'</FONT></B>; <B><FONT COLOR="#BC8F8F">'8'</FONT></B>; <B><FONT COLOR="#BC8F8F">'9'</FONT></B>;
                    <B><FONT COLOR="#BC8F8F">'a'</FONT></B>; <B><FONT COLOR="#BC8F8F">'b'</FONT></B>; <B><FONT COLOR="#BC8F8F">'c'</FONT></B>; <B><FONT COLOR="#BC8F8F">'d'</FONT></B>; <B><FONT COLOR="#BC8F8F">'e'</FONT></B>; <B><FONT COLOR="#BC8F8F">'f'</FONT></B>; <B><FONT COLOR="#5F9EA0">|</FONT></B>]

<B><FONT COLOR="#A020F0">let</FONT></B> buf <B><FONT COLOR="#5F9EA0">=</FONT></B> String.create 32

<B><FONT COLOR="#A020F0">let</FONT></B> <B><FONT COLOR="#A020F0">rec</FONT></B> hexstring_of_int idx len <B><FONT COLOR="#5F9EA0">=</FONT></B> function
  <B><FONT COLOR="#5F9EA0">|</FONT></B> n when n <B><FONT COLOR="#5F9EA0">&lt;=</FONT></B> 0 <B><FONT COLOR="#5F9EA0">-</FONT></B><B><FONT COLOR="#5F9EA0">&gt;</FONT></B> String.sub buf idx len
  <B><FONT COLOR="#5F9EA0">|</FONT></B> n <B><FONT COLOR="#5F9EA0">-</FONT></B><B><FONT COLOR="#5F9EA0">&gt;</FONT></B>
      <B><FONT COLOR="#A020F0">let</FONT></B> new_idx <B><FONT COLOR="#5F9EA0">=</FONT></B> idx <B><FONT COLOR="#5F9EA0">-</FONT></B> 1 <B><FONT COLOR="#A020F0">in</FONT></B>
      buf.[new_idx] &lt;<B><FONT COLOR="#5F9EA0">-</FONT></B> hexdigits.(n land 15);
      hexstring_of_int new_idx (len <B><FONT COLOR="#5F9EA0">+</FONT></B> 1) (n lsr 4);;

<B><FONT COLOR="#A020F0">let</FONT></B> n <B><FONT COLOR="#5F9EA0">=</FONT></B> <B><FONT COLOR="#A020F0">if</FONT></B> Array.length Sys.argv <B><FONT COLOR="#5F9EA0">&gt;</FONT></B> 1 <B><FONT COLOR="#A020F0">then</FONT></B> int_of_string Sys.argv.(1) <B><FONT COLOR="#A020F0">else</FONT></B> 1 <B><FONT COLOR="#A020F0">in</FONT></B>
<B><FONT COLOR="#A020F0">let</FONT></B> hx <B><FONT COLOR="#5F9EA0">=</FONT></B> Hashtbl.create n <B><FONT COLOR="#A020F0">and</FONT></B> c <B><FONT COLOR="#5F9EA0">=</FONT></B> <B><FONT COLOR="#A020F0">ref</FONT></B> 0 <B><FONT COLOR="#A020F0">in</FONT></B>
for i <B><FONT COLOR="#5F9EA0">=</FONT></B> 1 to n <B><FONT COLOR="#A020F0">do</FONT></B> Hashtbl.add hx (hexstring_of_int 32 0 i) true done;
for i <B><FONT COLOR="#5F9EA0">=</FONT></B> n downto 1 <B><FONT COLOR="#A020F0">do</FONT></B> <B><FONT COLOR="#A020F0">if</FONT></B> Hashtbl.mem hx (string_of_int i) <B><FONT COLOR="#A020F0">then</FONT></B> incr c done;
Printf.printf <B><FONT COLOR="#BC8F8F">&quot;%d\n&quot;</FONT></B> <B><FONT COLOR="#5F9EA0">!</FONT></B>c</pre></td></tr></table>
