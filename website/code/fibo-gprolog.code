%%% -*- mode: prolog -*-
%%% $Id: fibo-gprolog.code,v 1.2 2004-11-08 08:15:13 bfulgham Exp $
%%% http:<I><FONT COLOR="#B22222">//shootout.alioth.debian.org/
</FONT></I>
fibo(0,1).
fibo(1,1).
fibo(N,Val) :- N &gt; 0, N1 <B><FONT COLOR="#A020F0">is</FONT></B> N - 1, fibo(N1,Val1),
		N2 <B><FONT COLOR="#A020F0">is</FONT></B> N - 2, fibo(N2,Val2),
		Val <B><FONT COLOR="#A020F0">is</FONT></B> Val1 + Val2.

main :-
	argument_value(1, ACount),
	number_atom(Count, ACount),
	fibo(Count,Val),
	<B><FONT COLOR="#A020F0">write</FONT></B>(Val), nl, halt.

:- <B><FONT COLOR="#A020F0">initialization</FONT></B>(main).</pre></td></tr></table>
