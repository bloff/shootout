(declare (fixnum) (disable-interrupts))

(<B><FONT COLOR="#A020F0">define</FONT></B> (<B><FONT COLOR="#0000FF">main</FONT></B>)
  (<B><FONT COLOR="#A020F0">let</FONT></B> ((sum 0))
    (<B><FONT COLOR="#A020F0">do</FONT></B> ((myInt (read-line) (read-line)))
	((eof-object? myInt) (print sum))
      (<B><FONT COLOR="#A020F0">set!</FONT></B> sum (+ sum (string-&gt;number myInt))))))

(main)
