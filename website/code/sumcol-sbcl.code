(defun main ()
  <I><FONT COLOR="#B22222">;; fastest compilation mode 
</FONT></I>  (declare (optimize (speed 3) (debug 0) (safety 0)))
  (<B><FONT COLOR="#A020F0">let</FONT></B> ((sum 0))
    (declare (fixnum sum))
    (<B><FONT COLOR="#A020F0">do</FONT></B> ((line (read-line *standard-input*)
	    (read-line *standard-input* nil 'eof)))
	((eq line 'eof) (format t <B><FONT COLOR="#BC8F8F">&quot;~A~%&quot;</FONT></B> sum))
      (incf sum (the fixnum (parse-integer line))))))</pre></td></tr></table>
