<table width="100%" border="0" cellpadding="5" cellspacing="0"><tr><td bgcolor="#FFFFFF"><pre>
<B><FONT COLOR="#5F9EA0">#!/bin/bash
</FONT></B><I><FONT COLOR="#B22222"># $Id: nestedloop-bash.code,v 1.1 2004-11-02 08:52:37 bfulgham Exp $
</FONT></I><I><FONT COLOR="#B22222"># http://shootout.alioth.debian.org/
</FONT></I><I><FONT COLOR="#B22222">#
</FONT></I><I><FONT COLOR="#B22222"># Contributed by David Pyke
</FONT></I>
<B><FONT COLOR="#DA70D6">let</FONT></B> iter=1
[ ${<I><FONT COLOR="#B22222">#1} -gt 0 -a $1 -gt 0 ] &amp;&amp; iter=$1
</FONT></I>
x=0
a=$iter
<B><FONT COLOR="#A020F0">while</FONT></B> [ $a -gt 0 ]; <B><FONT COLOR="#A020F0">do</FONT></B> a=$((a - 1))
    b=$iter
    <B><FONT COLOR="#A020F0">while</FONT></B> [ $b -gt 0 ]; <B><FONT COLOR="#A020F0">do</FONT></B> b=$((b - 1))
	c=$iter
        <B><FONT COLOR="#A020F0">while</FONT></B> [ $c -gt 0 ]; <B><FONT COLOR="#A020F0">do</FONT></B> c=$((c - 1))
	    d=$iter
            <B><FONT COLOR="#A020F0">while</FONT></B> [ $d -gt 0 ]; <B><FONT COLOR="#A020F0">do</FONT></B> d=$((d - 1))
		e=$iter
                <B><FONT COLOR="#A020F0">while</FONT></B> [ $e -gt 0 ]; <B><FONT COLOR="#A020F0">do</FONT></B> e=$((e - 1))
		    f=$iter
                    <B><FONT COLOR="#A020F0">while</FONT></B> [ $f -gt 0 ]; <B><FONT COLOR="#A020F0">do</FONT></B> f=$((f - 1))
                        x=$((x + 1))
                    <B><FONT COLOR="#A020F0">done</FONT></B>
                <B><FONT COLOR="#A020F0">done</FONT></B>
            <B><FONT COLOR="#A020F0">done</FONT></B>
        <B><FONT COLOR="#A020F0">done</FONT></B>
    <B><FONT COLOR="#A020F0">done</FONT></B>
<B><FONT COLOR="#A020F0">done</FONT></B>

<B><FONT COLOR="#DA70D6">echo</FONT></B> $x
</pre></td></tr></table>
