</span><span class="kwa">while</span> <span class="sym">[</span> <span class="kwb">$a</span> <span class="sym">-</span>gt <span class="num">0</span> <span class="sym">];</span> <span class="kwa">do</span> a<span class="sym">=</span><span class="kwb">$</span><span class="sym">((</span>a <span class="num">- 1</span><span class="sym">))
</span>    b<span class="sym">=</span><span class="kwb">$iter
</span>    <span class="kwa">while</span> <span class="sym">[</span> <span class="kwb">$b</span> <span class="sym">-</span>gt <span class="num">0</span> <span class="sym">];</span> <span class="kwa">do</span> b<span class="sym">=</span><span class="kwb">$</span><span class="sym">((</span>b <span class="num">- 1</span><span class="sym">))
</span>	c<span class="sym">=</span><span class="kwb">$iter
</span>        <span class="kwa">while</span> <span class="sym">[</span> <span class="kwb">$c</span> <span class="sym">-</span>gt <span class="num">0</span> <span class="sym">];</span> <span class="kwa">do</span> c<span class="sym">=</span><span class="kwb">$</span><span class="sym">((</span>c <span class="num">- 1</span><span class="sym">))
</span>	    d<span class="sym">=</span><span class="kwb">$iter
</span>            <span class="kwa">while</span> <span class="sym">[</span> <span class="kwb">$d</span> <span class="sym">-</span>gt <span class="num">0</span> <span class="sym">];</span> <span class="kwa">do</span> d<span class="sym">=</span><span class="kwb">$</span><span class="sym">((</span>d <span class="num">- 1</span><span class="sym">))
</span>		e<span class="sym">=</span><span class="kwb">$iter
</span>                <span class="kwa">while</span> <span class="sym">[</span> <span class="kwb">$e</span> <span class="sym">-</span>gt <span class="num">0</span> <span class="sym">];</span> <span class="kwa">do</span> e<span class="sym">=</span><span class="kwb">$</span><span class="sym">((</span>e <span class="num">- 1</span><span class="sym">))
</span>		    f<span class="sym">=</span><span class="kwb">$iter
</span>                    <span class="kwa">while</span> <span class="sym">[</span> <span class="kwb">$f</span> <span class="sym">-</span>gt <span class="num">0</span> <span class="sym">];</span> <span class="kwa">do</span> f<span class="sym">=</span><span class="kwb">$</span><span class="sym">((</span>f <span class="num">- 1</span><span class="sym">))
</span>                        x<span class="sym">=</span><span class="kwb">$</span><span class="sym">((</span>x <span class="sym">+</span> <span class="num">1</span><span class="sym">))
</span>                    <span class="kwa">done
                done
            done
