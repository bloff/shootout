<B><FONT COLOR="#A020F0">proc</FONT></B> <B><FONT COLOR="#0000FF">main</FONT></B> {<FONT COLOR="#B8860B"></FONT>} {
    <B><FONT COLOR="#A020F0">global</FONT></B> argv
    <B><FONT COLOR="#A020F0">set</FONT></B> n [<B><FONT COLOR="#A020F0">lindex</FONT></B> $<FONT COLOR="#B8860B">argv</FONT> 0]
    <B><FONT COLOR="#A020F0">set</FONT></B> <B><FONT COLOR="#A020F0">last</FONT></B> [<B><FONT COLOR="#A020F0">expr</FONT></B> {$<FONT COLOR="#B8860B">n</FONT> - 1}]
    <B><FONT COLOR="#A020F0">for</FONT></B> {<B><FONT COLOR="#A020F0">set</FONT></B> i 0} {$<FONT COLOR="#B8860B">i</FONT> &lt; $<FONT COLOR="#B8860B">n</FONT>} {<B><FONT COLOR="#A020F0">incr</FONT></B> i} {
	<B><FONT COLOR="#A020F0">set</FONT></B> x($<FONT COLOR="#B8860B">i</FONT>) [<B><FONT COLOR="#A020F0">expr</FONT></B> {$<FONT COLOR="#B8860B">i</FONT> + 1}]
	<B><FONT COLOR="#A020F0">set</FONT></B> y($<FONT COLOR="#B8860B">i</FONT>) 0
    }
    <B><FONT COLOR="#A020F0">for</FONT></B> {<B><FONT COLOR="#A020F0">set</FONT></B> k 0} {$<FONT COLOR="#B8860B">k</FONT> &lt; 1000} {<B><FONT COLOR="#A020F0">incr</FONT></B> k} {
	<B><FONT COLOR="#A020F0">for</FONT></B> {<B><FONT COLOR="#A020F0">set</FONT></B> j $<FONT COLOR="#B8860B">last</FONT>} {$<FONT COLOR="#B8860B">j</FONT> &gt;= 0} {<B><FONT COLOR="#A020F0">incr</FONT></B> j -1} {
	    <B><FONT COLOR="#A020F0">incr</FONT></B> y($<FONT COLOR="#B8860B">j</FONT>) $<FONT COLOR="#B8860B">x</FONT>($<FONT COLOR="#B8860B">j</FONT>)
	}
    }
    <B><FONT COLOR="#A020F0">puts</FONT></B> <B><FONT COLOR="#BC8F8F">&quot;$y(0) $y($last)&quot;</FONT></B>
}

main</pre></td></tr></table>
