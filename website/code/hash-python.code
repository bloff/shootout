<span class="kwc">import</span> sys
<span class="slc">#sys.setcheckinterval(10000)</span>

<span class="kwb">def</span> <span class="kwd">main</span><span class="sym">():</span>
    n <span class="sym">=</span> <span class="kwa">int</span><span class="sym">(</span>sys<span class="sym">.</span>argv<span class="sym">[</span><span class="num">1</span><span class="sym">])</span>
    X <span class="sym">= {}</span>
    myhex <span class="sym">=</span> <span class="kwa">hex</span>
    <span class="kwb">for</span> i <span class="kwb">in</span> <span class="kwa">xrange</span><span class="sym">(</span><span class="num">1</span><span class="sym">,</span>n<span class="sym">+</span><span class="num">1</span><span class="sym">):</span>
        X<span class="sym">[</span><span class="kwd">myhex</span><span class="sym">(</span>i<span class="sym">)[</span><span class="num">2</span><span class="sym">:]] =</span> i
    c <span class="sym">=</span> <span class="num">0</span>
    has_key <span class="sym">=</span> X<span class="sym">.</span>has_key
    <span class="kwb">for</span> i <span class="kwb">in</span> <span class="kwa">xrange</span><span class="sym">(</span>n<span class="sym">,</span> <span class="num">0</span><span class="sym">, -</span><span class="num">1</span><span class="sym">):</span>
        c <span class="sym">+=</span> <span class="kwd">has_key</span><span class="sym">(</span>`i`<span class="sym">)</span>
    <span class="kwb">print</span> c

<span class="kwd">main</span><span class="sym">()</span>
