<table width="100%" border="0" cellpadding="5" cellspacing="0"><tr><td bgcolor="#FFFFFF"><pre>
<B><FONT COLOR="#A020F0">program</FONT></B> ackermann;
<B><FONT COLOR="#A020F0">uses</FONT></B> SysUtils;

<B><FONT COLOR="#A020F0">function</FONT></B> Ack(M, N : integer) : integer;
<B><FONT COLOR="#A020F0">begin</FONT></B>	
	<B><FONT COLOR="#A020F0">if</FONT></B> M = 0 <B><FONT COLOR="#A020F0">then</FONT></B> Ack := N+1
	<B><FONT COLOR="#A020F0">else</FONT></B> <B><FONT COLOR="#A020F0">if</FONT></B> N = 0 <B><FONT COLOR="#A020F0">then</FONT></B> Ack := Ack(M-1, 1)
	<B><FONT COLOR="#A020F0">else</FONT></B> Ack := Ack(M-1, Ack(M, N-1))
<B><FONT COLOR="#A020F0">End</FONT></B>;

<B><FONT COLOR="#A020F0">var</FONT></B> NUM, a : integer;

<B><FONT COLOR="#A020F0">begin</FONT></B>
	<B><FONT COLOR="#A020F0">if</FONT></B> ParamCount = 0 <B><FONT COLOR="#A020F0">then</FONT></B>
		NUM := 1
	<B><FONT COLOR="#A020F0">else</FONT></B>
		NUM := StrToInt(ParamStr(1));
		
	<B><FONT COLOR="#A020F0">if</FONT></B> NUM &lt; 1 <B><FONT COLOR="#A020F0">then</FONT></B> NUM := 1;
	a := Ack(3, NUM);
	WriteLn( <B><FONT COLOR="#BC8F8F">'Ack(3,'</FONT></B> + IntToStr(NUM) + <B><FONT COLOR="#BC8F8F">'): '</FONT></B> + IntToStr(a) );
<B><FONT COLOR="#A020F0">end</FONT></B>.</pre></td></tr></table>
