</FONT></I>include <B><FONT COLOR="#BC8F8F">&quot;std&quot;</FONT></B>;
n := atoi(System::argv 1);

open List;
open Text_file;

var x = Empty[string];
var s = readln stdin;
until s == <B><FONT COLOR="#BC8F8F">&quot;&quot;</FONT></B> <B><FONT COLOR="#A020F0">do</FONT></B>
  x = Cons (s,x);
  s = readln stdin;
done;

proc <B><FONT COLOR="#0000FF">plist</FONT></B>(x:list[string]){
  match x with
  | Cons (?s,?t) =&gt; { print s; plist t; }
  |  _ =&gt; {}
  endmatch;
};

plist x;
