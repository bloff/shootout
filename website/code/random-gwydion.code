<table width="100%" border="0" cellpadding="5" cellspacing="0"><tr><td bgcolor="#FFFFFF"><pre>
module:     randum
synopsis:   implementation of <B><FONT COLOR="#BC8F8F">&quot;Random Number Generator&quot;</FONT></B> benchmark
author:     Andreas Bogk
copyright:  public <B><FONT COLOR="#5F9EA0">domain</FONT></B>


<B><FONT COLOR="#A020F0">define</FONT></B> <B><FONT COLOR="#5F9EA0">constant</FONT></B> $IM = 139968;
<B><FONT COLOR="#A020F0">define</FONT></B> <B><FONT COLOR="#5F9EA0">constant</FONT></B> $IA = 3877;
<B><FONT COLOR="#A020F0">define</FONT></B> <B><FONT COLOR="#5F9EA0">constant</FONT></B> $IC = 29573;
<B><FONT COLOR="#A020F0">define</FONT></B> <B><FONT COLOR="#5F9EA0">variable</FONT></B> $LAST <B><FONT COLOR="#5F9EA0">::</FONT></B> <B><FONT COLOR="#228B22">&lt;integer&gt;</FONT></B> = 42;

<B><FONT COLOR="#A020F0">define</FONT></B> <B><FONT COLOR="#5F9EA0">method</FONT></B> <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">generate-random </FONT></I></B></FONT></B>(max <B><FONT COLOR="#5F9EA0">::</FONT></B> <B><FONT COLOR="#228B22">&lt;double-float&gt;</FONT></B>) <B><FONT COLOR="#A020F0">=&gt;</FONT></B> <B><FONT COLOR="#228B22">&lt;double-float&gt;</FONT></B>;
  $LAST <B><FONT COLOR="#5F9EA0">:</FONT></B>= <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">modulo</FONT></I></B></FONT></B>($LAST * $IA + $IC, $IM);
  max * $LAST / $IM;
<B><FONT COLOR="#A020F0">end</FONT></B> <B><FONT COLOR="#5F9EA0">method</FONT></B>;

<B><FONT COLOR="#A020F0">begin</FONT></B>
  <B><FONT COLOR="#A020F0">let</FONT></B> arg = <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">string-to-integer</FONT></I></B></FONT></B>(<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">element</FONT></I></B></FONT></B>(<B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">application-arguments</FONT></I></B></FONT></B>(), 0, <B><FONT COLOR="#5F9EA0">default:</FONT></B> <B><FONT COLOR="#BC8F8F">&quot;1&quot;</FONT></B>)) - 1;

  <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">for </FONT></I></B></FONT></B>(i <B><FONT COLOR="#A020F0">from</FONT></B> 1 <B><FONT COLOR="#A020F0">to</FONT></B> arg)
    <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">generate-random</FONT></I></B></FONT></B>(100.0);
  <B><FONT COLOR="#A020F0">end</FONT></B>;

  <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">format-out</FONT></I></B></FONT></B>(<B><FONT COLOR="#BC8F8F">&quot;%=\n&quot;</FONT></B>, <B><FONT COLOR="#0000FF"><B><I><FONT COLOR="#000000">generate-random</FONT></I></B></FONT></B>(100.0));
<B><FONT COLOR="#A020F0">end</FONT></B>;</pre></td></tr></table>
