</span><span class="line">   33 </span><span class="kwb"></span>    <span class="sym">}
</span><span class="line">   34 </span><span class="sym">}
</span><span class="line">   35 </span><span class="sym">
</span><span class="line">   36 </span><span class="sym"></span><span class="kwa">proc</span> blowup <span class="sym">{</span>num<span class="sym">} {
