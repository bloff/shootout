<table width="100%" border="0" cellpadding="5" cellspacing="0"><tr><td bgcolor="#FFFFFF"><pre>
<I><FONT COLOR="#B22222">#!/usr/bin/tclsh
</FONT></I><I><FONT COLOR="#B22222"># $Id: except-tcl.code,v 1.1 2004-11-02 08:52:30 bfulgham Exp $
</FONT></I><I><FONT COLOR="#B22222"># http://www.bagley.org/~doug/shootout/
</FONT></I><I><FONT COLOR="#B22222"># with help from Kristoffer Lawson
</FONT></I><I><FONT COLOR="#B22222"># modified by Miguel Sofer
</FONT></I>
<B><FONT COLOR="#A020F0">set</FONT></B> HI 0
<B><FONT COLOR="#A020F0">set</FONT></B> LO 0

<B><FONT COLOR="#A020F0">proc</FONT></B> <B><FONT COLOR="#0000FF">some_function</FONT></B> {<FONT COLOR="#B8860B">num</FONT>} {
    <B><FONT COLOR="#A020F0">if</FONT></B> {[<B><FONT COLOR="#A020F0">catch</FONT></B> {hi_function $<FONT COLOR="#B8860B">num</FONT>} result]} {
        <B><FONT COLOR="#A020F0">puts</FONT></B> stderr <B><FONT COLOR="#BC8F8F">&quot;We shouldn't get here ($result)&quot;</FONT></B>
    }
}

<B><FONT COLOR="#A020F0">proc</FONT></B> <B><FONT COLOR="#0000FF">hi_function</FONT></B> {<FONT COLOR="#B8860B">num</FONT>} {
    <B><FONT COLOR="#A020F0">if</FONT></B> {[<B><FONT COLOR="#A020F0">set</FONT></B> exc [<B><FONT COLOR="#A020F0">catch</FONT></B> {lo_function $<FONT COLOR="#B8860B">num</FONT>}]] == 11} {
        <I><FONT COLOR="#B22222"># handle
</FONT></I>	<B><FONT COLOR="#A020F0">incr</FONT></B> ::HI
    } else {
        <I><FONT COLOR="#B22222"># rethrow
</FONT></I>	<B><FONT COLOR="#A020F0">return</FONT></B> -code $<FONT COLOR="#B8860B">exc</FONT>
    }
}

<B><FONT COLOR="#A020F0">proc</FONT></B> <B><FONT COLOR="#0000FF">lo_function</FONT></B> {<FONT COLOR="#B8860B">num</FONT>} {
    <B><FONT COLOR="#A020F0">if</FONT></B> {[<B><FONT COLOR="#A020F0">set</FONT></B> exc [<B><FONT COLOR="#A020F0">catch</FONT></B> {blowup $<FONT COLOR="#B8860B">num</FONT>}]] == 10} {
        <I><FONT COLOR="#B22222"># handle
</FONT></I>	<B><FONT COLOR="#A020F0">incr</FONT></B> ::LO
    } else {
        <I><FONT COLOR="#B22222"># rethrow
</FONT></I>	<B><FONT COLOR="#A020F0">return</FONT></B> -code $<FONT COLOR="#B8860B">exc</FONT>
    }
}

<B><FONT COLOR="#A020F0">proc</FONT></B> <B><FONT COLOR="#0000FF">blowup</FONT></B> {<FONT COLOR="#B8860B">num</FONT>} {
    <B><FONT COLOR="#A020F0">if</FONT></B> {$<FONT COLOR="#B8860B">num</FONT> % 2} {
        <I><FONT COLOR="#B22222">#error &quot;Lo_exception&quot;
</FONT></I>	<B><FONT COLOR="#A020F0">return</FONT></B> -code 10
    } else {
        <I><FONT COLOR="#B22222">#error &quot;Hi_exception&quot;
</FONT></I>	<B><FONT COLOR="#A020F0">return</FONT></B> -code 11
    }
}

<B><FONT COLOR="#A020F0">proc</FONT></B> <B><FONT COLOR="#0000FF">main</FONT></B> {<FONT COLOR="#B8860B"></FONT>} {
    <B><FONT COLOR="#A020F0">global</FONT></B> argv HI LO
    <B><FONT COLOR="#A020F0">set</FONT></B> NUM [<B><FONT COLOR="#A020F0">lindex</FONT></B> $<FONT COLOR="#B8860B">argv</FONT> 0]
    <B><FONT COLOR="#A020F0">if</FONT></B> {$<FONT COLOR="#B8860B">NUM</FONT> &lt; 1} {
        <B><FONT COLOR="#A020F0">set</FONT></B> NUM 1
    }
    <B><FONT COLOR="#A020F0">incr</FONT></B> NUM
    <B><FONT COLOR="#A020F0">while</FONT></B> {[<B><FONT COLOR="#A020F0">incr</FONT></B> NUM -1]} {
        some_function $<FONT COLOR="#B8860B">NUM</FONT>
    }
    <B><FONT COLOR="#A020F0">puts</FONT></B> <B><FONT COLOR="#BC8F8F">&quot;Exceptions: HI=$HI / LO=$LO&quot;</FONT></B>
}

main</pre></td></tr></table>
