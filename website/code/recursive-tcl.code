<span class="kwa">proc</span> ack <span class="sym">{</span>n m<span class="sym">} {</span>
  <span class="kwa">if</span> <span class="sym">{</span><span class="kwb">$m</span><span class="sym">} {</span>
    <span class="kwa">if</span> <span class="sym">{</span><span class="kwb">$n</span><span class="sym">} {</span>
      <span class="kwa">return</span> <span class="sym">[</span>ack <span class="sym">[</span>ack <span class="sym">[</span><span class="kwa">incr</span> n <span class="sym">-</span><span class="num">1</span><span class="sym">]</span> <span class="kwb">$m</span><span class="sym">] [</span><span class="kwa">incr</span> m <span class="sym">-</span><span class="num">1</span><span class="sym">]]</span>
    <span class="sym">}</span> else <span class="sym">{</span>
      <span class="kwa">return</span> <span class="sym">[</span>ack <span class="num">1</span> <span class="sym">[</span><span class="kwa">incr</span> m <span class="sym">-</span><span class="num">1</span><span class="sym">]]</span>
    <span class="sym">}</span>
  <span class="sym">}</span>
  <span class="kwa">return</span> <span class="sym">[</span><span class="kwa">incr</span> n<span class="sym">]</span>
<span class="sym">}</span>

<span class="slc"># ---------------</span>

<span class="kwa">proc</span> fib <span class="sym">{</span>n<span class="sym">} {</span>
  <span class="kwa">if</span> <span class="sym">{</span><span class="kwb">$n</span> <span class="sym">&lt;</span> <span class="num">2</span><span class="sym">} {</span><span class="kwa">return</span> <span class="num">1</span><span class="sym">}</span>
  <span class="kwa">return</span> <span class="sym">[</span><span class="kwa">expr</span> <span class="sym">{[</span>fib <span class="sym">[</span><span class="kwa">incr</span> n <span class="sym">-</span><span class="num">2</span><span class="sym">]] + [</span>fib <span class="sym">[</span><span class="kwa">incr</span> n<span class="sym">]]}]</span>
<span class="sym">}</span>

<span class="kwa">proc</span> fibflt <span class="sym">{</span>n<span class="sym">} {</span>
  <span class="kwa">if</span> <span class="sym">{</span><span class="kwb">$n</span> <span class="sym">&lt;</span> <span class="num">2</span><span class="sym">} {</span><span class="kwa">return</span> <span class="num">1</span><span class="sym">}</span>
  <span class="kwa">return</span> <span class="sym">[</span><span class="kwa">expr</span> <span class="sym">{[</span>fibflt <span class="sym">[</span><span class="kwa">expr</span> <span class="sym">{</span><span class="kwb">$n</span> <span class="sym">-</span> <span class="num">2.0</span><span class="sym">}]] + [</span>fibflt <span class="sym">[</span><span class="kwa">expr</span> <span class="sym">{</span><span class="kwb">$n</span> <span class="sym">-</span> <span class="num">1.0</span><span class="sym">}]]}]</span>
<span class="sym">}</span>

<span class="slc"># ---------------</span>

<span class="kwa">proc</span> tak <span class="sym">{</span>x y z<span class="sym">} {</span>
  <span class="kwa">if</span> <span class="sym">{</span><span class="kwb">$y</span> <span class="sym">&gt;=</span> <span class="kwb">$x</span><span class="sym">} {</span><span class="kwa">return</span> <span class="kwb">$z</span><span class="sym">}</span>
  <span class="kwa">return</span> <span class="sym">[</span>tak <span class="sym">[</span>tak <span class="sym">[</span><span class="kwa">expr</span> <span class="sym">{</span><span class="kwb">$x</span> <span class="sym">-</span> <span class="num">1</span><span class="sym">}]</span> <span class="kwb">$y $z</span><span class="sym">] [</span>tak <span class="sym">[</span><span class="kwa">expr</span> <span class="sym">{</span><span class="kwb">$y</span> <span class="sym">-</span> <span class="num">1</span><span class="sym">}]</span> <span class="kwb">$z $x</span><span class="sym">] [</span>tak <span class="sym">[</span><span class="kwa">expr</span> <span class="sym">{</span><span class="kwb">$z</span> <span class="sym">-</span> <span class="num">1</span><span class="sym">}]</span> <span class="kwb">$x $y</span><span class="sym">]]</span>
<span class="sym">}</span>

<span class="kwa">proc</span> takflt <span class="sym">{</span>x y z<span class="sym">} {</span>
  <span class="kwa">if</span> <span class="sym">{</span><span class="kwb">$y</span> <span class="sym">&gt;=</span> <span class="kwb">$x</span><span class="sym">} {</span><span class="kwa">return</span> <span class="kwb">$z</span><span class="sym">}</span>
  <span class="kwa">return</span> <span class="sym">[</span>takflt <span class="sym">[</span>takflt <span class="sym">[</span><span class="kwa">expr</span> <span class="sym">{</span><span class="kwb">$x</span> <span class="sym">-</span> <span class="num">1.0</span><span class="sym">}]</span> <span class="kwb">$y $z</span><span class="sym">] [</span>takflt <span class="sym">[</span><span class="kwa">expr</span> <span class="sym">{</span><span class="kwb">$y</span> <span class="sym">-</span> <span class="num">1.0</span><span class="sym">}]</span> <span class="kwb">$z $x</span><span class="sym">] [</span>takflt <span class="sym">[</span><span class="kwa">expr</span> <span class="sym">{</span><span class="kwb">$z</span> <span class="sym">-</span> <span class="num">1.0</span><span class="sym">}]</span> <span class="kwb">$x $y</span><span class="sym">]]</span>
<span class="sym">}</span>

<span class="slc"># ---------------------------------</span>

<span class="kwa">interp</span> recursionlimit <span class="sym">{}</span> <span class="num">20000</span>
<span class="kwa">set</span> N <span class="sym">[</span><span class="kwa">lindex</span> <span class="kwb">$argv</span> <span class="num">0</span><span class="sym">]</span>
<span class="kwa">if</span> <span class="sym">{</span><span class="kwb">$N</span> <span class="sym">&lt;</span> <span class="num">1</span><span class="sym">} {</span><span class="kwa">set</span> N <span class="num">1</span><span class="sym">}</span>

<span class="kwa">puts</span> <span class="sym">[</span><span class="kwa">format</span> <span class="str">&quot;Ack(3,%d): %d&quot;</span> <span class="kwb">$N</span> <span class="sym">[</span>ack <span class="num">3</span> <span class="kwb">$N</span><span class="sym">]]</span>
<span class="kwa">puts</span> <span class="sym">[</span><span class="kwa">format</span> <span class="str">&quot;Fib(%.1f): %.1f&quot;</span> <span class="sym">[</span><span class="kwa">expr</span> <span class="sym">{</span><span class="num">27.0</span> <span class="sym">+</span> <span class="kwb">$N</span><span class="sym">}] [</span>fibflt <span class="sym">[</span><span class="kwa">expr</span> <span class="sym">{</span><span class="num">27.0</span> <span class="sym">+</span> <span class="kwb">$N</span><span class="sym">}]] ]</span>

<span class="kwa">set</span> N <span class="sym">[</span><span class="kwa">incr</span> N <span class="sym">-</span><span class="num">1</span><span class="sym">]</span>
<span class="kwa">puts</span>   <span class="sym">[</span><span class="kwa">format</span> <span class="str">&quot;Tak(%d,%d,%d): %d&quot;</span> <span class="sym">[</span><span class="kwa">expr</span> <span class="sym">{</span><span class="kwb">$N</span> <span class="sym">*</span> <span class="num">3</span><span class="sym">}] [</span><span class="kwa">expr</span> <span class="sym">{</span><span class="kwb">$N</span> <span class="sym">*</span> <span class="num">2</span><span class="sym">}]</span> <span class="kwb">$N</span>  <span class="sym">[</span>tak <span class="sym">[</span><span class="kwa">expr</span> <span class="sym">{</span><span class="kwb">$N</span> <span class="sym">*</span> <span class="num">3</span><span class="sym">}] [</span><span class="kwa">expr</span> <span class="sym">{</span><span class="kwb">$N</span> <span class="sym">*</span> <span class="num">2</span><span class="sym">}]</span> <span class="kwb">$N</span><span class="sym">]  ]</span>

<span class="kwa">puts</span> <span class="sym">[</span><span class="kwa">format</span> <span class="str">&quot;Fib(3): %d&quot;</span> <span class="sym">[</span>fib <span class="num">3</span><span class="sym">]]</span>
<span class="kwa">puts</span> <span class="sym">[</span><span class="kwa">format</span> <span class="str">&quot;Tak(3.0,2.0,1.0): %.1f&quot;</span> <span class="sym">[</span>takflt <span class="num">3.0 2.0 1.0</span><span class="sym">]]</span>

