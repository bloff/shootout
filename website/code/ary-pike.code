#!/<B><FONT COLOR="#5F9EA0">usr/bin/pike</FONT></B>
<I><FONT COLOR="#B22222">// -*- mode: pike -*-
</FONT></I><I><FONT COLOR="#B22222">// $Id: ary-pike.code,v 1.1 2004-11-02 08:52:29 bfulgham Exp $
</FONT></I><I><FONT COLOR="#B22222">// http://www.bagley.org/~doug/shootout/
</FONT></I>
<B><FONT COLOR="#228B22">void</FONT></B> <B><FONT COLOR="#0000FF">main</FONT></B>(<B><FONT COLOR="#228B22">int</FONT></B> argc, array(string) argv)
{
    <B><FONT COLOR="#228B22">int</FONT></B> n = (<B><FONT COLOR="#228B22">int</FONT></B>)argv[-1];
    <B><FONT COLOR="#A020F0">if</FONT></B> (n &lt; 1) n = 1;

    array(<B><FONT COLOR="#228B22">int</FONT></B>) x = allocate(n);
    array(<B><FONT COLOR="#228B22">int</FONT></B>) y = allocate(n);

    <B><FONT COLOR="#A020F0">for</FONT></B> (<B><FONT COLOR="#228B22">int</FONT></B> i; i&lt;n; i++) {
	x[i] = i + 1;
    }
    <B><FONT COLOR="#A020F0">for</FONT></B> (<B><FONT COLOR="#228B22">int</FONT></B> k; k&lt;1000; k++) {
	<B><FONT COLOR="#A020F0">for</FONT></B> (<B><FONT COLOR="#228B22">int</FONT></B> i=n-1; i&gt;=0; i--) {
	    y[i] += x[i];
	}
    }
    write(<B><FONT COLOR="#BC8F8F">&quot;%d %d\n&quot;</FONT></B>, y[0], y[-1]);
}</pre></td></tr></table>
