<span class="hl kwb">void</span> Piece<span class="hl sym">::</span><span class="hl kwd">genOrientation</span><span class="hl sym">(</span>BitVec vec<span class="hl sym">,</span> TUInt32 iOrient<span class="hl sym">,</span> Piece <span class="hl sym">&amp;</span> target<span class="hl sym">)</span>
<span class="hl sym">{</span>
   <span class="hl slc">// get (x,y) coordinates</span>
   TPts pts<span class="hl sym">;</span>
   <span class="hl kwd">setCoordList</span><span class="hl sym">(</span>vec<span class="hl sym">,</span> pts<span class="hl sym">);</span>

