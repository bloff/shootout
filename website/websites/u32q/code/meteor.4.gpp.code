<span class="hl kwb">void</span> Piece<span class="hl sym">::</span><span class="hl kwd">shiftUpLines</span><span class="hl sym">(</span>TPts <span class="hl sym">&amp;</span> pts<span class="hl sym">,</span> <span class="hl kwb">int</span> shift<span class="hl sym">) {</span>
