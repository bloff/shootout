<span class="hl kwb">void</span> Board<span class="hl sym">::</span><span class="hl kwd">genAllSolutions</span><span class="hl sym">(</span>BitVec boardVec<span class="hl sym">,</span> TUInt32 placedPieces<span class="hl sym">,</span> TUInt32 row<span class="hl sym">)</span>
<span class="hl sym">{</span>
