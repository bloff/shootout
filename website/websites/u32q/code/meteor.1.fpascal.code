   <span class="hl kwd">filldword</span><span class="hl sym">(</span>cache<span class="hl sym">,</span><span class="hl kwd">sizeof</span><span class="hl sym">(</span>cache<span class="hl sym">)</span> <span class="hl kwa">shr</span> <span class="hl num">2</span><span class="hl sym">,</span><span class="hl num">$ffffffff</span><span class="hl sym">);</span>
   <span class="hl kwd">gen_all_solutions</span><span class="hl sym">([], [],</span> <span class="hl num">0</span><span class="hl sym">);</span>

   <span class="hl kwd">writeln</span><span class="hl sym">(</span>m_nSoln<span class="hl sym">,</span><span class="hl str">' solutions found'</span><span class="hl sym">);</span>
   writeln<span class="hl sym">;</span>
   m_minSoln<span class="hl sym">.</span><span class="hl kwd">write</span><span class="hl sym">(</span>output<span class="hl sym">);</span>
   writeln<span class="hl sym">;</span>
   m_maxSoln<span class="hl sym">.</span><span class="hl kwd">write</span><span class="hl sym">(</span>output<span class="hl sym">);</span>
   writeln<span class="hl sym">;</span>
<span class="hl kwa">end</span><span class="hl sym">.</span>
