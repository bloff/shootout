  <span class="hl kwa">val</span> <span class="hl sym">() =</span> split <span class="hl sym">(</span>ws<span class="hl sym">,</span> <span class="hl num">0</span><span class="hl sym">, &amp;</span>u<span class="hl sym">, &amp;</span>tmp<span class="hl sym">)</span>
