<span class="hl slc">//</span>
  <span class="hl kwa">val</span> nworker <span class="hl sym">=</span> workshop_get_nworker <span class="hl sym">(</span>ws<span class="hl sym">)</span>
  <span class="hl kwa">val</span> <span class="hl sym">() =</span> assert_errmsg <span class="hl sym">(</span>nworker <span class="hl sym">&gt;</span> <span class="hl num">0</span><span class="hl sym">,</span> <span class="hl dir">#LOCATION)</span>
  <span class="hl kwa">val</span> INC <span class="hl sym">= (</span>N<span class="hl sym">+</span>nworker<span class="hl sym">-</span><span class="hl num">1</span><span class="hl sym">)/</span>nworker<span class="hl sym">/</span><span class="hl num">64</span>
  <span class="hl kwa">val</span> INC <span class="hl sym">=</span> max <span class="hl sym">(</span><span class="hl num">1</span><span class="hl sym">, (</span>int1_of_int<span class="hl sym">)</span>INC<span class="hl sym">)</span>
