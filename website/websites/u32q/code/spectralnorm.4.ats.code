<span class="hl com">(* ****** ****** *)</span>

<span class="hl kwa">fn</span> eval_AtA_times_u <span class="hl sym">{</span>lws<span class="hl sym">:</span>agz<span class="hl sym">} {</span>N<span class="hl sym">:</span>nat<span class="hl sym">} (</span>
  ws<span class="hl sym">: !</span>WSptr lws<span class="hl sym">,</span> N<span class="hl sym">:</span> int N<span class="hl sym">,</span> u<span class="hl sym">: &amp;</span>darr N<span class="hl sym">,</span> v<span class="hl sym">: &amp;</span>darr N<span class="hl sym">,</span> tmp<span class="hl sym">: &amp;</span>darr N
