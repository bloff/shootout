  <span class="hl kwa">fun</span> split <span class="hl sym">{</span>lws<span class="hl sym">:</span>agz<span class="hl sym">} {</span>i<span class="hl sym">:</span>nat <span class="hl sym">|</span> i <span class="hl sym">&lt;=</span> N<span class="hl sym">} {</span>l<span class="hl sym">,</span>l_tmp<span class="hl sym">:</span>addr<span class="hl sym">}</span>
