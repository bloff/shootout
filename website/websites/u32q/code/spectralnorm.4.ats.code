    <span class="hl kwa">prval</span> <span class="hl sym">() =</span> __velim <span class="hl sym">(</span>pf<span class="hl sym">)</span> <span class="hl kwa">and</span> <span class="hl sym">() =</span> __velim <span class="hl sym">(</span>pf_tmp<span class="hl sym">)</span>
