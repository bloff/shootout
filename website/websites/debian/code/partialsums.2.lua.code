<span class="kwa">local</span> n <span class="sym">=</span> <span class="kwb">tonumber</span><span class="sym">(</span>arg<span class="sym">[</span><span class="num">1</span><span class="sym">])</span>
<span class="kwa">local function</span> <span class="kwd">pr</span><span class="sym">(</span>fmt<span class="sym">,</span> x<span class="sym">)</span> io<span class="sym">.</span><span class="kwb">write</span><span class="sym">(</span><span class="kwb">string</span><span class="sym">.</span><span class="kwb">format</span><span class="sym">(</span>fmt<span class="sym">,</span> x<span class="sym">))</span> <span class="kwa">end</span>

<span class="kwa">do</span>
  <span class="kwa">local</span> sum1<span class="sym">,</span> sum2<span class="sym">,</span> sum3<span class="sym">,</span> sum4<span class="sym">,</span> sum5<span class="sym">,</span> sum6<span class="sym">,</span> sum7 <span class="sym">=</span> <span class="num">1</span><span class="sym">,</span> <span class="num">0</span><span class="sym">,</span> <span class="num">0</span><span class="sym">,</span> <span class="num">0</span><span class="sym">,</span> <span class="num">0</span><span class="sym">,</span> <span class="num">0</span><span class="sym">,</span> <span class="num">0</span>
  <span class="kwa">local</span> twothirds<span class="sym">,</span> <span class="kwb">sqrt</span><span class="sym">,</span> <span class="kwb">sin</span><span class="sym">,</span> <span class="kwb">cos</span> <span class="sym">=</span> <span class="num">2</span><span class="sym">/</span><span class="num">3</span><span class="sym">,</span> math<span class="sym">.</span><span class="kwb">sqrt</span><span class="sym">,</span> math<span class="sym">.</span><span class="kwb">sin</span><span class="sym">,</span> math<span class="sym">.</span><span class="kwb">cos</span>
  <span class="kwa">for</span> k<span class="sym">=</span><span class="num">1</span><span class="sym">,</span>n <span class="kwa">do</span>
    <span class="kwa">local</span> k2<span class="sym">,</span> sk<span class="sym">,</span> ck <span class="sym">=</span> k<span class="sym">*</span>k<span class="sym">,</span> <span class="kwb">sin</span><span class="sym">(</span>k<span class="sym">),</span> <span class="kwb">cos</span><span class="sym">(</span>k<span class="sym">)</span>
    <span class="kwa">local</span> k3 <span class="sym">=</span> k2<span class="sym">*</span>k
    sum1 <span class="sym">=</span> sum1 <span class="sym">+</span> twothirds^k
    sum2 <span class="sym">=</span> sum2 <span class="sym">+</span> <span class="num">1</span><span class="sym">/</span><span class="kwb">sqrt</span><span class="sym">(</span>k<span class="sym">)</span>
    sum3 <span class="sym">=</span> sum3 <span class="sym">+</span> <span class="num">1</span><span class="sym">/(</span>k2<span class="sym">+</span>k<span class="sym">)</span>
    sum4 <span class="sym">=</span> sum4 <span class="sym">+</span> <span class="num">1</span><span class="sym">/(</span>k3<span class="sym">*</span>sk<span class="sym">*</span>sk<span class="sym">)</span>
    sum5 <span class="sym">=</span> sum5 <span class="sym">+</span> <span class="num">1</span><span class="sym">/(</span>k3<span class="sym">*</span>ck<span class="sym">*</span>ck<span class="sym">)</span>
    sum6 <span class="sym">=</span> sum6 <span class="sym">+</span> <span class="num">1</span><span class="sym">/</span>k
