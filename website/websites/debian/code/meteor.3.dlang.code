      s<span class="sym">.</span><span class="kwd">spin</span><span class="sym">(</span>spun<span class="sym">);</span>
      <span class="kwa">if</span> <span class="sym">(</span>spun<span class="sym">.</span><span class="kwd">lessThan</span><span class="sym">(</span>m_minSoln<span class="sym">))</span>
         m_minSoln <span class="sym">=</span> spun<span class="sym">;</span>
      <span class="kwa">else if</span> <span class="sym">(</span>m_maxSoln<span class="sym">.</span><span class="kwd">lessThan</span><span class="sym">(</span>spun<span class="sym">))</span>
         m_maxSoln <span class="sym">=</span> spun<span class="sym">;</span>
   <span class="sym">}</span>
<span class="sym">}</span>

