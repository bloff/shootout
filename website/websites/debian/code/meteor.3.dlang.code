            <span class="kwa">uint</span> boardVec <span class="sym">= (</span>i <span class="sym">&lt;&lt; (</span><span class="num">2</span> <span class="sym">*</span> N_COL<span class="sym">)) |</span> iWord<span class="sym">;</span>
            <span class="kwa">if</span> <span class="sym">(</span>isleInfo<span class="sym">.</span>isKnown<span class="sym">[</span><span class="num">0</span><span class="sym">][</span>OPEN<span class="sym">] &amp;</span> mask<span class="sym">)</span>
               <span class="kwa">continue</span><span class="sym">;</span>

