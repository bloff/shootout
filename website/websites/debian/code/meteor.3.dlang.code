      <span class="kwa">int</span> pieceMask <span class="sym">=</span> <span class="kwd">getMask</span><span class="sym">(</span>iPiece<span class="sym">);</span>
      <span class="kwa">for</span> <span class="sym">(;</span> iPiece <span class="sym">&lt;</span> N_PIECE_TYPE<span class="sym">;</span> iPiece<span class="sym">++,</span> pieceMask <span class="sym">&lt;&lt;=</span> <span class="num">1</span><span class="sym">)</span>
      <span class="sym">{</span>
         <span class="slc">// skip if we've already used this piece</span>
