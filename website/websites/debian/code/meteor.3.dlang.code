            <span class="kwa">continue</span><span class="sym">;</span>

         placedPieces <span class="sym">|=</span> pieceMask<span class="sym">;</span>
         <span class="kwa">for</span> <span class="sym">(</span><span class="kwa">int</span> iOrient <span class="sym">=</span> <span class="num">0</span><span class="sym">;</span> iOrient <span class="sym">&lt;</span> allowed<span class="sym">.</span>nPieces<span class="sym">[</span>iPiece<span class="sym">];</span> iOrient<span class="sym">++) {</span>
            BitVec pieceVec <span class="sym">=</span> allowed<span class="sym">.</span>pieceVec<span class="sym">[</span>iPiece<span class="sym">][</span>iOrient<span class="sym">];</span>

            <span class="slc">// check if piece conflicts with other pieces</span>
            <span class="kwa">if</span> <span class="sym">(</span>pieceVec <span class="sym">&amp;</span> boardVec<span class="sym">)</span>
               <span class="kwa">continue</span><span class="sym">;</span>

