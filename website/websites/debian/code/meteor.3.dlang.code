            m_curSoln<span class="sym">.</span><span class="kwd">pushPiece</span><span class="sym">(</span>pieceVec<span class="sym">,</span> iPiece<span class="sym">,</span> row<span class="sym">);</span>

            <span class="slc">// recur or record solution</span>
            <span class="kwa">if</span> <span class="sym">(</span>placedPieces <span class="sym">!=</span> Piece<span class="sym">.</span>ALL_PIECE_MASK<span class="sym">)</span>
               <span class="kwd">genAllSolutions</span><span class="sym">(</span>boardVec<span class="sym">,</span> placedPieces<span class="sym">,</span> row<span class="sym">);</span>
