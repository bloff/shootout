   <span class="kwa">void</span> <span class="kwd">genAllSolutions</span><span class="sym">(</span>BitVec boardVec<span class="sym">,</span> <span class="kwa">uint</span> placedPieces<span class="sym">,</span> <span class="kwa">uint</span> row<span class="sym">)</span>
   <span class="sym">{</span>
      <span class="kwa">while</span> <span class="sym">((</span>boardVec <span class="sym">&amp;</span> TOP_ROW<span class="sym">) ==</span> TOP_ROW<span class="sym">) {</span>
         boardVec <span class="sym">&gt;&gt;=</span> N_COL<span class="sym">;</span>
