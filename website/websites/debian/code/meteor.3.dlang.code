            <span class="kwa">if</span> <span class="sym">(</span><span class="kwd">hasBadIslands</span><span class="sym">(</span>boardVec<span class="sym">,</span> row<span class="sym">)) {</span>
               boardVec ^<span class="sym">=</span> pieceVec<span class="sym">;</span>
