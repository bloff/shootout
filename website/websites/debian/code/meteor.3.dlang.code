   <span class="kwd">writefln</span><span class="sym">(</span>b<span class="sym">.</span>m_nSoln<span class="sym">,</span> <span class="str">&quot; solutions found</span><span class="esc">\n</span><span class="str">&quot;</span><span class="sym">);</span>
   <span class="kwd">writefln</span><span class="sym">(</span>b<span class="sym">.</span>m_minSoln<span class="sym">);</span>
   <span class="kwd">writefln</span><span class="sym">(</span>b<span class="sym">.</span>m_maxSoln<span class="sym">);</span>

   <span class="kwa">return</span> <span class="num">0</span><span class="sym">;</span>
<span class="sym">}</span>
