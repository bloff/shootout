         isleInfo<span class="sym">.</span>isKnown<span class="sym">[</span><span class="num">0</span><span class="sym">][</span>OPEN<span class="sym">] =</span> <span class="kwa">cast</span><span class="sym">(</span><span class="kwa">uint</span><span class="sym">)(-</span><span class="num">1</span><span class="sym">);</span>
      <span class="sym">}</span>
