   <span class="kwa">static void</span> <span class="kwd">calcAlwaysBad</span><span class="sym">() {</span>
      <span class="kwa">for</span> <span class="sym">(</span><span class="kwa">uint</span> iWord <span class="sym">=</span> <span class="num">1</span><span class="sym">;</span> iWord <span class="sym">&lt;</span> MAX_ISLAND_OFFSET<span class="sym">;</span> iWord<span class="sym">++) {</span>
