<span class="hl sym">(</span>defconstant <span class="hl sym">+</span>digits<span class="hl sym">-</span>per<span class="hl sym">-</span><span class="hl kwa">line</span><span class="hl sym">+</span> <span class="hl num">10</span><span class="hl sym">)</span>
