  <span class="hl sym">(</span>let <span class="hl sym">((</span>digits<span class="hl sym">-</span>printed <span class="hl num">0</span><span class="hl sym">)</span>
        <span class="hl sym">(</span>next<span class="hl sym">-</span>digit <span class="hl sym">(</span>make<span class="hl sym">-</span>digit<span class="hl sym">-</span>generator<span class="hl sym">)))</span>
    <span class="hl sym">(</span>loop <span class="hl kwa">while</span> <span class="hl sym">(</span>plusp digits<span class="hl sym">)</span>
          do <span class="hl sym">(</span><span class="hl kwa">if</span> <span class="hl sym">(&gt;=</span> digits <span class="hl sym">+</span>digits<span class="hl sym">-</span>per<span class="hl sym">-</span><span class="hl kwa">line</span><span class="hl sym">+)</span>
                 <span class="hl sym">(</span><span class="hl kwa">progn</span> <span class="hl sym">(</span>loop <span class="hl kwa">repeat</span> <span class="hl sym">+</span>digits<span class="hl sym">-</span>per<span class="hl sym">-</span><span class="hl kwa">line</span><span class="hl sym">+</span>
                              do <span class="hl sym">(</span>format t <span class="hl str">&quot;~d&quot;</span> <span class="hl sym">(</span>funcall next<span class="hl sym">-</span>digit<span class="hl sym">)))</span>
                        <span class="hl sym">(</span>incf digits<span class="hl sym">-</span>printed <span class="hl sym">+</span>digits<span class="hl sym">-</span>per<span class="hl sym">-</span><span class="hl kwa">line</span><span class="hl sym">+))</span>
                 <span class="hl sym">(</span><span class="hl kwa">progn</span> <span class="hl sym">(</span>loop <span class="hl kwa">repeat</span> digits
                              do <span class="hl sym">(</span>format t <span class="hl str">&quot;~d&quot;</span> <span class="hl sym">(</span>funcall next<span class="hl sym">-</span>digit<span class="hl sym">)))</span>
                        <span class="hl sym">(</span>loop <span class="hl kwa">repeat</span> <span class="hl sym">(- +</span>digits<span class="hl sym">-</span>per<span class="hl sym">-</span><span class="hl kwa">line</span><span class="hl sym">+</span> digits<span class="hl sym">)</span>
                              do <span class="hl sym">(</span>format t <span class="hl str">&quot; &quot;</span><span class="hl sym">))</span>
                        <span class="hl sym">(</span>incf digits<span class="hl sym">-</span>printed digits<span class="hl sym">)))</span>
             <span class="hl sym">(</span>format t <span class="hl str">&quot;~a:~d~%&quot;</span> #\Tab digits<span class="hl sym">-</span>printed<span class="hl sym">)</span>
             <span class="hl sym">(</span>decf digits <span class="hl sym">+</span>digits<span class="hl sym">-</span>per<span class="hl sym">-</span><span class="hl kwa">line</span><span class="hl sym">+))))</span>


