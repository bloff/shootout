<span class="hl kwa">local</span> <span class="hl kwb">sqrt</span><span class="hl sym">,</span> <span class="hl kwb">sin</span><span class="hl sym">,</span> <span class="hl kwb">cos</span> <span class="hl sym">=</span> math<span class="hl sym">.</span><span class="hl kwb">sqrt</span><span class="hl sym">,</span> math<span class="hl sym">.</span><span class="hl kwb">sin</span><span class="hl sym">,</span> math<span class="hl sym">.</span><span class="hl kwb">cos</span>
<span class="hl kwa">for</span> k<span class="hl sym">=</span><span class="hl num">1</span><span class="hl sym">,</span>n <span class="hl kwa">do</span>
