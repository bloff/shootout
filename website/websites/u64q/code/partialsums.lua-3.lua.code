  a5 <span class="hl sym">=</span> a5 <span class="hl sym">+</span> <span class="hl num">1</span><span class="hl sym">/(</span>k3<span class="hl sym">*</span>ck<span class="hl sym">*</span>ck<span class="hl sym">)</span>
  a6 <span class="hl sym">=</span> a6 <span class="hl sym">+</span> <span class="hl num">1</span><span class="hl sym">/</span>k
  a7 <span class="hl sym">=</span> a7 <span class="hl sym">+</span> <span class="hl num">1</span><span class="hl sym">/</span>k2
