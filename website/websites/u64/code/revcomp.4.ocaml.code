	      <span class="hl kwa">if</span> pos <span class="hl sym">&gt;</span> begin_pos <span class="hl kwa">then</span>
		handler s begin_pos <span class="hl sym">(</span>pos <span class="hl sym">-</span> begin_pos<span class="hl sym">);</span>
	      iter2 <span class="hl sym">(</span>pos<span class="hl sym">+</span><span class="hl num">1</span><span class="hl sym">) (</span>pos<span class="hl sym">+</span><span class="hl num">1</span><span class="hl sym">)</span> end_pos
	  <span class="hl sym">|</span> _ <span class="hl sym">-&gt;</span>
	      iter2 begin_pos <span class="hl sym">(</span>pos<span class="hl sym">+</span><span class="hl num">1</span><span class="hl sym">)</span> end_pos

