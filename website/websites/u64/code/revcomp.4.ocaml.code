<span class="hl com">(* don't allocate any buffers on x64 *)</span>
  <span class="hl kwa">let</span> nbuffers <span class="hl sym">=</span> <span class="hl kwa">if</span> arch64 <span class="hl kwa">then</span> <span class="hl num">0</span> <span class="hl kwa">else</span> <span class="hl num">256</span>


  <span class="hl kwa">let</span> buffer_len <span class="hl sym">=</span> <span class="hl num">1_000_000</span>

  <span class="hl kwa">let</span> buffers <span class="hl sym">=</span> <span class="hl kwc">Array</span><span class="hl sym">.</span>init nbuffers <span class="hl sym">(</span><span class="hl kwa">fun</span> _ <span class="hl sym">-&gt;</span> <span class="hl kwc">String</span><span class="hl sym">.</span>create buffer_len<span class="hl sym">)</span>
  <span class="hl kwa">let</span> buffer_pos <span class="hl sym">=</span> <span class="hl kwc">Array</span><span class="hl sym">.</span>create <span class="hl num">256</span> buffer_len
  <span class="hl kwa">let</span> last_buffer <span class="hl sym">=</span> <span class="hl kwb">ref</span> <span class="hl num">0</span>

  <span class="hl kwa">let</span> clear <span class="hl sym">() =</span>
    last_buffer <span class="hl sym">:=</span> <span class="hl num">0</span><span class="hl sym">;</span>
    <span class="hl kwa">for</span> i <span class="hl sym">=</span> <span class="hl num">0</span> <span class="hl kwa">to</span> <span class="hl num">255</span> <span class="hl kwa">do</span>
      buffer_pos<span class="hl sym">.(</span>i<span class="hl sym">) &lt;-</span> buffer_len<span class="hl sym">;</span>
    <span class="hl kwa">done</span>

  <span class="hl kwa">let</span> length <span class="hl sym">() =</span>
    <span class="hl sym">!</span>last_buffer <span class="hl sym">*</span> buffer_len <span class="hl sym">+ (</span>buffer_len <span class="hl sym">-</span> buffer_pos<span class="hl sym">.(!</span>last_buffer<span class="hl sym">))</span>

  <span class="hl kwa">let rec</span> blit_rev src end_pos dst dpos len <span class="hl sym">=</span>
    <span class="hl kwa">if</span> len <span class="hl sym">&gt;</span> <span class="hl num">0</span> <span class="hl kwa">then begin</span>
      dst<span class="hl sym">.[</span>dpos<span class="hl sym">] &lt;-</span> t<span class="hl sym">.(</span><span class="hl kwc">Char</span><span class="hl sym">.</span>code  src<span class="hl sym">.[</span>end_pos<span class="hl sym">]);</span>
      blit_rev src <span class="hl sym">(</span>end_pos<span class="hl sym">-</span><span class="hl num">1</span><span class="hl sym">)</span> dst <span class="hl sym">(</span>dpos<span class="hl sym">+</span><span class="hl num">1</span><span class="hl sym">) (</span>len<span class="hl sym">-</span><span class="hl num">1</span><span class="hl sym">)</span>
    <span class="hl kwa">end</span>

