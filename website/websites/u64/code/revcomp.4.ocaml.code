      <span class="hl kwa">let</span> b <span class="hl sym">=</span> buffers<span class="hl sym">.(!</span>last_buffer<span class="hl sym">)</span> <span class="hl kwa">in</span>
      <span class="hl kwa">let</span> bpos <span class="hl sym">=</span> buffer_pos<span class="hl sym">.(!</span>last_buffer<span class="hl sym">)</span> <span class="hl kwa">in</span>
