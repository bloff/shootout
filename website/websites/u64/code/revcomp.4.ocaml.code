      <span class="hl kwa">let</span> to_read <span class="hl sym">=</span> maxlen <span class="hl sym">-</span> pos <span class="hl kwa">in</span>
	<span class="hl kwa">if</span> to_read <span class="hl sym">&lt;</span> <span class="hl num">32000</span> <span class="hl kwa">then begin</span>
	  <span class="hl kwa">let</span> len <span class="hl sym">=</span> pos <span class="hl sym">-</span> begin_pos <span class="hl kwa">in</span>
	    <span class="hl kwc">String</span><span class="hl sym">.</span>blit s begin_pos s <span class="hl num">0</span> len<span class="hl sym">;</span>
	    iter <span class="hl num">0</span> len
