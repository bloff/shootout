    <span class="hl kwc">LineReader</span><span class="hl sym">.</span>read <span class="hl kwc">Unix</span><span class="hl sym">.</span>stdin <span class="hl num">1_000_000</span>
      <span class="hl sym">(</span><span class="hl kwa">fun</span> s pos len <span class="hl sym">-&gt;</span>
	 <span class="hl kwa">if</span> s<span class="hl sym">.[</span>pos<span class="hl sym">] =</span> '<span class="hl sym">&gt;</span>' <span class="hl kwa">then begin</span>
	   reverse <span class="hl sym">();</span>
