	<span class="hl kwa">if</span> nread <span class="hl sym">=</span> <span class="hl num">0</span> <span class="hl kwa">then</span> raise <span class="hl kwd">End_of_file</span><span class="hl sym">;</span>
	<span class="hl kwa">let</span> end_pos <span class="hl sym">=</span> pos <span class="hl sym">+</span> nread <span class="hl kwa">in</span>
	iter2 begin_pos pos end_pos

    <span class="hl kwa">and</span> iter2 begin_pos pos end_pos <span class="hl sym">=</span>
      <span class="hl kwa">if</span> pos <span class="hl sym">=</span> end_pos <span class="hl kwa">then</span>
	iter begin_pos end_pos
      <span class="hl kwa">else</span>
	<span class="hl kwa">match</span> s<span class="hl sym">.[</span>pos<span class="hl sym">]</span> <span class="hl kwa">with</span>
