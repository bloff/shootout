    <span class="hl com">(* max_line_length : *)</span> <span class="hl kwb">int</span> <span class="hl sym">-&gt;</span>
    <span class="hl com">(* handler : *)</span> <span class="hl sym">(</span><span class="hl kwb">string</span> <span class="hl sym">-&gt;</span> <span class="hl kwb">int</span> <span class="hl sym">-&gt;</span> <span class="hl kwb">int</span> <span class="hl sym">-&gt;</span> <span class="hl kwb">unit</span><span class="hl sym">) -&gt;</span> <span class="hl kwb">unit</span>

