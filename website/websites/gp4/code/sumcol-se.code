<span class="kwa">class</span> SUMCOL

creation make

<span class="kwa">feature

</span>   make <span class="kwa">is

      local
</span>	 sum<span class="sym">:</span> <span class="kwb">INTEGER</span><span class="sym">;
</span>      <span class="kwa">do
</span>	 sum <span class="sym">:=</span> <span class="num">0
</span>	 <span class="kwa">from
</span>	    io.read_line
	 until
	    io.end_of_input
	 <span class="kwa">loop
</span>	    sum <span class="sym">:=</span> sum <span class="sym">+</span> io.last_string.to_integer
	    io.read_line
	 <span class="kwa">end
</span>         std_output.put_integer<span class="sym">(</span>sum<span class="sym">)
</span>         std_output.put_character<span class="sym">(</span><span class="str">'%N'</span><span class="sym">)
</span>      <span class="kwa">end
end</span>
