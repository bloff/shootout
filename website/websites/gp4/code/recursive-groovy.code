def <span class="kwd">ack</span><span class="sym">(</span>x<span class="sym">,</span> y<span class="sym">)</span>
<span class="sym">{</span>
  <span class="kwa">if</span> <span class="sym">(</span>x <span class="sym">==</span> <span class="num">0</span><span class="sym">)</span> <span class="kwa">return</span> y <span class="sym">+</span> <span class="num">1</span>
  <span class="kwa">if</span> <span class="sym">(</span>y <span class="sym">==</span> <span class="num">0</span><span class="sym">)</span> <span class="kwa">return</span> <span class="kwd">ack</span><span class="sym">(</span>x <span class="sym">-</span> <span class="num">1</span><span class="sym">,</span> <span class="num">1</span><span class="sym">)</span>
  <span class="kwa">return</span> <span class="kwd">ack</span><span class="sym">(</span>x <span class="sym">-</span> <span class="num">1</span><span class="sym">,</span> <span class="kwd">ack</span><span class="sym">(</span>x<span class="sym">,</span> y <span class="sym">-</span> <span class="num">1</span><span class="sym">))</span>
<span class="sym">}</span>

<span class="slc">// --------------</span>

def <span class="kwd">fib</span><span class="sym">(</span><span class="kwb">int</span> n<span class="sym">)</span>
<span class="sym">{</span>
  <span class="kwa">if</span> <span class="sym">(</span>n <span class="sym">&lt;</span> <span class="num">2</span>I<span class="sym">)</span> <span class="kwa">return</span> <span class="num">1</span>I
  <span class="kwa">return</span> <span class="kwd">fib</span><span class="sym">(</span>n <span class="sym">-</span> <span class="num">2</span>I<span class="sym">) +</span> <span class="kwd">fib</span><span class="sym">(</span>n <span class="sym">-</span> <span class="num">1</span>I<span class="sym">)</span>
<span class="sym">}</span>

def <span class="kwd">fib</span><span class="sym">(</span><span class="kwb">double</span> n<span class="sym">)</span>
<span class="sym">{</span>
  <span class="kwa">if</span> <span class="sym">(</span>n <span class="sym">&lt;</span> <span class="num">2.0</span>D<span class="sym">)</span> <span class="kwa">return</span> <span class="num">1.0</span>D
  <span class="kwa">return</span> <span class="kwd">fib</span><span class="sym">(</span>n <span class="sym">-</span> <span class="num">2.0</span>D<span class="sym">) +</span> <span class="kwd">fib</span><span class="sym">(</span>n <span class="sym">-</span> <span class="num">1.0</span>D<span class="sym">)</span>
<span class="sym">}</span>

<span class="slc">// --------------</span>

def <span class="kwd">tak</span><span class="sym">(</span><span class="kwb">int</span> x<span class="sym">,</span> <span class="kwb">int</span> y<span class="sym">,</span> <span class="kwb">int</span> z<span class="sym">)</span>
<span class="sym">{</span>
  <span class="kwa">if</span> <span class="sym">(</span>y <span class="sym">&lt;</span> x<span class="sym">)</span> <span class="kwa">return</span> <span class="kwd">tak</span><span class="sym">(</span><span class="kwd">tak</span><span class="sym">(</span>x <span class="sym">-</span> <span class="num">1</span>I<span class="sym">,</span> y<span class="sym">,</span> z<span class="sym">),</span> <span class="kwd">tak</span><span class="sym">(</span>y <span class="sym">-</span> <span class="num">1</span>I<span class="sym">,</span> z<span class="sym">,</span> x<span class="sym">),</span> <span class="kwd">tak</span><span class="sym">(</span>z <span class="sym">-</span> <span class="num">1</span>I<span class="sym">,</span> x<span class="sym">,</span> y<span class="sym">))</span>
  <span class="kwa">return</span> z
<span class="sym">}</span>

def <span class="kwd">tak</span><span class="sym">(</span><span class="kwb">double</span> x<span class="sym">,</span> <span class="kwb">double</span> y<span class="sym">,</span> <span class="kwb">double</span> z<span class="sym">)</span>
<span class="sym">{</span>
  <span class="kwa">if</span> <span class="sym">(</span>y <span class="sym">&lt;</span> x<span class="sym">)</span> <span class="kwa">return</span> <span class="kwd">tak</span><span class="sym">(</span><span class="kwd">tak</span><span class="sym">(</span>x <span class="sym">-</span> <span class="num">1.0</span>D<span class="sym">,</span> y<span class="sym">,</span> z<span class="sym">),</span> <span class="kwd">tak</span><span class="sym">(</span>y <span class="sym">-</span> <span class="num">1.0</span>D<span class="sym">,</span> z<span class="sym">,</span> x<span class="sym">),</span> <span class="kwd">tak</span><span class="sym">(</span>z <span class="sym">-</span> <span class="num">1.0</span>D<span class="sym">,</span> x<span class="sym">,</span> y<span class="sym">))</span>
  <span class="kwa">return</span> z
<span class="sym">}</span>

