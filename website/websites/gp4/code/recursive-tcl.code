<span class="slc"># The Computer Language Shootout</span>
<span class="slc"># http://shootout.alioth.debian.org/</span>
<span class="slc"># contributed by Andrew McParland, based on code from the Doug Bagley benchmarks</span>

<span class="kwa">interp</span> recursionlimit <span class="sym">{}</span> <span class="num">20000</span>

<span class="slc"># Note reversal of arguments</span>
<span class="kwa">proc</span> ack <span class="sym">{</span>n m<span class="sym">} {</span>
    <span class="kwa">if</span> <span class="sym">{</span><span class="kwb">$m</span><span class="sym">} {</span>
        <span class="kwa">if</span> <span class="sym">{</span><span class="kwb">$n</span><span class="sym">} {</span>
            <span class="kwa">return</span> <span class="sym">[</span>ack <span class="sym">[</span>ack <span class="sym">[</span><span class="kwa">incr</span> n <span class="sym">-</span><span class="num">1</span><span class="sym">]</span> <span class="kwb">$m</span><span class="sym">] [</span><span class="kwa">incr</span> m <span class="sym">-</span><span class="num">1</span><span class="sym">]]</span>
        <span class="sym">}</span> else <span class="sym">{</span>
            <span class="kwa">return</span> <span class="sym">[</span>ack <span class="num">1</span> <span class="sym">[</span><span class="kwa">incr</span> m <span class="sym">-</span><span class="num">1</span><span class="sym">]]</span>
        <span class="sym">}</span>
    <span class="sym">}</span> else <span class="sym">{</span>
        <span class="kwa">return</span> <span class="sym">[</span><span class="kwa">incr</span> n<span class="sym">]</span>
    <span class="sym">}</span>
<span class="sym">}</span>

<span class="kwa">proc</span> fib <span class="sym">{</span>n<span class="sym">} {</span>
    <span class="kwa">if</span> <span class="sym">{</span><span class="kwb">$n</span> <span class="sym">&lt;</span> <span class="num">2</span><span class="sym">} {</span>
        <span class="kwa">return</span> <span class="num">1</span>
    <span class="sym">}</span> else <span class="sym">{</span>
        <span class="kwa">return</span> <span class="sym">[</span><span class="kwa">expr</span> <span class="sym">{ [</span>fib <span class="sym">[</span><span class="kwa">expr</span> <span class="sym">{</span><span class="kwb">$n</span><span class="sym">-</span><span class="num">2</span><span class="sym">}]] + [</span>fib <span class="sym">[</span><span class="kwa">expr</span> <span class="sym">{</span><span class="kwb">$n</span><span class="sym">-</span><span class="num">1</span><span class="sym">}]] } ]</span>
    <span class="sym">}</span>
<span class="sym">}</span>

<span class="kwa">proc</span> tak <span class="sym">{</span>x y z<span class="sym">} {</span>
    <span class="kwa">if</span> <span class="sym">{</span><span class="kwb">$y</span> <span class="sym">&gt;=</span> <span class="kwb">$x</span><span class="sym">} {</span>
        <span class="kwa">return</span> <span class="kwb">$z</span>
    <span class="sym">}</span> else <span class="sym">{</span>
        <span class="kwa">return</span> <span class="sym">[</span>tak <span class="sym">[</span>tak <span class="sym">[</span><span class="kwa">expr</span> <span class="sym">{</span><span class="kwb">$x</span><span class="sym">-</span><span class="num">1</span><span class="sym">}]</span> <span class="kwb">$y $z</span><span class="sym">] [</span>tak <span class="sym">[</span><span class="kwa">expr</span> <span class="sym">{</span><span class="kwb">$y</span><span class="sym">-</span><span class="num">1</span><span class="sym">}]</span> <span class="kwb">$z $x</span><span class="sym">] [</span>tak <span class="sym">[</span><span class="kwa">expr</span> <span class="sym">{</span><span class="kwb">$z</span><span class="sym">-</span><span class="num">1</span><span class="sym">}]</span> <span class="kwb">$x $y</span><span class="sym">]]</span>
    <span class="sym">}</span>
<span class="sym">}</span>

<span class="kwa">set</span> n <span class="sym">[</span><span class="kwa">expr</span> <span class="sym">[</span><span class="kwa">lindex</span> <span class="kwb">$argv</span> <span class="num">0</span><span class="sym">] -</span> <span class="num">1</span><span class="sym">]</span>

<span class="kwa">puts</span> <span class="sym">[</span><span class="kwa">format</span> <span class="str">&quot;Ack(3,%d): %d&quot;</span> <span class="sym">[</span><span class="kwa">expr</span> <span class="kwb">$n</span><span class="sym">+</span><span class="num">1</span><span class="sym">] [</span>ack <span class="sym">[</span><span class="kwa">expr</span> <span class="kwb">$n</span><span class="sym">+</span><span class="num">1</span><span class="sym">]</span> <span class="num">3</span><span class="sym">]]</span>
<span class="kwa">puts</span> <span class="sym">[</span><span class="kwa">format</span> <span class="str">&quot;Fib(%.1f): %.1f&quot;</span> <span class="sym">[</span><span class="kwa">expr</span> <span class="kwb">$n</span><span class="sym">+</span><span class="num">28.0</span><span class="sym">] [</span>fib <span class="sym">[</span><span class="kwa">expr</span> <span class="kwb">$n</span><span class="sym">+</span><span class="num">28.0</span><span class="sym">]]]</span>
<span class="kwa">set</span> n3 <span class="sym">[</span><span class="kwa">expr</span> <span class="kwb">$n</span><span class="sym">*</span><span class="num">3</span><span class="sym">];</span> <span class="kwa">set</span> n2 <span class="sym">[</span><span class="kwa">expr</span> <span class="kwb">$n</span><span class="sym">*</span><span class="num">2</span><span class="sym">]</span>
<span class="kwa">puts</span> <span class="sym">[</span><span class="kwa">format</span> <span class="str">&quot;Tak(%d,%d,%d): %d&quot;</span> <span class="kwb">$n3 $n2 $n</span> <span class="sym">[</span>tak <span class="kwb">$n3 $n2 $n</span><span class="sym">]]</span>
<span class="kwa">puts</span> <span class="sym">[</span><span class="kwa">format</span> <span class="str">&quot;Fib(3): %d&quot;</span> <span class="sym">[</span>fib <span class="num">3</span><span class="sym">]]</span>
<span class="kwa">puts</span> <span class="sym">[</span><span class="kwa">format</span> <span class="str">&quot;Tak(3.0,2.0,1.0): %.1f&quot;</span> <span class="sym">[</span>tak <span class="num">3.0 2.0 1.0</span><span class="sym">]]</span>
