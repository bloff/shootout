   <span class="kwa">while</span> <span class="sym">(</span>k<span class="sym">++ &lt;</span> n<span class="sym">)</span> sum <span class="sym">+= (</span>a<span class="sym">= -</span>a<span class="sym">)/(</span><span class="num">2</span><span class="sym">*</span>k <span class="sym">-</span><span class="num">1</span><span class="sym">);</span>
