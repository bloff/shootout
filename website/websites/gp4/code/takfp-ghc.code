<span class="kwa">import</span> System<span class="sym">(</span>getArgs<span class="sym">)</span>

main <span class="sym">=</span> <span class="kwa">do</span> n <span class="sym">&lt;-</span> getArgs <span class="sym">&gt;&gt;=</span> readIO.head
          putStrLn $ show $ tak <span class="sym">(</span><span class="num">3</span><span class="sym">*</span>n<span class="sym">) (</span><span class="num">2</span><span class="sym">*</span>n<span class="sym">)</span> n

tak <span class="sym">::</span> <span class="kwb">Float</span> <span class="sym">-&gt;</span> <span class="kwb">Float</span> <span class="sym">-&gt;</span> <span class="kwb">Float</span> <span class="sym">-&gt;</span> <span class="kwb">Float</span>
tak x y z <span class="sym">|</span> y<span class="sym">&gt;=</span>x      <span class="sym">=</span> z
          <span class="sym">|</span> otherwise <span class="sym">=</span> tak <span class="sym">(</span>tak <span class="sym">(</span>x<span class="sym">-</span><span class="num">1</span><span class="sym">)</span> y z<span class="sym">) (</span>tak <span class="sym">(</span>y<span class="sym">-</span><span class="num">1</span><span class="sym">)</span> z x<span class="sym">) (</span>tak <span class="sym">(</span>z<span class="sym">-</span><span class="num">1</span><span class="sym">)</span> x y<span class="sym">)</span>
