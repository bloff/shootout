    tot <span class="sym">=</span> <span class="num">0.0</span>
    <span class="kwb">for</span> k <span class="kwb">in</span> <span class="kwa">range</span><span class="sym">(</span><span class="num">1</span><span class="sym">,</span> n<span class="sym">+</span><span class="num">1</span><span class="sym">):</span>
        sk <span class="sym">=</span> <span class="kwd">sin</span><span class="sym">(</span>k<span class="sym">)</span>
        tot <span class="sym">+=</span> <span class="num">1.0</span> <span class="sym">/ (</span>k<span class="sym">*</span>k<span class="sym">*</span>k <span class="sym">*</span> sk<span class="sym">*</span>sk<span class="sym">)</span>
    <span class="kwb">print</span> <span class="str">&quot;%.9f</span><span class="esc">\t</span><span class="str">Flint Hills&quot;</span> <span class="sym">%</span> tot

    tot <span class="sym">=</span> <span class="num">0.0</span>
    <span class="kwb">for</span> k <span class="kwb">in</span> <span class="kwa">range</span><span class="sym">(</span><span class="num">1</span><span class="sym">,</span> n<span class="sym">+</span><span class="num">1</span><span class="sym">):</span>
        ck <span class="sym">=</span> <span class="kwd">cos</span><span class="sym">(</span>k<span class="sym">)</span>
        tot <span class="sym">+=</span> <span class="num">1.0</span> <span class="sym">/ (</span>k<span class="sym">*</span>k<span class="sym">*</span>k <span class="sym">*</span> ck<span class="sym">*</span>ck<span class="sym">)</span>
    <span class="kwb">print</span> <span class="str">&quot;%.9f</span><span class="esc">\t</span><span class="str">Cookson Hills&quot;</span> <span class="sym">%</span> tot

    tot <span class="sym">=</span> <span class="num">0.0</span>
    <span class="kwb">for</span> k <span class="kwb">in</span> <span class="kwa">range</span><span class="sym">(</span><span class="num">1</span><span class="sym">,</span> n<span class="sym">+</span><span class="num">1</span><span class="sym">):</span> tot <span class="sym">+=</span> <span class="num">1.0</span><span class="sym">/</span>k
    <span class="kwb">print</span> <span class="str">&quot;%.9f</span><span class="esc">\t</span><span class="str">Harmonic&quot;</span> <span class="sym">%</span> tot

    tot <span class="sym">=</span> <span class="num">0.0</span>
