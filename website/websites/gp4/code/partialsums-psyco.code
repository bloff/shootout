    <span class="kwb">print</span> <span class="str">&quot;%.9f</span><span class="esc">\t</span><span class="str">Riemann Zeta&quot;</span> <span class="sym">%</span> tot

    tot <span class="sym">=</span> <span class="num">0.0</span>
    <span class="kwb">for</span> k <span class="kwb">in</span> <span class="kwa">range</span><span class="sym">(</span><span class="num">1</span><span class="sym">,</span> n<span class="sym">,</span> <span class="num">2</span><span class="sym">):</span> tot <span class="sym">+=</span> <span class="num">1.0</span><span class="sym">/</span>k
