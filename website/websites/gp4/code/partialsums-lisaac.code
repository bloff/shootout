    <span class="hl sym">+</span> a6<span class="hl sym">,</span> a7<span class="hl sym">,</span> a8<span class="hl sym">,</span> a9<span class="hl sym">,</span> alt  <span class="hl sym">:</span>REAL_64<span class="hl sym">;</span>
