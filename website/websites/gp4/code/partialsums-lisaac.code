      a8 <span class="hl sym">:=</span> a8 <span class="hl sym">+</span> alt<span class="hl sym">/</span>k.to_real_64<span class="hl sym">;</span>
      alt <span class="hl sym">:= -</span>alt<span class="hl sym">;</span>
    <span class="hl sym">};</span>

