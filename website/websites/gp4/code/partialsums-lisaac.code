      a7 <span class="hl sym">:=</span> a7 <span class="hl sym">+</span> <span class="hl num">1</span>.to_real_64<span class="hl sym">/(</span>k <span class="hl sym">*</span> k<span class="hl sym">);</span>
    <span class="hl sym">};</span>

