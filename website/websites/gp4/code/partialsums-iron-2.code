<span class="hl kwa">from</span> sys <span class="hl kwa">import</span> argv
<span class="hl kwa">from</span> math <span class="hl kwa">import</span> sin <span class="hl kwa">as</span> _sin<span class="hl sym">,</span> cos <span class="hl kwa">as</span> _cos

<span class="hl kwa">def</span> <span class="hl kwd">doit</span><span class="hl sym">():</span>
    n <span class="hl sym">= (</span><span class="hl kwb">len</span><span class="hl sym">(</span>argv<span class="hl sym">) &gt;</span> <span class="hl num">1</span><span class="hl sym">)</span> <span class="hl kwa">and</span> <span class="hl kwb">int</span><span class="hl sym">(</span>argv<span class="hl sym">[</span><span class="hl num">1</span><span class="hl sym">])</span> <span class="hl kwa">or</span> <span class="hl num">25000</span>

