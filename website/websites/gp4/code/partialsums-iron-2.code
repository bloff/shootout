    sum1 <span class="hl sym">=</span> sum2 <span class="hl sym">=</span> sum3 <span class="hl sym">=</span> sum4 <span class="hl sym">=</span> sum5 <span class="hl sym">=</span> sum6 <span class="hl sym">=</span> sum7 <span class="hl sym">=</span> sum8 <span class="hl sym">=</span> <span class="hl num">0.0</span>
    twoThirds <span class="hl sym">=</span> <span class="hl num">2.0</span> <span class="hl sym">/</span> <span class="hl num">3.0</span>

    <span class="hl kwa">for</span> k <span class="hl kwa">in</span> <span class="hl kwb">xrange</span><span class="hl sym">(</span><span class="hl num">1</span><span class="hl sym">,</span> n <span class="hl sym">+</span> <span class="hl num">1</span><span class="hl sym">):</span>
