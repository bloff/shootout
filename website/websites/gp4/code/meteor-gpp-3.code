TUInt32 <span class="kwd">getFirstOne</span><span class="sym">(</span><span class="kwb">const</span> BitVec <span class="sym">&amp;</span> v<span class="sym">,</span> TUInt32 startPos <span class="sym">=</span> <span class="num">0</span><span class="sym">) {</span>
   <span class="kwa">if</span> <span class="sym">(</span>v <span class="sym">== (</span>BitVec<span class="sym">)</span><span class="num">0</span><span class="sym">)</span>
      <span class="kwa">return</span> <span class="num">0</span><span class="sym">;</span>

