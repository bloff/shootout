<span class="kwb">void</span> Piece<span class="sym">::</span><span class="kwd">shiftUpLines</span><span class="sym">(</span>TCoordList <span class="sym">&amp;</span> coords<span class="sym">,</span> <span class="kwb">int</span> shift<span class="sym">) {</span>
   <span class="slc">// shifts are not so simple in the vertical direction</span>
   <span class="kwa">for</span> <span class="sym">(</span><span class="kwb">int</span> iCoord <span class="sym">=</span> <span class="num">0</span><span class="sym">;</span> iCoord <span class="sym">&lt;</span> N_ELEM<span class="sym">;</span> iCoord<span class="sym">++) {</span>
      <span class="kwb">int</span> <span class="sym">&amp;</span> rx <span class="sym">=</span> coords<span class="sym">[</span>iCoord<span class="sym">][</span>X<span class="sym">];</span>
      <span class="kwb">int</span> <span class="sym">&amp;</span> ry <span class="sym">=</span> coords<span class="sym">[</span>iCoord<span class="sym">][</span>Y<span class="sym">];</span>

