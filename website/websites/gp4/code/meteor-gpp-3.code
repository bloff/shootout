   <span class="kwd">memset</span><span class="sym">(&amp;</span>m_cells<span class="sym">[</span><span class="num">0</span><span class="sym">][</span><span class="num">0</span><span class="sym">],</span> val<span class="sym">,</span> N_CELL<span class="sym">);</span>
<span class="sym">}</span>

string Solution<span class="sym">::</span><span class="kwd">toString</span><span class="sym">(</span><span class="kwb">void</span><span class="sym">)</span> <span class="kwb">const</span>
<span class="sym">{</span>
   string result<span class="sym">;</span>
   result<span class="sym">.</span><span class="kwd">reserve</span><span class="sym">(</span>N_CELL <span class="sym">*</span> <span class="num">2</span><span class="sym">);</span>

   <span class="kwa">for</span> <span class="sym">(</span><span class="kwb">int</span> y <span class="sym">=</span> <span class="num">0</span><span class="sym">;</span> y <span class="sym">&lt;</span> N_ROW<span class="sym">;</span> y<span class="sym">++) {</span>
      <span class="kwa">for</span> <span class="sym">(</span><span class="kwb">int</span> x <span class="sym">=</span> <span class="num">0</span><span class="sym">;</span> x <span class="sym">&lt;</span> N_COL<span class="sym">;</span> x<span class="sym">++) {</span>
         <span class="kwb">int</span> val <span class="sym">=</span> m_cells<span class="sym">[</span>y<span class="sym">][</span>x<span class="sym">];</span>
         result <span class="sym">+= ((</span>val <span class="sym">==</span> NO_PIECE<span class="sym">)</span> ? <span class="str">'.'</span> <span class="sym">:</span> <span class="kwb">char</span><span class="sym">(</span><span class="str">'0'</span> <span class="sym">+</span> val<span class="sym">));</span>
         result <span class="sym">+=</span> <span class="str">' '</span><span class="sym">;</span>
      <span class="sym">}</span>
      result <span class="sym">+=</span> <span class="str">'</span><span class="esc">\n</span><span class="str">'</span><span class="sym">;</span>

      <span class="slc">// indent every second line</span>
      <span class="kwa">if</span> <span class="sym">(</span>y <span class="sym">%</span> <span class="num">2</span> <span class="sym">==</span> <span class="num">0</span><span class="sym">)</span>
         result <span class="sym">+=</span> <span class="str">&quot; &quot;</span><span class="sym">;</span>
   <span class="sym">}</span>

   <span class="kwa">return</span> result<span class="sym">;</span> <span class="slc">// copies result. Oh well</span>
<span class="sym">}</span>

<span class="kwb">void</span> Solution<span class="sym">::</span><span class="kwd">setCells</span><span class="sym">(</span><span class="kwb">void</span><span class="sym">)</span>
<span class="sym">{</span>
