<span class="kwb">void</span> Piece<span class="sym">::</span><span class="kwd">setCoordList</span><span class="sym">(</span><span class="kwb">const</span> BitVec <span class="sym">&amp;</span> vec<span class="sym">,</span> TCoordList <span class="sym">&amp;</span> coords<span class="sym">) {</span>
