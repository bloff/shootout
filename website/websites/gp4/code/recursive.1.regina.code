<span class="com">/* ----------------------------- */</span>

ack <span class="sym">:</span>
  <span class="kwa">if</span> <span class="kwd">ARG</span><span class="sym">(</span><span class="num">1</span><span class="sym">) ==</span> <span class="num">0</span> then <span class="sym">;</span> <span class="kwa">return</span> <span class="kwd">ARG</span><span class="sym">(</span><span class="num">2</span><span class="sym">) +</span> <span class="num">1</span>
  <span class="kwa">if</span> <span class="kwd">ARG</span><span class="sym">(</span><span class="num">2</span><span class="sym">) ==</span> <span class="num">0</span> then <span class="sym">;</span> <span class="kwa">return</span> <span class="kwd">ack</span><span class="sym">(</span><span class="kwd">ARG</span><span class="sym">(</span><span class="num">1</span><span class="sym">) -</span> <span class="num">1</span><span class="sym">,</span> <span class="num">1</span><span class="sym">)</span>
  <span class="kwa">return</span> <span class="kwd">ack</span><span class="sym">(</span><span class="kwd">ARG</span><span class="sym">(</span><span class="num">1</span><span class="sym">) -</span> <span class="num">1</span><span class="sym">,</span> <span class="kwd">ack</span><span class="sym">(</span><span class="kwd">ARG</span><span class="sym">(</span><span class="num">1</span><span class="sym">),</span> <span class="kwd">ARG</span><span class="sym">(</span><span class="num">2</span><span class="sym">) -</span> <span class="num">1</span><span class="sym">))</span>

<span class="com">/* ----------- */</span>

fib <span class="sym">:</span>
  <span class="kwa">if</span> <span class="kwd">ARG</span><span class="sym">(</span><span class="num">1</span><span class="sym">) &lt;</span> <span class="num">2</span> then <span class="sym">;</span> <span class="kwa">return</span> <span class="num">1</span>
  <span class="kwa">return</span> <span class="kwd">fib</span><span class="sym">(</span><span class="kwd">ARG</span><span class="sym">(</span><span class="num">1</span><span class="sym">) -</span> <span class="num">2</span><span class="sym">) +</span> <span class="kwd">fib</span><span class="sym">(</span><span class="kwd">ARG</span><span class="sym">(</span><span class="num">1</span><span class="sym">) -</span> <span class="num">1</span><span class="sym">)</span>

<span class="com">/* ----------- */</span>

tak <span class="sym">:</span>
