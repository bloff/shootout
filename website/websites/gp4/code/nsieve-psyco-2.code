<span class="kwc">import</span> sys<span class="sym">,</span> psyco

<span class="kwb">def</span> <span class="kwd">nsieve</span><span class="sym">(</span>n<span class="sym">):</span>
    flags <span class="sym">= [</span>True<span class="sym">] * (</span>n<span class="sym">+</span><span class="num">1</span><span class="sym">)</span>
    count<span class="sym">,</span> n1 <span class="sym">=</span> <span class="num">0</span><span class="sym">,</span> n<span class="sym">+</span><span class="num">1</span>
    <span class="kwb">for</span> i <span class="kwb">in</span> <span class="kwa">xrange</span><span class="sym">(</span><span class="num">2</span><span class="sym">,</span> n1<span class="sym">):</span>
        <span class="kwb">if</span> flags<span class="sym">[</span>i<span class="sym">]:</span>
            count <span class="sym">+=</span> <span class="num">1</span>
            k <span class="sym">=</span> i <span class="sym">&lt;&lt;</span> <span class="num">1</span>
            <span class="kwb">while</span> k <span class="sym">&lt;</span> n1<span class="sym">:</span>
                <span class="kwb">if</span> flags<span class="sym">[</span>k<span class="sym">]:</span> flags<span class="sym">[</span>k<span class="sym">] =</span> False
                k <span class="sym">+=</span> i
    <span class="kwb">print</span> <span class="str">&quot;Primes up to %8u %8u&quot;</span> <span class="sym">% (</span>n<span class="sym">,</span> count<span class="sym">)</span>

psyco<span class="sym">.</span><span class="kwd">bind</span><span class="sym">(</span>nsieve<span class="sym">)</span>
m <span class="sym">=</span> <span class="kwa">int</span><span class="sym">(</span>sys<span class="sym">.</span>argv<span class="sym">[</span><span class="num">1</span><span class="sym">])</span>
<span class="kwb">for</span> i <span class="kwb">in</span> <span class="num">0</span><span class="sym">,</span> <span class="num">1</span><span class="sym">,</span> <span class="num">2</span><span class="sym">:</span>
    <span class="kwd">nsieve</span><span class="sym">(</span><span class="num">10000</span> <span class="sym">&lt;&lt; (</span>m<span class="sym">-</span>i<span class="sym">))</span>
