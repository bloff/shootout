		<span class="kwa">for</span><span class="sym">(</span><span class="kwb">int</span> i <span class="sym">=</span> <span class="num">0</span><span class="sym">;</span> i <span class="sym">&lt;</span> CellsPerBoard<span class="sym">; ++</span>i<span class="sym">)</span>
			cells<span class="sym">[</span>i<span class="sym">].</span><span class="kwd">setPlace</span><span class="sym">(</span>i<span class="sym">,</span> cells<span class="sym">);</span>
