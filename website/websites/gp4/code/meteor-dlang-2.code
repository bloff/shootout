	<span class="sym">}</span>
<span class="sym">}</span>

<span class="kwc">class</span> PieceCell <span class="sym">{</span>
	PieceCell<span class="sym">[</span><span class="num">6</span><span class="sym">]</span> around<span class="sym">;</span>
	<span class="kwb">bool</span> marked<span class="sym">;</span>

