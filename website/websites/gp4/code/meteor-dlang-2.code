	<span class="sym">}</span>

	<span class="slc">/// attempt to put a piececell in a boardcell</span>
	<span class="kwb">bool</span> <span class="kwd">put</span><span class="sym">(</span>PieceConfig pc<span class="sym">,</span> uint pieceIndex<span class="sym">,</span> uint place<span class="sym">)</span>
	in <span class="sym">{</span>
