<span class="hl kwa">import</span> sys

<span class="hl kwa">def</span> <span class="hl kwd">main</span><span class="hl sym">():</span>
    <span class="hl kwa">from</span> math <span class="hl kwa">import</span> sin<span class="hl sym">,</span> cos
    n <span class="hl sym">=</span> <span class="hl kwb">int</span><span class="hl sym">(</span>sys<span class="hl sym">.</span>argv<span class="hl sym">[</span><span class="hl num">1</span><span class="hl sym">])</span>

    a1 <span class="hl sym">=</span> alt <span class="hl sym">=</span> <span class="hl num">1</span><span class="hl sym">.</span>
    a2 <span class="hl sym">=</span> a3 <span class="hl sym">=</span> a4 <span class="hl sym">=</span> a5 <span class="hl sym">=</span> a6 <span class="hl sym">=</span> a7 <span class="hl sym">=</span> a8 <span class="hl sym">=</span> a9 <span class="hl sym">=</span> <span class="hl num">0</span><span class="hl sym">.</span>
    twoThirds <span class="hl sym">=</span> <span class="hl num">2</span><span class="hl sym">. /</span> <span class="hl num">3</span><span class="hl sym">.</span>

    <span class="hl kwa">for</span> k <span class="hl kwa">in</span> <span class="hl kwb">xrange</span><span class="hl sym">(</span><span class="hl num">1</span><span class="hl sym">,</span> n <span class="hl sym">+</span> <span class="hl num">1</span><span class="hl sym">):</span>
        k <span class="hl sym">=</span> <span class="hl kwb">float</span><span class="hl sym">(</span>k<span class="hl sym">)</span>
        ki <span class="hl sym">=</span> <span class="hl num">1</span><span class="hl sym">. /</span> k
        ks<span class="hl sym">,</span> kc <span class="hl sym">=</span> <span class="hl kwd">sin</span><span class="hl sym">(</span>k<span class="hl sym">),</span> <span class="hl kwd">cos</span><span class="hl sym">(</span>k<span class="hl sym">)</span>

        a1 <span class="hl sym">+=</span> twoThirds <span class="hl sym">**</span> k
        a2 <span class="hl sym">+=</span> ki <span class="hl sym">**</span> <span class="hl num">.5</span>
        a3 <span class="hl sym">+=</span> ki <span class="hl sym">/ (</span>k <span class="hl sym">+</span> <span class="hl num">1</span><span class="hl sym">.)</span>
        a4 <span class="hl sym">+=</span> ki <span class="hl sym">/</span> ks <span class="hl sym">* (</span>ki <span class="hl sym">/</span> ks<span class="hl sym">) /</span> k
        a5 <span class="hl sym">+=</span> ki <span class="hl sym">/</span> kc <span class="hl sym">* (</span>ki <span class="hl sym">/</span> kc<span class="hl sym">) /</span> k
        a6 <span class="hl sym">+=</span> ki
        a7 <span class="hl sym">+=</span> ki <span class="hl sym">/</span> k
        a8 <span class="hl sym">+=</span> alt <span class="hl sym">/</span> k
        a9 <span class="hl sym">+=</span> alt <span class="hl sym">/ (</span>k <span class="hl sym">+</span> k <span class="hl sym">-</span> <span class="hl num">1</span><span class="hl sym">.)</span>
        alt <span class="hl sym">= -</span>alt

    <span class="hl kwa">print</span> <span class="hl sym">(</span><span class="hl str">'%0.9f</span><span class="hl esc">\t</span><span class="hl str">(2/3)^k</span><span class="hl esc">\n</span><span class="hl str">'</span>
           <span class="hl str">'%0.9f</span><span class="hl esc">\t</span><span class="hl str">k^-0.5</span><span class="hl esc">\n</span><span class="hl str">'</span>
           <span class="hl str">'%0.9f</span><span class="hl esc">\t</span><span class="hl str">1/k(k+1)</span><span class="hl esc">\n</span><span class="hl str">'</span>
           <span class="hl str">'%0.9f</span><span class="hl esc">\t</span><span class="hl str">Flint Hills</span><span class="hl esc">\n</span><span class="hl str">'</span>
           <span class="hl str">'%0.9f</span><span class="hl esc">\t</span><span class="hl str">Cookson Hills</span><span class="hl esc">\n</span><span class="hl str">'</span>
           <span class="hl str">'%0.9f</span><span class="hl esc">\t</span><span class="hl str">Harmonic</span><span class="hl esc">\n</span><span class="hl str">'</span>
           <span class="hl str">'%0.9f</span><span class="hl esc">\t</span><span class="hl str">Riemann Zeta</span><span class="hl esc">\n</span><span class="hl str">'</span>
           <span class="hl str">'%0.9f</span><span class="hl esc">\t</span><span class="hl str">Alternating Harmonic</span><span class="hl esc">\n</span><span class="hl str">'</span>
           <span class="hl str">'%0.9f</span><span class="hl esc">\t</span><span class="hl str">Gregory'</span><span class="hl sym">) % (</span>a1<span class="hl sym">,</span> a2<span class="hl sym">,</span> a3<span class="hl sym">,</span> a4<span class="hl sym">,</span> a5<span class="hl sym">,</span> a6<span class="hl sym">,</span> a7<span class="hl sym">,</span> a8<span class="hl sym">,</span> a9<span class="hl sym">)</span>

<span class="hl kwd">main</span><span class="hl sym">()</span>
