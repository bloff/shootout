      <span class="kwa">j</span> p<span class="sym">&gt;</span>vz f<span class="sym">&#64;</span> fswap dz f<span class="sym">*</span> f<span class="sym">-</span> <span class="kwa">j</span> p<span class="sym">&gt;</span>vz f<span class="sym">!</span>
