Imports System

<span class="kwa">Public Module</span> Harmonic
   <span class="kwa">Sub</span> <span class="kwd">Main</span><span class="sym">(</span><span class="kwa">ByVal</span> <span class="kwd">cmdArgs</span><span class="sym">()</span> <span class="kwa">As</span> <span class="kwb">String</span><span class="sym">)</span>
      <span class="kwa">Dim</span> n <span class="kwa">As</span> <span class="kwb">Integer</span> <span class="sym">=</span> Int32.<span class="kwd">Parse</span><span class="sym">(</span><span class="kwd">cmdArgs</span><span class="sym">(</span><span class="num">0</span><span class="sym">))</span>
      <span class="kwa">Dim</span> partialSum <span class="kwa">As</span> <span class="kwb">Double</span> <span class="sym">=</span> <span class="num">0.0</span>
      <span class="kwa">For</span> i <span class="kwa">As</span> <span class="kwb">Integer</span> <span class="sym">=</span> <span class="num">1</span> <span class="kwa">To</span> n
         partialSum <span class="sym">+=</span> <span class="num">1.0</span><span class="sym">/</span>i
      <span class="kwa">Next</span>
      Console.<span class="kwd">WriteLine</span><span class="sym">(</span><span class="str">&quot;{0:f9}&quot;</span><span class="sym">,</span> partialSum<span class="sym">)</span>
   <span class="kwa">End Sub</span>
<span class="kwa">End Module</span>
