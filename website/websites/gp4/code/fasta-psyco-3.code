    line_length <span class="sym">=</span> <span class="num">60</span>
    s_length <span class="sym">=</span> <span class="kwa">len</span><span class="sym">(</span>s<span class="sym">)</span>
    wrap <span class="sym">=</span> <span class="num">0</span>

    <span class="kwb">while</span> todo <span class="sym">&gt;</span> <span class="num">0</span><span class="sym">:</span>
        <span class="kwb">if</span> todo <span class="sym">&gt;</span> line_length<span class="sym">:</span>
            out_length <span class="sym">=</span> line_length
        <span class="kwb">else</span><span class="sym">:</span>
            out_length <span class="sym">=</span> todo

        <span class="kwb">while</span> out_length <span class="sym">&gt;= (</span>s_length <span class="sym">-</span> wrap<span class="sym">):</span>
            stdout<span class="sym">.</span><span class="kwd">write</span><span class="sym">(</span>s<span class="sym">[</span>wrap<span class="sym">:])</span>
            out_length <span class="sym">-=</span> s_length <span class="sym">-</span> wrap
            wrap <span class="sym">=</span> <span class="num">0</span>

        <span class="kwb">print</span> s<span class="sym">[</span>wrap<span class="sym">:</span>wrap <span class="sym">+</span> out_length<span class="sym">]</span>
        wrap <span class="sym">+=</span> out_length
        todo <span class="sym">-=</span> line_length


