<span class="kwb">def</span> <span class="kwd">makeCumulative</span><span class="sym">(</span>genelist<span class="sym">):</span>
    cp <span class="sym">=</span> <span class="num">0.0</span>
    <span class="kwb">for</span> i <span class="kwb">in</span> <span class="kwa">xrange</span><span class="sym">(</span><span class="kwa">len</span><span class="sym">(</span>genelist<span class="sym">)):</span>
        cp <span class="sym">+=</span> genelist<span class="sym">[</span>i<span class="sym">][</span><span class="num">1</span><span class="sym">]</span>
        genelist<span class="sym">[</span>i<span class="sym">][</span><span class="num">1</span><span class="sym">] =</span> cp


<span class="kwb">def</span> <span class="kwd">selectRandom</span><span class="sym">(</span>genelist<span class="sym">):</span>
    r <span class="sym">=</span> <span class="kwd">gen_random</span><span class="sym">(</span><span class="num">1.0</span><span class="sym">)</span>
    <span class="kwb">for</span> pair <span class="kwb">in</span> genelist<span class="sym">:</span>
        <span class="kwb">if</span> r <span class="sym">&lt;</span> pair<span class="sym">[</span><span class="num">1</span><span class="sym">]:</span>
            <span class="kwb">return</span> pair<span class="sym">[</span><span class="num">0</span><span class="sym">]</span>


<span class="kwb">def</span> <span class="kwd">makeRandomFasta</span><span class="sym">(</span><span class="kwa">id</span><span class="sym">,</span> desc<span class="sym">,</span> genelist<span class="sym">,</span> todo<span class="sym">):</span>
    <span class="kwb">print</span> <span class="str">&quot;&gt;&quot;</span> <span class="sym">+</span> <span class="kwa">str</span><span class="sym">(</span><span class="kwa">id</span><span class="sym">),</span> desc
    line_length <span class="sym">=</span> <span class="num">60</span>
    a <span class="sym">=</span> <span class="kwd">array</span><span class="sym">(</span><span class="str">&quot;c&quot;</span><span class="sym">,</span> <span class="str">&quot; &quot;</span> <span class="sym">*</span> line_length<span class="sym">)</span>
    <span class="kwb">for</span> i <span class="kwb">in</span> <span class="kwa">xrange</span><span class="sym">(</span>todo <span class="sym">/</span> line_length<span class="sym">):</span>
        <span class="kwb">for</span> j <span class="kwb">in</span> <span class="kwa">xrange</span><span class="sym">(</span>line_length<span class="sym">):</span>
            a<span class="sym">[</span>j<span class="sym">] =</span> <span class="kwd">selectRandom</span><span class="sym">(</span>genelist<span class="sym">)</span>
        a<span class="sym">.</span><span class="kwd">tofile</span><span class="sym">(</span>stdout<span class="sym">)</span>
        <span class="kwb">print</span>

    todo <span class="sym">=</span> todo <span class="sym">%</span> line_length
    <span class="kwb">while</span> todo <span class="sym">&gt;</span> <span class="num">0</span><span class="sym">:</span>
        <span class="kwb">if</span> todo <span class="sym">&gt;</span> line_length<span class="sym">:</span>
            out_length <span class="sym">=</span> line_length
        <span class="kwb">else</span><span class="sym">:</span>
            out_length <span class="sym">=</span> todo

        pick <span class="sym">=</span> <span class="str">&quot;&quot;</span>
        <span class="kwb">for</span> i <span class="kwb">in</span> <span class="kwa">xrange</span><span class="sym">(</span>out_length<span class="sym">):</span>
            pick <span class="sym">+=</span> <span class="kwd">selectRandom</span><span class="sym">(</span>genelist<span class="sym">)</span>

        <span class="kwb">print</span> pick
        todo <span class="sym">-=</span> line_length


<span class="kwb">def</span> <span class="kwd">makeRepeatFasta</span><span class="sym">(</span><span class="kwa">id</span><span class="sym">,</span> desc<span class="sym">,</span> s<span class="sym">,</span> todo<span class="sym">):</span>
